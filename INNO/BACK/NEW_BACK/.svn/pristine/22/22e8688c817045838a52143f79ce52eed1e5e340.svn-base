.first_filter_item{
	border:1px dashed #333;
}
.filter_item{
	width:100%;
	/*height:300px;*/
	background:#fff;
	position:relative;
	z-index:1;
	-moz-border-radius: 10px; 
	-webkit-border-radius: 10px;
	border-radius: 10px;
	/*margin:20px 0px;*/
	padding:10px;
	padding-left:20px;
	box-sizing:border-box;
	-moz-box-sizing:border-box;
	-webkit-box-sizing:border-box;
	margin-bottom:15px;
}
.basic_filter{
	padding:10px 0px;
	border-top:1px solid #d2d2d2;
	position:relative;
}
.browse_filter{
	padding:10px 0px;
	border-top:1px solid #d2d2d2;
	position:relative;
}
.filter_item .basic_filter:first-child{
	border:none;
}
.filter_item .browse_filter:first-child{
	border:none;
}
