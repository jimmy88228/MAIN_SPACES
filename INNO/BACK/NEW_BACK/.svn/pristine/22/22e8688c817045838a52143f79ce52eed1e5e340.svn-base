.filter_lists{
	overflow-y: auto;
	height: calc(100vh - 400px);
}
.filter_list{
	border:1px solid #d2d2d2;
	padding:20px;
}
.add_new_filter{
	display:inline-block;
	background:#28A5FF;
	color:#fff;
	-moz-border-radius: 5px; 
	-webkit-border-radius: 5px;
	border-radius: 5px;
	border:none;
	padding:8px 15px;
	cursor:pointer;
}
.filter_stay{
	padding-left:120px;
