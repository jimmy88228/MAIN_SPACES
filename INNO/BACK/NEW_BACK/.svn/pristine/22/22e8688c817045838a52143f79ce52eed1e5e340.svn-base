.item_switch .ivu-switch{
	display:block;
	width: 100%;
	height: 30px;
	line-height: 30px;
	-moz-border-radius: 5px; 
	-webkit-border-radius: 5px;
	border-radius: 5px;
	border:0 none;
	/* background-color: #d4d4d4; */
}
.item_switch .ivu-switch:after{
	content:'';
	width:21px;
	height:21px;
	border-radius: 3px;
	top:4px;
	left:4px;
}
.item_switch  .ivu-switch-checked:after{
	content:'';
	left: 35px;
}
.item_switch .ivu-switch-inner{
	left: 36px;
	font-size:16px;
}
.item_switch .ivu-switch-checked .ivu-switch-inner{
	left:8px;
}
