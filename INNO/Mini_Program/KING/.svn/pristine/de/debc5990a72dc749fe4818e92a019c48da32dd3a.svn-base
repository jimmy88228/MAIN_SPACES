page{
  background-color:#F3F3F3;
}
.square_area{
  padding:0px 15rpx;
  padding-top:20rpx;
}
.live_item{
  border-radius:20rpx;
  overflow: hidden;
  background-color:#fff;
  margin-bottom:23rpx;
  position:relative;
}
.live_status{
  border-radius:100rpx;
  background:rgba(0,0,0,0.54);
  font-size:20rpx;
  font-weight:bold;
  color:#fff;
  padding:5rpx 16rpx;
  position:absolute;
  top:23rpx;
  left:19rpx;
}
.cover_img_area{
  width:100%;
  height:574rpx;
}
.cover_img{
  width:100%;
  height:100%;
  display:block;
  background-color:#efefef;
}
.live_cont{
  display:flex;
  justify-content: space-between;
  align-items: center;
  padding:24rpx 17rpx 27rpx 27rpx;
}
.live_cont_l{
  max-width:310rpx;
}
.live_name{
  font-size:24rpx;
  font-family:Hiragino Sans GB;
  font-weight:normal;
  color:rgba(51,51,51,1);
  line-height:36rpx;
  margin-bottom:15rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
}
.live_circular{
  width:26rpx;
  height:26rpx;
  background:rgba(252,173,169,1);
  border-radius:50%;
  display:inline-block;
  vertical-align: middle;
  margin-right:10rpx;
}
.live_author{
  display:inline-block;
  vertical-align: middle;
  font-size:20rpx;
  font-family:Hiragino Sans GB;
  font-weight:normal;
  color:rgba(51,51,51,1);
}
.goods_item{
  width:100rpx;
  height:100rpx;
  margin:0px 8rpx;
  border-radius:8rpx;
  display:inline-block;
  vertical-align: middle;
  position:relative;
  overflow: hidden;
}
.goods_img{
  width:100%;
  height:100%;
  display:block;
}
.goods_mask{
  position:absolute;
  top:0px;
  left:0px;
  background:rgba(17,17,17,0.2);
  width:100%;
  height:100%;
  display:flex;
  align-items: center;
}
.goods_more{
  color:#fff;
  text-align:center;
  margin:0 auto;
  width:100%;
  font-size:20rpx;
}
.goods_item .price{
  position:absolute;
  left:0px;
  bottom:0px;
  padding-left:10rpx;
  padding-bottom:10rpx;
  font-size:18rpx;
  font-family:Hiragino Sans GB;
  font-weight:bold;
  color:rgba(255,255,255,1);
}
.goods_tip{
  display:flex;
  background-color:#efefef;
  font-size:24rpx;
  font-family:Hiragino Sans GB;
  font-weight:normal;
  color:rgba(99,103,240,1);
  line-height:30rpx;
  box-sizing: border-box;
  justify-content: center;
  align-items: center;
  width:100%;
  height:100%;
  text-align:center;
  position:relative;
  z-index:5;
  /* padding:25rpx; */
}
.goods_tip_stay{
}
.before_staue{
  background-color:#F1EFFD;
  color:rgba(99,103,240,1);
}
.after_staue{
  background-color:#F0EDF1;
  color:#B2B2B2;
}
.living_staue{
  background:linear-gradient(115deg,rgba(255,0,0,1),rgba(255,101,33,1));
  color:#fff;
}
/* .subscribe--live-player-subscribe__btn
.subscribe--live-player-hasSubscribe
.subscribe--live-player-notSubscribe */
.subscribe--live-player-subscribe__btn{
  width:100rpx !important;
  height:100rpx !important;
  font-size:24rpx !important;
  background-color:#EFF0FD !important;
  color:rgba(99,103,240,1) !important;
  display:flex !important;
  justify-content: center !important;
  align-items: center !important;
  padding:25rpx !important;
  box-sizing: border-box !important;
  line-height:30rpx !important;
  font-family:Hiragino Sans GB !important;
}
.line_icon{
  display:inline-block;
  vertical-align: bottom;
}
._line{
  display:inline-block;
  width:4rpx;
  background-color:#fff;
  margin:0px 2rpx;
}
.line_1{
  height:5rpx;
}
.line_2{
  height:12rpx;
}
.line_3{
  height:17rpx;
}
.line_4{
  height:9rpx;
}
