.my_collage_list{
  padding-top:90rpx;
  padding-bottom:100rpx;
}
.collage_status_tap{
  position:fixed;
  left:0px;
  top:0px;
  width:100%;
  box-sizing:border-box;
  z-index:10;
}
.c_status_tap_item{
  height:90rpx;
  line-height:90rpx;
}
.c_status_tap_point{
  position:absolute;
  left:12.5%;
  bottom:0px;
  width:50rpx;
  height:6rpx;
  transition: left .35s;
  -moz-transition: left .35s;
  transform: translateX(-50%);
}
.my_collage_item{
  padding-left:30rpx;
  margin-top: 16rpx;
  margin-bottom: 16rpx;
}
.collage_title{
  padding:26rpx 32rpx 20rpx 0px;
}
.my_collage_info{
  padding:20rpx 0px;
  padding-right:30rpx;
}
.activity_img{
  width:160rpx;
  height: 160rpx;
    border:1rpx solid rgba(239,239,239,1);
}
.activity_img_stay{
  width:100%;
}
.activity_img_stay .img{
  width:100%;
  padding-top:100%;
  position:relative;
}
.activity_img_stay .img image{
  display:block;
  width:100%;
  position:absolute;
  top:50%;
  left:50%;
  transform: translate(-50%,-50%);
  -moz-transform: translate(-50%,-50%);
}
.activity_info{
  padding-left:20rpx;
}
.activity_name{
  margin-bottom:5rpx;
  font-size:24rpx;
  color:rgba(27,27,27,1);
}
.collage_price{
  margin-bottom:18rpx;
}

.my_collage_operate{
  padding:14rpx 0px;
  padding-right:30rpx;
}
.operate_btn{
  display:inline-block;
  width:140rpx;
  height: 56rpx;
  line-height: 56rpx;
  text-align: center;
  margin-left:20rpx;
}
.collage_tip{
  width:76rpx;
  height:31rpx;
  border:1px solid #E60012;
  display:flex;
  justify-content: center;
  align-items: center;
  border-radius:100rpx;
  border-top-left-radius: 0px;
  margin-bottom:10rpx;
  padding: 4rpx;
}
.tabbar_img image {
     width:70rpx;
}
.ordertTab {
    display: flex;
    align-items: center;
    text-align: center;
    background: #fff;
    color: #777;
    width: 100%;
    position: fixed;
    z-index: 2;
    overflow: visible;
    border-top: 1rpx solid rgba(239,239,239,1);
}

.active {
    color: #cd718f;
    font-weight: bold;
}

.lines {
    position: absolute;
    bottom: -3rpx;
    left: 50%;
    transform: translateX(-50%);
    width: 52rpx;
    height: 4rpx;
    border-radius: 20rpx;
    border: 1rpx solid transparent;
}

.orderTabTit {
    position: relative;
    width: 50%;
    line-height: 90rpx;
    height: 90rpx;
    box-sizing: border-box;
    font-size:28rpx;
}

.ordertTab text {
    display: inline-block;
    vertical-align: middle;
}
.ml_20{
    font-size: 20rpx;
    margin-left: 20rpx;
    color:rgba(127,127,127,1);
}
.collage{
    font-size: 24rpx;
    color:rgba(0,0,0,1);
}
.collage_title{
    display: flex;
    align-items: center;
    justify-content: space-between;
}
.change_status{
    font-weight:bold;
    color:rgba(127,127,127,1);
}
.collage_market_price{
    color:rgba(178,178,178,1);
}
.activity_tip{
  width:76rpx;
  height:31rpx;
  line-height: 31rpx;
  border:1px solid #E60012;
  display:flex;
  justify-content: center;
  align-items: center;
  border-radius:5rpx;
  /*border-top-left-radius: 0px;*/
  margin-bottom:20rpx;
}