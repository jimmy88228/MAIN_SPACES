.swiper_module {
    position: relative;
}
.custom_swiper_item {
    position: relative;
}
.s_item_view{
    position: relative;
}
.video_swiper{
  min-height: 422rpx !important;
}
.videoBox{
  position:absolute;
  top:50%;
  left:50%;
  transform: translate(-50%,-50%);
  width: 750rpx;
  height: 422rpx;
}
.swiper_img {
    width: 100%;
    display:block;
    background-color:#fff;
}

.switch_point {
    position: absolute;
    width: 100%;
    bottom: 16rpx;
    left:0;
    padding-right: 24rpx;
    box-sizing: border-box;
    text-align: center;
}

.switch_point .point {
    background: #fff;
    width: 10rpx;
    height: 10rpx;
    border-radius: 10rpx;
    display: inline-block;
    margin: 0px 6rpx;
    transition: all 0.32s;
    border:2rpx solid #ccc;
}

/* .switch_point .active {
    background: rgba(0, 0, 0, 1);
} */


/* common */



/*热区*/
.map_area {
    position: absolute;
    z-index: 10;
}

.play_icon_area {
    position: absolute;
    top: 0px;
    left: 0px;
    width: 100%;
    height: 100%;
    z-index: 11;
}
 
.play_icon {
    position: absolute;
    top: 50%;
    left: 50%;
    width: 120rpx;
    height: 120rpx;
    transform: translate(-50%, -50%);
    background: rgba(0, 0, 0, 0.4);
    border-radius: 100%;
}

.btn_link {
    position: absolute;
    top: 0px;
    left: 0px;
    right: 0px;
    bottom: 0px;
    z-index: 10;
    opacity: 0;
} 
/* module_type */
.module_type3 .custom_swiper_item{
    /* width: 500rpx !important;
    height: 500rpx !important; */
    overflow: unset;
}
.module_type3 .s_item_view{
    position: absolute;
    top:50%;
    left:50%;
    width: 500rpx !important;
    height: 500rpx !important;
    transform: translate(-50%, -50%) scale(0.7);
    transition: transform 1s;
    z-index:1;
    background-color:#fff;
    box-sizing: border-box;
    box-shadow: 0px 0px 20rpx #efefef;
}
.module_type3 .custom_swiper_item.active{
    z-index: 2;
}
.module_type3 .active .s_item_view{
    transform: translate(-50%, -50%) scale(1);
}
.module_type2 .swiper_img{
    background-color:#8e8e8e;
}
.module_type2 .s_item_view{
    margin:10rpx 15rpx;
    overflow: hidden;
}
