@import "/wxParse/wxParse.wxss";
page{
  background: #fbfbfb;
}
.main {
  position: relative;
  padding-bottom: 120rpx;
}
.main.i_p_box{
padding-bottom: 182rpx;
}
._swiper_box{
  width: 100%;
  height: 750rpx;
  position: relative;
}
._swiper {
  width: 100%;
  height: 100%;
}

.img_swiper_box {
  width: 100%;
  height: 100%;
}

.img_swiper {
  width: 100%;
  height: 100%;
}

.count_down {
  position: relative;
  padding: 24rpx 30rpx;
  box-sizing: border-box;
  width: 100%;
  color: #fff;
}

.c_d_label {
  line-height: 24rpx;
  height: 24rpx;
  padding: 4rpx 6rpx;
  margin-right: 8rpx; 
}

.c_d_p_char {
  line-height: 28rpx;
  margin-right: 2rpx;
}

.c_d_price {
  line-height: 34rpx;
  padding-right: 10rpx;
}

.c_d_m_k {
  /* color: #f9ab9a; */
  line-height: 22rpx;
}

.l_h {
  text-decoration: line-through;
}

.c_d_time {
  position: absolute;
  right: 35rpx;
  top: 50%;
  transform: translateY(-50%);
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.c_d_tip {
  /* color: rgba(249, 171, 154, 1); */
  padding-right: 12rpx;
}

.c_d_day {
  padding-right: 5rpx;
}

.sk_content {
  width: 100%;
  position: relative;
}

.msg_title {
  width: 100%;
  padding-right: 120rpx;
  box-sizing: border-box;
  padding-bottom: 24rpx;
}
.img_share_box {
  width: 98rpx;
  height: 98rpx;
  position: absolute;
  right: 30rpx;
  top: 20rpx;
  z-index: 1;
}
.img_share{
  width: 100%;
  height: 100%;
}
.parting_line {
  width: 100%;
  height: 14rpx;
  background: #fbfbfb;
}

.p_box {
  padding: 30rpx;
  box-sizing: border-box;
  background: #fff;
}

.tab {
  width: 100%;
  height: 100%;
  display: flex;
  justify-content:center;
  align-items: center;
  flex-direction: column;
}

.tab_index {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
}

.img_index {
}
 

.buy_tab {
  position: fixed;
  bottom: 0;
  left: 0;
  padding: 8rpx;
  width: 734rpx;
  height: 90rpx;
  border-top:1px solid rgba(235,235,235,1);
  background: #fff;
  z-index: 2;
}
.tab_content{
  width: 100%;
  height: 100%;
  display: flex;
  flex-wrap: nowrap;
}
.tab_index{
  width: 160rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.img_index{
  width: 60rpx;
  height: 60rpx;
  /* width: 34rpx;
  height: 30rpx; */
}
.t_i_text {
  color: #111;
  line-height: 24rpx;
  margin-top: 10rpx;
}
.tab_btn_box{
  display: flex;
  width: 574rpx;
  border-radius: 4rpx;
  overflow: hidden;
  color: #fff;
}
.tab_btn_sm{
  flex: 1;
  /* display:table-cell;
  vertical-align:middle; */
}
.tab_btn_lg{
  flex: 1;
  /* display:table-cell;
  vertical-align:middle; */
}
.tab_flex{
  height: 100%;
  display: flex;
  justify-content:center;
  align-items: center;
  flex-direction: column;
  box-sizing:border-box;
}
.c_d_tip{
  padding: 0 8rpx;
}
.btn{
  width: 100%;
  height: 100%;
  color: #fff;
}

.g_detail{
  background: #fff;
}
.g_detail image{
  max-width: 100%;
}
.tab_btn_lg.font_op{
  color: rgba(255, 255, 255, 0.3);
}
.tab_btn_lg.is_sub_btn{
  border:1px solid #000;
}
.tip_box{
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 18rpx 0;
  color: #fff;
  position: absolute;
  left: 0;
  top: 0;
  transform: translateY(-100%);
}


.tip_box .tip_text{
  line-height: 26rpx;
}
.tip_box .tip_btn{
  margin-left: 12rpx;
  border-radius: 24rpx;
  padding: 10rpx 14rpx;
  font-size: 22rpx;
  line-height: 24rpx;
}
._char{
  margin-left: -2rpx;
  line-height: 28rpx;
  padding-bottom: 4rpx;
}
._text{
  line-height: 26rpx;
}
.t_disabled{
  background:rgba(239,239,239,1);
  color: #B2B2B2;
}

.switch_point {
    position: absolute;
    width: 100%;
    bottom: 16rpx;
    left:0;
    padding-right: 24rpx;
    box-sizing: border-box;
    text-align: center;
}

.switch_point .point {
    background: #fff;
    width: 10rpx;
    height: 10rpx;
    border-radius: 50%;
    display: inline-block;
    margin: 0px 6rpx;
    transition: all 0.32s;
    border:2rpx solid transparent;
}

/* 已选 */
.promote_box{
    display: flex;
    align-items: center;
    width: 100%;
    box-sizing: border-box;
    padding: 20rpx 30rpx;
    padding-right: 10rpx;
    font-size: 24rpx;
    background: #fff;
    color: #7f7f7f;
    border-top: 1px solid #efefef;
    position: relative;
}
.promote_box.start{
    align-items: flex-start;
}
.promote_title{
    width: 60rpx;
    padding-right: 20rpx;
}

.promote_content{
    width: 580rpx;
    height: 100%; 
    color: #212121; 
    display: -webkit-box;
    overflow: hidden;
    text-overflow: ellipsis;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    word-wrap: break-word; 
}
/* .pr+ */
.promote_flex{
    width: 100%;
    display: flex;
    align-items: center;
}
.promote_flex.p_top{
    padding-top: 10rpx;
    padding-left: 70rpx;
    box-sizing: border-box; 
}
.promote_flex.p_top_lg{
    padding-top: 10rpx;
    padding-left: 108rpx;
    box-sizing: border-box; 
}

.img_promote{
    width: 50rpx;
    height: 50rpx;
}
.select_tip{
  margin-left: -80rpx;
  margin-right: 80rpx;
}
._p_r_10{
  padding-right:10rpx;
}
.p_b_80{
  margin-bottom: 80rpx;
}

.price_explain{
  width:100%;
}
.price_explain_img{
  width:100%;
}
.price_box{
  margin-left: 10rpx;
}
.c_d_text{
  display: flex;
} 