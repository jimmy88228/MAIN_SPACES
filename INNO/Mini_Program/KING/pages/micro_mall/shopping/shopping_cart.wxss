.shopping_page {
    padding-top: 50rpx;
    padding-bottom: 96rpx;
    /* padding-bottom:200rpx; */
}

input {
    border: 1px solid #e9e9e9;
    border-radius: 10rpx;
}

.package_carts_list {
    border: 1px solid #e6e6e6;
    border-top: none;
    border-radius: 10rpx;
    width: 90%;
    margin: 0 auto;
}

.package_carts_item {
    padding: 20rpx;
    border-bottom: 1px solid #e6e6e6;
}

.package_carts_list .package_carts_item:last-child {
    border: 0px;
}