/* pages/component/custom/tabBar.wxss */
.d_flex{
  display: -webkit-box; 
  display: -moz-box; 
  display: -ms-flexbox; 
  display: -webkit-flex;
  display: flex;
  align-items :center;
}
.flex1{
  -webkit-box-flex: 1 ;
  -moz-box-flex: 1;             
  -webkit-flex: 1;
  -ms-flex: 1 ; 
  flex: 1;
  justify-content: center;
  align-items:center;
}
.tabbar_stay{
  width:100%;
  height:96rpx;
}
.custom_tabbar_area{
  background:#fff;
  position:fixed;
  left:0px;
  bottom:0px;
  width:100%;
  /* height:96rpx; */
  height: 100rpx;
  text-align:center;
  z-index:99;
  box-shadow: 0px 0px 18rpx 0px rgba(0, 0, 0, 0.08);
}
.tabbar_item{
  height: 100%;
}
.tabbar_img{
  /* height:44rpx; */
}
.tabbar_img image{
  width:78rpx;
  height:78rpx;
  display:block;
  margin:0 auto;
}
.tabbar_txt{
  font-size:28rpx;
  padding-left: 6rpx;
  color:#000;
}
.jump_btn{
  /* padding:11rpx 0px; */
  line-height:normal;
  width: 100%;
  height: 100%;
}
.fix_iphoneX{
  padding-bottom:34px;
}