.main {
    width: 750rpx;
    box-sizing: border-box;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.title {
    margin: 102rpx 0 66rpx 0;
}

.inputStyle {
    width: 480rpx;
    height: 80rpx;
    background: rgba(255, 255, 255, 1);
    border: 1px solid;
    border-radius: 10rpx;
    padding: 0 20rpx;
    box-sizing: border-box;
}

.tips {
    margin: 18rpx 0 190rpx 0;
    color: rgba(127, 127, 127, 1);
    font-size: 24rpx;
    line-height: 24rpx;
}

.button {
    width: 300rpx;
    height: 86rpx;
    border-radius: 10rpx;
    color: #fff;
    font-size: 28rpx;
}

.bold {
    font-weight: bold;
}
