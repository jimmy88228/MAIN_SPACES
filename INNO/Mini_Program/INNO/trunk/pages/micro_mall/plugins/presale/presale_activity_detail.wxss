.goods_swiper_gallery {
    position: relative;
}

.goods_swiper_area {
    position: relative;
}

.goods_swiper {
    width: 750rpx;
    height: 750rpx;
    overflow: hidden;
    position: relative;
}

.switch_point {
    position: absolute;
    width: 100%;
    bottom: 20rpx;
    left: 0px;
    text-align: center;
}

.switch_point .point {
    width: 11rpx;
    height: 11rpx;
    border-radius: 50%;
    display: inline-block;
    margin: 0px 5rpx;
    transition: all 0.32s;
}

.swiper_item {
    line-height: 750rpx;
    background: #fff;
    overflow: hidden;
}

.slide-image {
    position: absolute;
    top: 50%;
    left: 50%;
    width: 100%;
    height: 100%;
    display: block;
    transform: translate(-50%, -50%);
}

