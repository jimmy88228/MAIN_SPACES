    flex: 0;
    min-width: 102rpx;
}

.rule_list .rule_item_stay {
    display: inline-block;
    text-align: center;
    position: relative;
    z-index: 2;
    height: 100%;
}

.rule_list .rule_item .rult_state_icon icon.selected {
    display: none;
    background-color: #fff;
    border-radius: 100%;
}

.rule_list .rule_item .rult_state_icon icon.no_selected {
    display: inline-block;
    background-color: #efefef;
    border-radius: 100%;
}

.rule_list .rule_item.ruled .rult_state_icon icon.selected,
.rule_list .rule_item.is_rule .rult_state_icon icon.selected {
    display: inline-block;
}

.rule_list .rule_item.ruled .rult_state_icon icon.no_selected,
.rule_list .rule_item.is_rule .rult_state_icon icon.no_selected {
    display: none;
}

