}

.rule_list .rule_line.ruled {
    /* background:#E60012; */
}

.rule_list .rule_line:last-child {
    display: none;
}

.rule_list .rule_item:last-child {
    text-align: right;
    -webkit-box-flex: 0;
    -moz-box-flex: 0;
    -webkit-flex:0;
