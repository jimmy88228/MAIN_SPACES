.main{
  background: #fff;
  padding-top: 35rpx;
}
.video_list_box{
  position: relative;
  height: 600rpx;
}
.video_item_box{
  /* margin-left:38rpx; */
  margin-left:0;
  /* width: 620rpx; */
  width: 100%;
  height: 100%;
}
.tip{
  font-size: 22rpx;
  color: #7f7f7f;
  height: 42rpx;
}
.video_box{
  width: 100%;
  height: 100%;
  position: relative;
}
.video_item{
  width: 100%;
  height: 100%;
}

.img_box,.img_cover{
  position: absolute;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
  z-index: 1;
}
.img_box .img_cover{
  z-index: 2;
}
.img_video{
  position: absolute;
  width: 100rpx;
  height: 100rpx;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
  z-index: 3;
}

.img_full_screen{
  position: absolute;
  width: 70rpx;
  height: 46rpx;
  right: 0;
  bottom: 0;
  z-index: 3;
}

.img_full_screen{
  position: absolute;
  width: 70rpx;
  height: 46rpx;
  right: 10rpx;
  bottom: 14rpx;
  z-index: 3;
}
.img_mute,.img_mute_active{
  position: absolute;
  width: 70rpx;
  height: 46rpx;
  left: 10rpx;
  bottom: 14rpx;
  z-index: 3;
}