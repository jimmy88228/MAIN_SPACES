page{
    background-color:#fff;
    width:100%;
    box-sizing:border-box;
  }
  .index_page{
    width:100%;
    box-sizing:border-box; 
  } 
  .index_page image{
    display:block;
  }
  .page_tab{
    width:100%;
  }

  .img{
    width: 100%;
    display: block;
  }
  .filterBox{
    transition: filter 0.32s;
  }
  .filter{
    filter: blur(15rpx);
  }
  .img-content{
    width:750rpx;
    height: 598rpx;
    display: block;
  }