.center-box {
    flex: 1;
    width: 100%;
    height: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    box-sizing: border-box;
}

.center-box .center-box-item {
    flex: none;
}

.btn::after,
.btn-plain::after,
.btn-black::after,
.btn-white::after {
    display: none;
}

.btn,
.btn-plain,
.btn-black,
.btn-white {
    margin: 0;
    position: unset;
    box-sizing: unset;
    display: inline-block;
    background: none;
    border: none;
    padding-top: 0;
    padding-left: 0;
    padding-bottom: 0;
    padding-right: 0;
    line-height: unset;
    text-align: center;
    font-size: 26rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    border-radius: 6rpx;
    box-sizing: border-box;
}

.btn,
.btn-black,
.btn-white {
    height: 90rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}

.btn-black {
    background-color: #000;
    color: white;
}

.btn.no-enable,
.btn-black.no-enable {
    color: #ccc;

}

.btn-black.no-enable {
    background-color: #efefef;
    border: 1px solid #eee;
}


.btn-hover,
.btn-plain.btn-hover,
.btn-white.btn-hover {
    background-color: rgba(127, 127, 127, 0.05);
}

.btn-black.btn-hover {
    background-color: #333;
}

.btn-white {
    background-color: none;
    border: 1px solid #000;
    color: #000;
}

.newline {
    display: block;
    word-wrap: break-word;
    overflow: hidden;
    white-space: pre-wrap;
    text-overflow: ellipsis;
    /* text-align: justify;
    text-justify: inter-ideograph; */
}

.no-newline {
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.no-scroll {
    overflow: hidden;
    width: 100%;
    height: 100%;
}

.flex-b-w {
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.flex {
    display: flex;
    justify-content: flex-start;
    align-items: flex-start;
}

.flex-s-c {
    display: flex;
    justify-content: flex-start;
    align-items: center;
}

.flex-c-c {
    display: flex;
    justify-content: center;
    align-items: center;
}

.flex-e-c {
    display: flex;
    justify-content: end;
    align-items: center;
}
.flex-col-c-c{
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}
button{ 
    background: none;
    margin: 0;
    padding: 0;
    white-space:nowrap;
    border-radius: 0;
    font-size: 26rpx;
    overflow: visible;
}
button:after{
    border:none;
}