<!--pages/draw/draw_acts/draw_acts.wxml-->

<block wx:if="{{actInfo.activityTypeCode == 'dazhuanpan'}}">
  <turn_plate 
    id="act" 
    setting="{{setting}}"
     ></turn_plate>
</block> 

<block wx:if="{{actInfo.activityTypeCode == 'shuiguoji'}}">
  <fruit_act 
    id="act" 
    setting="{{setting}}"
     ></fruit_act>
</block>

<block wx:if="{{actInfo.activityTypeCode == 'fanfankan'}}">
  <flop_act 
    id="act" 
    setting="{{setting}}"
     ></flop_act>
</block>

<block wx:if="{{actInfo.activityTypeCode == 'yaoyiyao'}}">
<!-- <block wx:if="{{actInfo.activityTypeCode == 'zajingdan' || actInfo.activityTypeCode == 'yaoyiyao'}}"> -->
  <matrix_act 
    id="act" 
    setting="{{setting}}"
     ></matrix_act>
</block>

<block wx:if="{{actInfo.activityTypeCode == 'zajingdan'}}">
  <smash_eggs 
    id="act" 
    setting="{{setting}}"
     ></smash_eggs>
</block>

