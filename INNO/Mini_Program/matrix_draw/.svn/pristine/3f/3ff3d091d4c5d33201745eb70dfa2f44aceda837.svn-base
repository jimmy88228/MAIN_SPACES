page{
  background-color: rgba(240, 240, 240, 1);
}
.content-box{
  padding: 25rpx;
  box-sizing: border-box;
}
.list{
  width: 100%;
  height: 176rpx;
  padding: 20rpx;
  box-sizing: border-box; 
  position: relative;
  margin-bottom: 16rpx;
  display: flex;
  align-items: center;
}
.coupon-name{
  padding-left: 30rpx;
  font-size:32rpx;
  color: #fff; 
  word-break: break-all;
  position: relative;
}
.arrow{ 
  width: 12rpx;
  height: 12rpx;
  border-top: 1px solid #fff;
  border-right: 1px solid #fff;
  position: absolute;
  right: 30rpx;
  top: 50%;
  transform: translateY(-50%) rotate(45deg);
}
.img-coupon{
  width: 100%;
  height: 100%;
  position: absolute;
  left: 50%;
  top: 50%;
  transform:translate(-50%,-50%);
  z-index: 0;
}