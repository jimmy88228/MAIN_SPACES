    <image src="{{brand_info.icon_url}}micro_mall/brand_icon.png" mode="widthFix"/>
  </view>
  <view class="code_img">
    <image src="{{codeUrl}}" mode="widthFix"/>
  </view>
  <view>
    <view>长按分享二维码</view>
    <view>邀请好友和我一起成为 <text style="color:{{brand_info.style.font_color}};">{{brand_info.brand_name}} VIP 会员</text> </view>
  </view>
</view>


