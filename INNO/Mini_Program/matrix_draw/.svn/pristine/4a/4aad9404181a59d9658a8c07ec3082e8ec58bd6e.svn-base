<view class="code_form" style="{{boxStyle}}{{zIndex}}">
  <form bindsubmit="formAction" report-submit="true" >
  <!-- <window class="window" ext-class="window-ext"> -->
      <view class="code_page_bg"></view>
      <view class="code_page" >
          <view class="code_area">
            <view>
              <canvas canvas-id="barCanvas" class="barCanvas"></canvas>
              <view class="code_text">{{codeNum}}</view>
            </view>
            <view class="pay_code_view">
              <canvas class="pay_code" canvas-id='payCode'></canvas>
            </view>
          </view>
        <view class="component_close" bindtap="onDetached"></view>
      </view>
  <!-- </window> -->
  </form>
</view>