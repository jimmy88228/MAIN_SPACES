page{
    background-color:rgba(4, 5, 53, 1);
    width:100%;
    box-sizing:border-box;
  }
  .index_page{
    width:100%;
    box-sizing:border-box; 
  } 
  .index_page image{
    display:block;
  }
  .page_tab{
    width:100%;
  }

  .img{
    width: 100%;
    display: block;
  }
  .filterBox{
    transition: filter 0.3s linear;
  }
  .filter{
    filter: blur(20rpx);
  }
  .draw_box{
    position: relative;
  }
  .img-content{
    width:750rpx;
    height: 598rpx;
    display: block;
  }
  .img-content.front{
    position: absolute;
    left: 50%;
    top: 0;
    transform: translateX(-50%);
    z-index: 2;
    height: 100%;
  }