<window class="window" ext-class="window-ext">
    <view class="dialog-box" style="{{mainStyle}}" catchtouchmove="noAction">
        <view class="dialog-all">
            <text class="dialog-title">{{_title}}</text>
            <text class="dialog-content">{{_content}}</text>
        </view>
        <view class="dialog-btn-box">
            <button class="dialog-btn start" hover-class="end-hover" bindtap="dismiss">暂不授权</button>
            <auth-button class="dialog-btn end" auth openType="getUserInfo" bindauthed="onAuthed">
                马上授权
            </auth-button>
        </view>
    </view>
</window>