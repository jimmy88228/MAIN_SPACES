@import "/static/fonts/HelveticaNeueLT-font.wxss";
@import "/static/wxss/common.wxss";
@import "/common/anim/anim.wxss";

page {
    --page-bg-color: #f7f7f7;
    --text-main-color: #171717;
    --text-sub-color: #7f7f7f;
    --text-tips-color: #b2b2b2;
    display: flex;
    flex-direction: column;
    box-sizing: border-box;
    height: 100%;
    width: 100%;
    font-size: 24rpx;
    color: var(--text-main-color);
    font-family: 'HelveticaNeueLT Std Blk Cn', 'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif;
}