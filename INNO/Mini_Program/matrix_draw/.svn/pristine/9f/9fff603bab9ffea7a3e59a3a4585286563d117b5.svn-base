.main{
  position: absolute;
  width: 650rpx;
  height: 400rpx;
  top: 50%;
  left: 50%;
  background:#fff;
  border-radius: 20rpx;
  box-sizing: border-box;
  opacity: 0;
  transform: translate(-50%,-50%);
  box-sizing: border-box;
  padding:50rpx 0;
}
.content-box{
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center; 
  height: 100%;
}
.user-head{
  position: absolute;
  top: 0;
  left: 50%;
  transform: translate(-50%,-50%);
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  z-index: 1;
  overflow: hidden;
}
.img-head{
  width: 100%;
  height: 100%;
}
.user-tip{
  color: #7f7f7f;
}
/* .share-tips-box{
  width: 100%;
  text-align: center;
} */
.share-tips,.share-condition{
  text-align: center;
  word-break: break-all;
}
.pro-bar-box{
  position: relative;
  width: 70%;
  height: 12rpx;
  border-radius: 30rpx;
  background:#dfdfdf;
}
.pro-bar{
  position: absolute;
  left: 0;
  top: 0;
  border-radius: 12rpx 0 0 12rpx;
  background:#F0570F;
  height: 100%;
  z-index: 1;
}
.pro-cir{
  position: absolute;
  z-index: 2;
  width: 12rpx;
  height: 12rpx;
  border-radius: 50%;
  background:#fff;
  border:6rpx solid #F0570F;
  top: 50%;
  transform: translateY(-50%);
  margin-left: -9rpx;
}
.share-btn{
  width: 230rpx;
  border-radius: 40rpx;
  background: #F0570F;
  color: #fff;
  height: 60rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border: 4rpx solid #F0570F;
}

.loginBtn{
  z-index: 111;
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
}