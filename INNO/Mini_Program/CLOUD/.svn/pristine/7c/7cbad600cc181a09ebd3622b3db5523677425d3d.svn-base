/* pages/component/micro-page/micro-mix/micro-page/micro-page.wxss */
page{
    --navH:"";
  }
  /* .main{
    box-sizing: border-box;
    position: relative;
  }
  .main.main-left{
    display: flex;
  } */
  .main{
      /* min-height: 50vh; */
  }
  .stickyBox{
    left:0px;
    z-index: 100;
    background: #fff;
    position: sticky;
  }
  .stickyBox.stickyBox-left{
    width: 200rpx; 
    display: inline-block;
    vertical-align: top;
  }
  .stickyBox.stickyBox-left.stickyBox-isIp-true{
   height: calc(100vh - 90rpx - 34px); 
  }
  .stickyBox.stickyBox-top{
    width: 100%;
  }
  .stickyBox.noSticky{
    position: relative;
    top:0;
  }
  .scroll{
    width: 100%;
    height: 100%;
    position:relative;
    z-index: 12;
  }
  .page_tab{
    display:flex;
    z-index: 12;
    /* background: #fff; */
    height: 100%;
    position: relative;
  }
  .page_tab.page_tab-left{
    flex-direction: column;
  }
  
  .p_tab_item{
    display: flex;
    align-items: center;
    justify-content: center;
    min-width: 25%;
    height: var(--navH);
    font-size:28rpx;
    color:#b2b2b2;
    flex-shrink: 0;
    position: relative;
    flex: 1;
  }
  .p_tab_item.p_tab_item-left{
    flex: none;
  }
  
  .p_tab_item.current{  
    color:#333;
  }
  .current .tab_line{
    position: absolute;
    width: 100%;
    height: 4rpx;
    bottom: 8rpx;
    left: 0;
    display: flex;
    justify-content: center;
  }
  .current .tab_line::after{
    content:"";
    width: 40%;
    height: 100%;
    background:#333;
  }
  .p_tab_txt{
    position: absolute;
    top: 50%;
    left: 0;
    transform: translateY(-50%);
    width: 100%;
    text-align: center;
    line-height: 36rpx;
    display: -webkit-box;
    overflow: hidden;
    text-overflow: ellipsis;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
    word-break:break-all;
  }
  .current .tab_line{
    position: absolute;
    width: 100%;
    height: 4rpx;
    bottom: 8rpx;
    left: 0;
    display: flex;
    justify-content: center;
  }
  .current .tab_line::after{
    content:"";
    width: 40%;
    height: 100%;
    background:#333;
  }
  .content-box{
    width:100%;
    height:100%;
    /* position: relative; */
    position: relative; 
    box-sizing: border-box;
  }
  .content-box.content-box-left{
    display: inline-block;
    vertical-align: top;
    width: calc(100% - 200rpx);
  }
  .content{
    /* top:0px;
    left:0px; */
    width:100%;
    position: relative;
  }
  
  .unenable_area{
    padding-top:300rpx;
    color:#b2b2b2;
    font-size:40rpx;
    text-align:center;
  }