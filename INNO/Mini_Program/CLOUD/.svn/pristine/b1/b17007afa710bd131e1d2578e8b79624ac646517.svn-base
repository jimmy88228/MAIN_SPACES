page{
  background: #fff;
  --main-color:rgb(239, 131, 0);
  --font-color:rgb(240, 71, 0);
  --main-color-sm:rgb(253, 238, 231);
}
.main{
  box-sizing: border-box;
  padding-bottom: 200rpx;
}
.box{
  width: 700rpx;
  margin: 0 auto;
}
.err_tip{
  position: absolute; 
  right: 25rpx;
  top: 50%;
  transform: translateY(-50%);
  font-size: 24rpx;
  color: var(--font-color);
}
.camera_box{
  width: 690rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin: 0 auto;
}
.voucher_title{
  width: 110rpx;
  height: 90rpx;
  margin-left: 55rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  color: #7f7f7f;
}
.content{
  display: inline-flex;
  flex-direction: column;
}
.camera{
  width: 330rpx;
  height: 203rpx;
  margin-bottom: 40rpx;
  border-radius: 6rpx;
}
.c_tip{
  font-size: 24rpx;
  padding: 18rpx 36rpx;
  display: inline-flex;
  align-items: center;
  margin: 0 auto;
  background-color: var(--main-color-sm);
  color: var(--font-color);
  border-radius: 40rpx;
}
.camera_sm{
  width:24rpx;
  height: 19rpx;
  margin-right: 10rpx;
}
.footer{
  position: fixed;
  left: 0;
  bottom: 50rpx;
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.submit{
  width: 600rpx;
  height: 96rpx;
  border-radius: 8rpx;
  color: #fff;
  font-size: 30rpx;
  background:var(--main-color);
}
.protocol_box{
  display: inline-flex;
  flex-direction: column;
  justify-content: center;
}
.protocol{
  display: inline-flex;
  align-items: center;
  font-size: 24rpx;
  margin-top: 32rpx;
  padding-left: 20rpx;
}
.p_text{
  color: #7f7f7f;
  margin-left: 10rpx;
}
.p_tip{
  color: rgb(0, 131, 206);
  text-decoration:underline;
  padding-right: 20rpx;
}