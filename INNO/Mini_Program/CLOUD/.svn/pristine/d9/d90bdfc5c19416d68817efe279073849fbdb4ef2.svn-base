/* pages/micro_mall/header/indexHeader.wxss */
.index_header{
  width:100%;
  background-color:#fff;
  position:fixed;
  top:0px;
  left:0px;
  z-index:11;
} 
.index_header.header_place{
  position: relative;
  height: 90rpx;
  background-color:none;
  z-index: 10;
}
.left_menu{
  position:relative;
  height:90rpx;
} 

.search_wrapper{
    display: flex;
    align-items: center;
    padding-left: 26rpx;
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
}
.toStore{
    display: flex;
    align-items: center;
    justify-content: center;
    width:60rpx;
    height:60rpx;
    border-radius:50%;
}
.img_scan{
  width: 46rpx;
  height: 46rpx;
}
.scan{
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  width:60rpx;
  height:60rpx;
  border-radius:50%;
}
.scan_font{
  font-size: 18rpx;
  line-height: 20rpx;  
  padding-top: 4rpx;
}
.store_brand,.p_scan{
  margin-left: 15rpx;
  box-sizing: content-box;
}
.scan_box{
  width: 30rpx;
  height: 30rpx;  
  position: relative;
}
.scan_label_box{
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
  z-index: 2;
  display: flex;
  align-items: center;
  width: 80%;
  height: 50%;
}
.scan_label{
  margin-right: 2rpx;
  width: 1px;
  height: 100%;
  background-color: #fff;
}
.scan_bg{
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
  z-index: 1;
  width: 80%;
  height: 50%;
  background-color: #000;
}
.scan_lt{
  position: absolute;
  width: 4rpx;
  height: 4rpx;
  left: 0;
  top: 0;
  background-color: transparent;
  border-top: 1px solid #000;
  border-left: 1px solid #000;
}
.scan_rt{
  position: absolute;
  width: 4rpx;
  height: 4rpx;
  right: 0;
  top: 0;
  background-color: transparent;
  border-top: 1px solid #000;
  border-right: 1px solid #000;
}
.scan_lb{
  position: absolute;
  width: 4rpx;
  height: 4rpx;
  left: 0;
  bottom: 0;
  background-color: transparent;
  border-left: 1px solid #000;
  border-bottom: 1px solid #000;
}
.scan_rb{
  position: absolute;
  width: 4rpx;
  height: 4rpx;
  right: 0;
  bottom: 0;
  background-color: transparent;
  border-right: 1px solid #000;
  border-bottom: 1px solid #000;
}
.icon_store{
    display: block;
    width: 40rpx;
    height: 40rpx;
}
.input_content{
    display: flex;
    align-items: center;
    margin-left: 20rpx;
    width:535rpx;
    height:60rpx;
    border-radius:30rpx;
    padding: 0 20rpx;
    box-sizing: border-box;
}
.icon_search{
    display: inline-block;
    vertical-align: middle;
    width: 40rpx;
    height: 40rpx;
}
.icon_brand{
  position: absolute;
  left: 20rpx;
  top: 50%;
  transform: translateY(-50%);
}
.search_input{
    display: inline-block;
    vertical-align: middle;
    margin-left: 20rpx;
    width: 100%;
    font-size:24rpx;
    height: 60rpx;
    line-height: 60rpx;
}
.search_input_2{
  display: inline-block;
  vertical-align: middle;
  font-size:24rpx;
  height: 60rpx;
  line-height: 60rpx;
  color: #B2B2B2;
}
.change_style{
    font-size:24rpx;
    color: #B2B2B2;
}
.search_wrapper_2{
    display: flex;
    align-items: center;
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
}
.brandIconBox{
  width:200rpx;
  height:60rpx;
  padding:0 20rpx;
}
.icon_brandIcon{
  width:200rpx;
  height:60rpx;
} 
.input_content_2{ 
    border-radius:30rpx;
    height:60rpx;
    box-sizing: border-box; 
    max-width: 340rpx;
    /* max-width: 390rpx; */
    display: flex;
    justify-content: center;
    align-items: center;
    position: relative;
    width: 100%;
}
 