} */
.user_func_stay .func_icon{
  width: 80rpx;
  height: 80rpx;
  margin: 0 auto;
  background: center center no-repeat;
  background-size: auto 100%;
}
.service_txt{
  padding:10rpx 0px;
}
.service_item .r_arrow {
  position: absolute;
  top: 50%;
  right: 10rpx;
  margin-top: -25rpx;
}