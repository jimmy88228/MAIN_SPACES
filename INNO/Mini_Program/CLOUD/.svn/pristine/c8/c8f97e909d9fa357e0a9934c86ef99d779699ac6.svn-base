.service_area {
  width: 100%;
  padding-left: 25rpx;
  box-sizing: border-box;
  overflow: hidden;
  display:flex;
  flex-wrap: wrap;
}
.item_area{
  flex-shrink: 0;
  border-bottom:1px solid #efefef;
}
.service_item{
  padding:10rpx 0px;
  position: relative;
}
.user_func_stay {
  vertical-align: middle;
  position: relative;
}
