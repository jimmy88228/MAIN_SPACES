.select_area{
  padding:120rpx 10rpx 300rpx 40rpx;
  background-color:#fff;
}
.select_line{
  display:flex;
  justify-content: flex-start;
  align-items: center;
  margin-bottom:35rpx;
  padding:20rpx 0px;
  font-size: 32rpx;
  line-height: 32rpx;
}
.s_icon,.no_s_icon{
  width:35rpx;
  height:35rpx;
  margin-right:10rpx;
  display:block;
}
.agree_area{
  padding:20rpx 0px;
  font-size:22rpx;
  text-align: center;
}
.next_btn{
  width: 240rpx;
  height: 90rpx;  
  display:flex;
  margin:20rpx auto;
  justify-content: center;
  align-items: center;
  border-radius: 10rpx;
  color:#fff;
}

.agree_list{
  padding:34rpx;
  padding-top:40rpx;
  padding-bottom:30rpx;
  width:100%;
  box-sizing: border-box;
  line-height:normal;
  /* background-color:#fff; */
  display:flex;
  align-items: flex-start;
  flex-wrap: wrap;
  justify-content: center;
}

.agree_item{
  padding:5rpx 0px;
  display:inline-block;
  vertical-align: middle;
  flex-shrink: 0;
}
.agree_tip{
  white-space: nowrap;
  padding-right: 28rpx;
}
