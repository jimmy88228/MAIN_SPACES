.cs_link_area{
  position:fixed;
  top:0px;
  left:0px;
  z-index:15;
  border-radius:100%;
  width:90rpx;
  height:90rpx;
  -moz-box-shadow:0px 0px 10rpx #969696;
  -webkit-box-shadow:0px 0px 10rpx #969696;
  box-shadow:0px 0px 10rpx #969696;
  background-color:#fff;
  text-align:center;
  font-size:24rpx;
  border-top-right-radius: 0;
  border-bottom-right-radius: 0;
}
.posLeft{
  border-radius:100%;
  border-top-left-radius: 0;
  border-bottom-left-radius: 0;
}
.cs_link{
  position:relative;
  width:100%;
  height:100%;
  z-index:10;
}
.cs_link_img{
  position:absolute;
  display:block;
  width:100%;
  height:100%;
  top:50%;
  left:50%;
  transform: translate(-50%,-50%);
}
.cs_link_txt{
  margin-top:40rpx;
  text-align:center;
  font-size: 20rpx;
}
.user_authorize_btn{
  position:absolute;
  top:0px;
  left:0px;
  width:100%;
  height:100%;
  z-index:11;
}
