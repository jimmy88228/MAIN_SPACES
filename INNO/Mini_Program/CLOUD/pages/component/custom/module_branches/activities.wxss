/*活动样式*/

.activity_module {
  padding: 10rpx 25rpx;
  position: relative;
  letter-spacing: 0.5px;
}

.act_module_bg {
  position: absolute;
  top: 0px;
  left: 0px;
  z-index: 0;
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  /* padding:0px 25rpx; */
}

.act_m_img_bg {
  width: 100%;
  height: 100%;
  display: block;
}

.activity_module_stay {
  box-shadow: 0px 2px 10px 0px rgba(0, 0, 0, 0.1);
  background: #fff;
  border-radius: 10rpx;
  overflow: hidden;
}

.act_title {
  /* height: 100rpx; */
  padding: 0px 20rpx;
  background: #fff;
}

.act_title .hold_cell {
  padding-top: 28rpx;
  padding-bottom: 28rpx;
}

.act_count_down {
  font-weight: normal;
}

.time_down_txt {
  margin-left: 24rpx;
  margin-right: 15rpx;
}

.day_down {
  margin-right: 10rpx;
  min-width: 45rpx;
}

.time_down {
  width: 42rpx;
  height: 42rpx;
  background-color: #efefef;
  line-height: 42rpx;
  text-align: center;
}

.act_item {
  box-sizing: border-box;
  width: 100%;
  padding-bottom: 10rpx;
}

.act_item .act_goods_info {
  box-sizing: border-box;
}

.act_item .act_goods_name {
  font-size: 22rpx;
  word-break: break-all;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  line-height: 30rpx;
  position: relative;
  z-index: 2;
  height:60rpx;
}

.act_item .original_p {
  padding: 0px;
  margin: 0px;
  font-weight:normal;
}

.act_img_view {
  position: relative;
  width: 100%;
  padding-top: 100%;
}

.act_img_view_stay {
  position: absolute;
  top: 0px;
  left: 0px;
  width: 100%;
  padding-top: 100%;
  height: 0px;
  background-color: #fff;
}

.act_img_view_stay image {
  display: block;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  background-color:#fff;
}

.img_view_border {
  position: absolute;
  top: 0px;
  left: 0px;
  border: 2rpx solid #000;
  opacity: 0.18;
  width: 100%;
  height: 100%;
  z-index: 2;
}

.activity_name {
  font-size: 30rpx;
}

.act_tip {
  position: absolute;
  top: 7rpx;
  left: 6rpx;
  width: 62rpx;
  height: 26rpx;
  line-height: 18rpx;
  color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 2;
  font-size: 18rpx;
}

.act_img_view image {
  width: 100%;
  height: 100%;
  display: block;
  backgrond-color:#fff;
}

.act_list {
  padding: 0rpx 10rpx;
  padding-bottom: 22rpx;
}

.act_btn {
  color: #fff;
  width: 160rpx;
  height: 60rpx;
  line-height: normal;
  border-radius: 4rpx;
  margin-left: auto;
}
.json_act_count {
  display: inline-flex;
  width: auto !important;
  height: 26rpx;
  font-size: 18rpx;
  border: 1rpx solid #eb213a;
  border-radius: 100rpx;
  justify-content: center;
  align-items: center;
  margin-top:10rpx;
  padding:0px 10rpx;
  white-space:nowrap;
}
.act_item .price_tip {
  font-size: 18rpx;
}

.act_item .price_num {
  font-size: 22rpx;
  white-space: nowrap;
}

.act_item .original_p {
  font-size: 18rpx;
}

