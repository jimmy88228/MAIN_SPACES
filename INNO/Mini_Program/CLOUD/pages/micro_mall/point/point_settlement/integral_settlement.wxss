/*配送方式弹框*/

.shipping_way_alert_bg {
    position: fixed;
    top: 0px;
    left: 0px;
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.6);
    z-index: 3;
}

.shipping_way_alert {
    position: fixed;
    top: 300rpx;
    left: 50%;
    width: 600rpx;
    background: #fff;
    border-radius: 6rpx;
    overflow: hidden;
    z-index: 3;
    margin-left: -300rpx;
    text-align: center;
    /* transform:translate(-50%,-50%);
  -ms-transform:translate(-50%,-50%);
  -moz-transform:translate(-50%,-50%);
  -webkit-transform:translate(-50%,-50%);
  -o-transform:translate(-50%,-50%); */
}

.shipping_way_alert .radio_select {
    position: static;
    display: inline-block;
    vertical-align: middle;
}

.shipping_way_tit {
    height: 80rpx;
    line-height: 80rpx;
    text-align: left;
    padding-left: 30rpx;
    border-bottom: 1px solid #e9e9e9;
}

.shipping_way_cont {
    height: 110rpx;
}

.shipping_way_submit {
    background: #000;
    color: #fff;
    height: 88rpx;
    line-height: 88rpx;
}

.shipping_way_alert_bg.is_show {
    animation: bg_show 0.5s;
    -moz-animation: bg_show 0.5s;
    -ms-animation: bg_show 0.5s;
    -o-animation: bg_show 0.5s;
    -webkit-animation: bg_show 0.5s;
    animation-fill-mode: forwards;
    -moz-animation-fill-mode: forwards;
    -ms-animation-fill-mode: forwards;
    -o-animation-fill-mode: forwards;
    -webkit-animation-fill-mode: forwards;
}

.shipping_way_alert_bg.is_hide {
    animation: bg_hide 0.5s;
    -moz-animation: bg_hide 0.5s;
    -ms-animation: bg_hide 0.5s;
    -o-animation: bg_hide 0.5s;
    -webkit-animation: bg_hide 0.5s;
    animation-fill-mode: forwards;
    -moz-animation-fill-mode: forwards;
    -ms-animation-fill-mode: forwards;
    -o-animation-fill-mode: forwards;
    -webkit-animation-fill-mode: forwards;
}

.shipping_way_alert.is_show {
    animation: show_alert 0.5s;
    -moz-animation: show_alert 0.5s;
    -ms-animation: show_alert 0.5s;
    -o-animation: show_alert 0.5s;
    -webkit-animation: show_alert 0.5s;
    animation-fill-mode: forwards;
    -moz-animation-fill-mode: forwards;
    -ms-animation-fill-mode: forwards;
    -o-animation-fill-mode: forwards;
    -webkit-animation-fill-mode: forwards;
}

.shipping_way_alert.is_hide {
    animation: hide_alert 0.5s;
    -moz-animation: hide_alert 0.5s;
    -ms-animation: hide_alert 0.5s;
    -o-animation: hide_alert 0.5s;
    -webkit-animation: hide_alert 0.5s;
    animation-fill-mode: forwards;
    -moz-animation-fill-mode: forwards;
    -ms-animation-fill-mode: forwards;
    -o-animation-fill-mode: forwards;
    -webkit-animation-fill-mode: forwards;
}


