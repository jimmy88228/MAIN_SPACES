@keyframes animShow_cart
{
0% {
    opacity: 1;
    transform: translate(20%,-20%) scale(1,1);
}
5% {
    opacity: 1;
    transform: translate(20%,-20%) scale(0.6,0.6);
}
15% {
    opacity: 1;
    transform: translate(20%,-20%) scale(1,1);
}
80% {
    opacity: 1;
    transform: translate(20%,-20%) scale(1,1);
}
90% {
    opacity: 0.5;
    transform: translate(20%,-40%) scale(1,1);
}
100% {
    opacity: 0.2;
    transform: translate(20%,-50%) scale(1.2,1.2);
}
}

 
