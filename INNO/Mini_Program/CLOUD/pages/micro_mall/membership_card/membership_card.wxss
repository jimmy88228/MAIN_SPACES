  position: relative;
  background: #fff;
  border-radius: 10rpx;
}
.protocol {
  position: absolute;
  right: 28rpx;
  top: 32rpx;
}
.protocol_link{
  display:inline-block;
}

.membership_card_box {
  text-align: center;
  margin-top: 232rpx;
  margin-bottom: 46rpx;
}

.membership_card {
  width: 590rpx;
}

.code_img_box {
  text-align: center;
}

.code_img {
  width: 480rpx;
  height: 120rpx; 
}

.code_text {
  margin-top: 20rpx;
  font-size: 24rpx;
}

.use_tip {
  font-size: 28rpx;
  margin-top: 40rpx;
  font-weight: bolder;
}

.protocol_text,
.tip_icon {
  display: inline-block;
  vertical-align: middle;
}

.protocol_text.android {
  margin-top: 6rpx;
}

.tip_icon {
  width: 20rpx;
  height: 20rpx;
  margin-right: 10rpx;
}

.code_img_box.center {
  position: absolute;
  left: 50%;
  top: 48%;
  transform: translate(-50%, -50%);
}

.barCanvas{
  width:500rpx;
  height:150rpx;
  margin:0 auto;
}

.logos{
    position: relative;
    width: 502rpx;
    height: 279rpx;
    margin: 0 auto;
    border-radius:10rpx;
    margin-top: 178rpx;
    margin-bottom: 43rpx;
}
.logo_pic{
    display: block;
    width: 100%;
    height: 100%;
}
.bg{
    position: absolute;
    left: 0;
    top: 0;
    display: block;
    width: 502rpx;
    height: 279rpx;
}
.wrapper{
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
    width: 350rpx;
    height: 75rpx;
}