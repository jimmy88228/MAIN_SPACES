<template>
    <rewrite-area>
        <Form :label-width="80" class="no-tip flex-b-c" inline>
            <div>
                <FormItem :label-width="0">
                    <rewrite-search v-model="searchForm.searchq" @search="search" placeholder="请输入姓名进行搜索"></rewrite-search>
                </FormItem>
                <FormItem label="审核状态">
                    <Select class="base-select" @on-change="search" v-model="searchForm.state">
                        <Option v-for="item in stateList" :key="item.id" :value="item.id">{{item.name}}</Option>
                    </Select>
                </FormItem>
                <linkageSelect isShowGauge :hideSelect="['structure']" :searchForm="searchForm" @on-change="search"></linkageSelect>
            </div>
            <div class="flex"></div>
        </Form>
    </rewrite-area>
</template>

<script>
import linkageSelect from "@/models/components/linkage-select";

export default {
    name: "earlyWarnExamineSearchForm",
    components: {linkageSelect},
    data() {
        return {};
    },
    props: {
        searchForm: {
            type: Object,
            default: () => {},
        },
        stateList: {
            type: Array,
            default:()=>{
                return []
            }
        }
    },
    methods: {
        search() {
            this.$emit("search");
        },
    },
};
</script>

<style lang="less" scoped></style>