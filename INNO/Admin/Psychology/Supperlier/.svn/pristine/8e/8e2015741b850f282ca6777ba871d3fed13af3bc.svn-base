.dialog-edit-group {
    display: flex;
    flex-direction: column;

    >.dialog-edit-title {
        margin-top: 10px;
        font-size: 13px;
    }

    >.dialog-edit-item {
        display: flex;
        width: 100%;
        flex-direction: row;
        align-items: center;
        padding: 5px 10px;
        max-width: 300px;
        min-width: 250px;

        >.name {
            display: inline-block;
            flex: none;
            min-width: 60px;
            white-space: nowrap;
            word-wrap: break-word;
            word-break: break-all;
            text-align: justify;
            text-align-last: justify;
        }

        >.cev {
            display: inline-block;
            margin-left: 10px;
            flex: 1;
        }
    }

    >*:first-child {
        margin-top: 0;
    }
}

.dialog-edit-group.center {
    align-items: center;
}

