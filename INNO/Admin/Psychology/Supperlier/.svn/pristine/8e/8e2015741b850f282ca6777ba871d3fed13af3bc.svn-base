.list-box {
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;

    >* {
        margin-top: 10px;
    }

    >*:first-child {
        margin-top: 0;
    }
}

.bg-bottom-toolbar {
    border-top-color: #e9eaec;
    border-top-width: @border-width-base;
    border-top-style: @border-style-base;
    box-shadow: @shadow-card;
    background: white;
}

.cev-toolbar,
.cev-toolbar>.left,
.cev-toolbar>.right {
    flex: none;
    display: flex;
    flex-direction: row;
    align-items: center;
}

.cev-toolbar {
    width: 100%;
    justify-content: space-between;
}

.cev-toolbar>.left,
.cev-toolbar.start {
    justify-content: flex-start;

    >* {
        margin-left: 10px;
    }

    >*:first-child {
        margin-left: 0px;
    }
}

.cev-toolbar>.right,
.cev-toolbar.end {
    justify-content: flex-end;

    >* {
        margin-right: 10px;
    }

    >*:last-child {
        margin-right: 0px;
    }
}

