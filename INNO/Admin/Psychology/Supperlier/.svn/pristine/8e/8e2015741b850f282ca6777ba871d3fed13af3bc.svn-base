/* 链接图标 */
.arrow-radius {
    width: 22px;
    height: 22px;
    background: #f6f6f6;
    border-radius: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    position: absolute;
    bottom: 13px;
    right: 9px;
}

.arrow-radius::after {
    content: "";
    display: block;
    width: 7px;
    height: 7px;
    border: 2px solid #4e5969;
    transform: translateX(-25%) rotate(45deg);
    border-left-color: transparent;
    border-bottom-color: transparent;
}

._tip {
    color: #7f7f7f;
    font-size: 12px;
}

.relative {
    position: relative;
}

.fixed {
    position: fixed;
}

.absolute {
    position: absolute;
}

.translate-center {
    left: 50%;
    top: 50%;
    transform: translate(-50%, -50%);
}


