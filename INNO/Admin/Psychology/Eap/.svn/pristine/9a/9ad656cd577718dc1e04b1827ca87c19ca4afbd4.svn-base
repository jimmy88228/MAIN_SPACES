<template>
    <rewrite-area>
        <Form class="no-tip flex-b-c" inline>
            <div class="flex">
                <FormItem label="心理咨询师" :label-width="100">
                  <data-select v-model="searchForm.consultation_id" type="psychiatrist" valueKey="consultantId" nameKey="name" @change="search()">
                      <Option slot="default-option" :value="0">全部</Option>
                  </data-select>
                </FormItem>
                <FormItem label="咨询结果" :label-width="100">
                    <Select v-model="searchForm.consultation_result" @on-change="search()">
                        <Option :value="item.id" v-for="item in result_list" :key="item.id">{{item.name}}</Option>
                    </Select>
                </FormItem>
            </div>
            <!-- <div class="flex">
                <Button @click="batch" v-hasAction="true">批量处理</Button>
            </div> -->
        </Form>
    </rewrite-area>
</template>

<script>
export default {
    name: "actIndexSearchForm",
    data() {
        return {
            result_list:[{
                id:-1,
                name:"全部"
            },{
                id:0,
                name:"未处理"
            },{
                id:1,
                name:"已处理"
            }]
        };
    },
    props: {
        searchForm: {
            type: Object,
            default: () => {},
        },
        stateKey: {
            type: Object,
            default: () => {},
        },
    },
    methods: {
        search() {
            this.$emit("search");
        },
        batch(){
            this.$emit("batch")
        }
    },
};
</script>

