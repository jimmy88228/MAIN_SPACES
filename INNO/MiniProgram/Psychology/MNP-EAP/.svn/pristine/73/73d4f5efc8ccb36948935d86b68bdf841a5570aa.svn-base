import Conf from "../../config/config.js";
import { WechatApiList, UserApiList, EduApiList,EvaluateApiList, pageManageApiList,resourceApiList,consultantApiList, commonApiList} from './http.api.js';
function addRequests(apiList, apiHost){
	if(apiList instanceof Object){
		for(let i in apiList){
			let api  = apiList[i];
			if(api instanceof Object){
				api.u = apiHost + api.u;
				api.m = api.m ? api.m : '';
			} else if(typeof(api) == 'string'){
				api = apiHost + api
			}
			apiList[i] = api;
		}
		return apiList;
	}
	return {};
}
let apiDomain = Conf.apiDomain || {};

// 拼装API
export const Apis = {
	...addRequests(WechatApiList, apiDomain.WECHATAPI),
	...addRequests(EduApiList, apiDomain.EDEAPI),
	...addRequests(UserApiList, apiDomain.USERAPI),
	...addRequests(EvaluateApiList, apiDomain.EVALUATEAPI),
	...addRequests(pageManageApiList, apiDomain.PAGEAPI),
	...addRequests(resourceApiList, apiDomain.RESOURCEAPI),
	...addRequests(consultantApiList, apiDomain.CONSULTANT),
	...addRequests(commonApiList, apiDomain.COMMONAPI),
	
}
console.log("install", Apis)
