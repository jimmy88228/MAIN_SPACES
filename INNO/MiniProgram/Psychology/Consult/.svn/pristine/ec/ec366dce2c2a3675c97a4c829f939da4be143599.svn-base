.room{ 
    background-color:#f7f7f7;
    min-height: 100vh;
    padding: 25rpx;
    box-sizing: border-box;
}
.btn-box{  
    width: 100%; 
    box-sizing: border-box;
    background:#FFFFFF;
    padding: 30rpx;
    position: fixed;
    left: 0;
    bottom: 0;
}
.btn{
    width: 100%;
    height: 100rpx;
    border-radius: 10rpx;   
    background: #21B014;
    color: #fff;
    font-size: 32rpx;
}   
.box{
    padding: 30rpx;
    box-sizing: border-box;
    border-radius: 20rpx;
    margin-top: 20rpx;
    background-color: #fff;
}