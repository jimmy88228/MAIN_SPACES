page {
    font-size: 26rpx;
    color: #222222;
    font-family: Arial;
}

image {
    display: block;
    height: auto;
    image-rendering:-moz-crisp-edges;
    image-rendering:-o-crisp-edges;
    image-rendering: crisp-edges;
    image-rendering:-webkit-optimize-contrast;
    -ms-interpolation-mode:nearest-neighbor;
    will-change: transform
}

button {
    margin: 0;
    display: flex;
    justify-content: center;
    align-items: center;
}

button::after {
    border: none;
}

// layoute
.fixed {
    position: fixed;
}

.flex {
    display: flex;
}

.flex1,
.flex-1 {
    flex: 1;
}

.flex-col {
    flex-direction: column;
}

.flex-c-c {
    display: flex;
    justify-content: center;
    align-items: center;
}

.flex-s-c {
    display: flex;
    justify-content: flex-start;
    align-items: center;
}

