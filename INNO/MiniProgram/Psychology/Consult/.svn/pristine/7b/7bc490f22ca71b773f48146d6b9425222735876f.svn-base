    display: flex;
    justify-content: flex-end;
}

.flex-c-s {
    display: flex;
    justify-content: center;
    align-items: flex-start;
}

.flex-b-c {
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.flex-b-s {
    display: flex;
    justify-content: space-between;
    align-items: flex-start;
}

.flex-b-e {
    display: flex;
    justify-content: space-between;
    align-items: flex-end;
}

.flex-e-c {
    display: flex;
    justify-content: flex-end;
    align-items: center;
}

.flex-a-s {
    display: flex;
    justify-content: space-around;
    align-items: flex-start;
}

.flex-a-c {
    display: flex;
    justify-content: space-around;
    align-items: center;
}

.flex-a-e {
    display: flex;
    justify-content: space-around;
    align-items: flex-end;
}

.flex-col-1 {
    display: flex;
    flex: 1;
    flex-direction: column;
}

.f-shrink-0 {
    flex-shrink: 0;
}

.f-wrap,
.flex-wrap {
    flex-wrap: wrap;
}

.fixed-X-50 {
    position: fixed;
}

.translateX-50,
.fixed-X-50 {
    left: 50%;
    transform: translateX(-50%);
}

.fixed-full {
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
}

// adapters
.customTab {
    padding-bottom: 90rpx !important;
}

.ipx_p_b,
.fix_iphoneX {
    padding-bottom: 34px !important;
}

.customTab.ipx_p_b,
.customTab.fix_iphoneX {
    padding-bottom: calc(90rpx + 34px) !important;
}

.ipx_b_b {
    bottom: 34px !important;
}

.customTab.ipx_b_b {
    padding-bottom: 0 !important;
    bottom: calc(90rpx + 34px) !important;
}

.padding-area-inset-bottom {
    padding-bottom: env(safe-area-inset-bottom);
    padding-bottom: constant(safe-area-inset-bottom);
}

.bottom-area-inset-bottom {
    bottom: env(safe-area-inset-bottom);
    bottom: constant(safe-area-inset-bottom);
}

