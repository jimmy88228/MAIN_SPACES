@import "/common/base/component/default-wxss/index.wxss";
.custom-nav-container{
  padding-top: var(--status-bar-height);
}
.nav-box{
  height: var(--nav-bar-height);
}
/* 普通样式类型 */
.normal-nav{
  height: 100%;
}
.icon-arrow.left{
  transform: rotate(-135deg);
  width: 24rpx;
  height: 24rpx;
  margin-left: 20rpx;
  color: var(--deep-text-color);
}
.normal-nav{
  display: flex;
  align-items: center;
}
.c-n-left,
.c-n-right,
.c-n-center{
  flex: 1;
}
.c-n-center{
  text-align: center;
}

/* 有店铺的那种样式类型 */
.store-like-nav{
  display: flex;
  align-items: center;
  height: 100%;
  padding-left: 12rpx;
}
.store-like-nav .back-or-home-btn{
  margin-right: 12rpx;
}
.store-logo-con{
  width: 72rpx;
  height: 72rpx;
  margin-right: 20rpx;
}
.store-logo-image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.store-like-nav .store-name{
  color: #ffffff;
  font-size: 28rpx;
}
.store-like-nav .store-staff-name{
  color: #9D1628;
  font-size: 22rpx;
}
