

<transition show="{{show}}" >
  <view class="sku-selector">
    <view class="sku-con">
      <view class="sku-type-list">
        <view class="sku-type-item"  wx:for="{{type}}" wx:for-item="skuTypeList"  wx:for-index="typeIndex" wx:key="index" >
          <view class="sku-type-header">sku类型</view>
          <view class="sku-list">
            <view class="sku-item {{availablePrimeRef[(valueInLabel[skuItem])] ? '': 'disabled'}} {{selectedRef[skuItem] ? 'selected' : ''}}" wx:for="{{skuTypeList}}" wx:for-item="skuItem" wx:key="index" catchtap="onClickSelType" data-type="{{skuItem}}" data-prime="{{valueInLabel[skuItem]}}" data-prime-index="{{typeIndex}}">
              {{skuItem}}
            </view>
          </view>
        </view>
      </view>
    </view>
  </view>
</transition>
<include src="/common/base/component/index.wxml" />