@import "/common/base/component/default-wxss/index.wxss";

.product-list-box {
  position: fixed;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  width: 690rpx;
  height: 80vh;
  overflow: hidden;
  border-radius: 40rpx;
}

.product-list {
  position: relative;
  width: 100%;
  height: 100%;
  overflow: hidden;
}