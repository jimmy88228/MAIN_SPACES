page {
  font-size: 26rpx;
  background:#F2F2F2;
  color:#333;
}
view, text, scroll-view {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}
image{
  display: block;
}
.button-hover{
  background-color: unset;
  color: unset
}
button{
  display:flex;
  align-items:center;
  justify-content:center;
  background: none;
  margin: 0;
  padding: 0;
  white-space:nowrap;
  border-radius: 0;
  font-size: 24rpx;
  overflow: visible;
}
::-webkit-scrollbar {
  display:none;
  width:0;
  height:0;
  color:transparent;
}