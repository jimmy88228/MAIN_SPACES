@import "/common/base/component/default-wxss/index.wxss";

.nav-box{
  width: 100%;
}

.nav-stay {
  width: 100%;
}

.nav {
  width: 100%;
  color: #333;
  box-sizing: border-box;
  height: 0;
}

.fixed {
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  z-index: 1001;
}

.nav-bg {
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  background: white;
  transition: 0.5s all;
}

.nav-body {
  position: relative;
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: row;
  align-items: center; 
  box-sizing: border-box;
}

.back-link {
  display: inline-flex;
  align-items: center;
  padding-left: 30rpx;
  padding-right: 30rpx;
  height: 100%;
}

.back-link-hover {
  background-color: rgba(127, 127, 127, 0.05);
}

.back-i {
  width: 32rpx;
  height: 32rpx;
}
.back-i-v2 {
  width: 44rpx; 
}

.back-i-close {
  width: 56rpx;
  height: 56rpx;

}

.custom-content-box {
  display: inline-block;
  font-size: 30rpx;
}

.title-content {
  /* position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%); */
  font-size: 34rpx;
  max-width: 380rpx;
  overflow: hidden;
  text-overflow: ellipsis;
}