.btn_box{
  position: relative;
  z-index: 9;
  margin-right: 20rpx;
}

.btn_box:last-child{
  margin-right: 0;
}

.btn_box:last-child .oifr-btn{
  color: var(--main-color);
  border-color: var(--main-color); 
}


.returnMsg{ 
  width: 100%;
  height: 80rpx;
  background-color: #fff;
  color: #7f7f7f;
  font-size: 24rpx; 
  box-sizing: border-box;
  padding:0 38rpx 0 30rpx;
  border-top: 1px solid #dfdfdf;
}

.r_m_title{
  font-weight: bold;
}
.r_m_num_box{
  height: 100%;
  font-size: 26rpx;
  color: var(--main-color);
}
.r_m_num_item{
  margin-right: 70rpx;
  height: 100%;
  font-weight: bold;
}
.r_m_num_item:last-child{
  margin-right: 0;
}
.arrow_return{
  width: 12rpx;
  height: 12rpx;
  border: 4rpx solid var(--main-color);
  border-top: none;
  border-left: none;
  transform: rotate(-45deg);
  margin-left: 2rpx;
}
