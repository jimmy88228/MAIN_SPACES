}


.order_opreate_btns {
width: 100%;
position: fixed;
left: 0px;
bottom: 0px;
display: flex;
align-items: center;
}

.order_opreate_btns_stay {
/* padding: 16rpx 0px; */
padding-right: 20rpx;
width: 100%;
box-sizing: border-box;
display: flex;
align-items: center;
height: var(--order-info-footer-height);
/* background-color: #fff; */
position: relative;
justify-content: flex-end;
flex-wrap: wrap;
background-color: #fff;
}


.oifr-btn-group {
  display: flex;
  align-items: center;
}

button.oifr-btn {
  width: 150rpx;
  height: 60rpx;
  border-radius: 30rpx;
  font-size: 24rpx;
  border: 1px solid #b2b2b2;
  color: #7f7f7f;
  /* margin-right: 18rpx; */
  transition: opacity 0.1s ease;
}

button.oifr-btn.active {
  opacity: 0.5;
}

button.oifr-btn:last-of-type {
  margin-right: 0;
}

.btn_box{
  position: relative;
  z-index: 9;
  /* margin-right: 20rpx; */
  width: 100%;
}

.btn_box:last-child{
  margin-right: 0;
}

.btn_box .oifr-btn.refuse{
  color: #F5475F;
  border-color: #F5475F; 
}

.btn_box .oifr-btn.refund{
  background-color: rgb(90, 175, 51);
  color: #fff;
  border-color: rgb(90, 175, 51);
}
.btn_box .oifr-btn.consent,.btn_box .oifr-btn.receive{
  color: rgb(90, 175, 51);
  border-color: rgb(90, 175, 51);
}

.btn_box .oifr-btn{
  color: var(--main-color);
  border-color: var(--main-color); 
  margin:0;
  margin-right: 20rpx;
}

