.remark_area,.remark_area_box{
  width:100%;
  padding:20rpx 16rpx 42rpx 16rpx;
  height:150rpx; 
  box-sizing: border-box;
  color: #7f7f7f;
  /* background-color:#F3F3F3; */
  border: 1px solid #dfdfdf;
  word-break: break-all;
  font-size: 26rpx; 
  overflow: hidden;
  line-height: 44rpx;
}
.custom_remark_area{
  word-break: break-all;
  font-size: 26rpx; 
  overflow: hidden;
  height: 100%;
}
.remark_num{
  position:absolute;
