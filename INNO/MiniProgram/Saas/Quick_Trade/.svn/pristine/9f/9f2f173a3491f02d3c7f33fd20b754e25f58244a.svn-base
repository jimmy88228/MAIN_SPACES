<view class="tab-bar-container" style="{{tabBarStyle}}" hidden="{{hideTabBar}}">
  <view class="tab-bar" wx:if="{{tabBarList && tabBarList.length}}">
    <view class="tab-bar-middle-line"></view>
    <view class="tab-bar-list">
      <view class="tab-bar-item {{selected === item.name ? 'active' : ''}}" wx:for="{{tabBarList}}" wx:key="text" catchtap="handleTabTap" data-url="{{item.pagePath}}">
        <view class="tb-image-con">
          <image class="tb-image" src="{{item.iconPath}}" style="visibility: {{selected === item.name ? 'hidden' : 'visible'}};" />
          <image class="tb-image" src="{{item.selectedIconPath}}" style="visibility: {{selected === item.name ? 'visible' : 'hidden'}};" />
        </view>
        <view class="tb-text">
          <text>{{item.text}}</text>
        </view>
      </view>
    </view>
  </view>
</view>
<include src="/common/base/component/index.wxml" />