
page {
    background: #f3f3f3;
    width:100%;
    height:100%;
}
.err_tip {
    color: #db585a;
    margin: 8rpx 0px;
    font-size: 22rpx;
}

.area_list {
    padding: 0rpx;
    padding-bottom: 100rpx;
    border-top: 1px solid #efefef;
    /* width:100%;
    height:100%; */
    /* box-sizing: border-box; */
}
.area_view{
    width:100%;
    height:100%;
}

.area_box {
    display: block;
    width: 100%;
    margin-bottom: 10rpx;
    padding: 30rpx;
    box-sizing: border-box;
    position: relative;
    background: #fff;
}

.margin_0 {
    margin-bottom: 0;
}

.area_box:first-child {
    border-top: 0;
}

.gift_card_box {
    padding-right: 0px;
}

.area_box .title {
    color: #7f7f7f;
    font-size: 24rpx;
}

.area_box .tip {
    margin-top: 10rpx;
    font-size: 22rpx;
}

.area_box .red_select_msg {
    margin-left: auto;
    padding-right: 35rpx;
}

.area_box .red_select_msg.none {
    padding-right: 10rpx;
}

.r_arrow {
    width: 50rpx;
    position: absolute;
    /* top: 10rpx; */
    top: 50%;
    transform: translateY(-50%);
    right: 18rpx;
    display: block;
}



.radio_select {
    position: absolute;
    right: 8rpx;
    top: 50%;
    transform: translateY(-50%);
    width: 75rpx;
    height: 75rpx;
    display: inline-flex;
    align-items: center;
    justify-content: center;
}

.radio_icon {
    width: 31rpx;
    height: 31rpx;
    display: block;
}
.click_mask{
  position:absolute;
  top:0px;
  left:0px;
  width:100%;
  height:100%;
  background-color:rgba(0,0,0,0.1);
}
.coupon_item {
    padding-right: 60rpx;
}
.coupon_item .r_arrow {
    position: absolute;
    top: 50%;
    right: 18rpx;
    transform: translateY(-50%);
    -ms-transform: translateY(-50%);
    -moz-transform: translateY(-50%);
    -webkit-transform: translateY(-50%);
    -o-transform: translateY(-50%);
}
.coupon_item .title {
    padding: 0px;
}
.area_box.padding_0 {
    padding: 0rpx 30rpx 0rpx 30rpx;
}
.coupon_box {
    color: #1b1b1b;
    display: flex;
    align-items: center;
    min-height: 100rpx;
    /* padding:10rpx 0px; */
    padding-right:40rpx;
}

.coupon_content {
    color: #fff;
    padding-left: 11rpx;
    border-radius: 4px;
    margin-left:auto;
}

.coupon_box.none_coupon {
    justify-content: space-between;
    padding-right:0px;
}

.less_padding {
    padding: 38rpx;
    padding-right: 24rpx;
    padding-left: 40rpx;
}

.express_info_box {
    padding: 0px;
    background:#f3f3f3;
}

.express_info_item {
    padding: 30rpx;
    padding-bottom: 20rpx;
    border-bottom: 1px solid #e9e9e9;
    position: relative;
    background: #fff;
}

.express_info_box .express_info_item:last-child {
    border: none;
}
.express_info_item.borderNone{
    border:none;
}
.goIdentity {
    width: 100%;
    height: 100rpx;
    position: relative;
    box-sizing: border-box;
    display: flex;
    justify-content: space-between;
    font-size: 24rpx;
    padding: 0 18rpx 0 30rpx;
    margin-top:10rpx;
    background: #fff;
}

.goIdentityPart {
    height: 100%;
    display: inline-flex;
    align-items: center;
}
.tip_title {
    color: #b2b2b2;
}
.rightClass {
    width: 50rpx;
}
.checkClass{
    width: 18rpx;
}
.checkText{
    color: #50C41D;
    margin:0 12rpx;
}
.goods_item {
    width: 100%;
    border-bottom: 1px solid #efefef;
    padding: 20rpx 0px;
}
.goods_item:last-child {
    border: none;
    padding-bottom: 0;
}
.goods_img_box {
    width: 215rpx;
    display: inline-block;
    vertical-align: top;
    overflow: hidden;
    border: 1px solid rgba(230, 230, 230, 1);
}
.goods_img_box_stay {
    width: 100%;
    padding-top: 100%;
    overflow: hidden;
    position: relative;
}
.goods_img_box_stay image {
    display: block;
    width: 100%;
    height: 100%;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
}
.goods_info {
    display: inline-flex;
    flex-wrap: wrap;
    padding-left: 30rpx;
    position: relative;
    min-height: 215rpx;
    width: 470rpx;
    box-sizing: border-box;
}

.goods_info_top {
    width: 100%;
}

.nationBuy {
    height:36rpx;
    padding: 0 12rpx;
    display:inline-flex;
    align-items:center;
    font-size: 20rpx; 
    color: #fff;
    border-radius:5rpx;
    margin-right:10rpx;
}

.goods_msg {
    display: flex;
    align-self: flex-end;
    flex-direction: column;
}

.goods_msg > view {
    font-weight: bold;
}

.shop_price {
    font-size: 26rpx;
    font-weight: bold;
}

.present_price {
    font-size: 26rpx;
    font-weight: bold;
}

.market_price {
    text-decoration: line-through;
}

.goods_name {
    word-break: break-all;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    padding-right: 60rpx;
    margin-top: 15rpx;
}

.goods_number {
    position: absolute;
    top: 16rpx;
    right: 0px;
}

.use_cash_coupon_item {
    position: relative;
    line-height: 70rpx;
    border-bottom: 1px dashed #e9e9e9;
}

.coupon_del {
    width: 50rpx;
    position: absolute;
    top: 50%;
    right: 20rpx;
    margin-top: -25rpx;
}

.cash_ticket_input {
    width: 100%;
    box-sizing: border-box;
    height: 70rpx;
    padding-left: 10rpx;
    padding-right: 170rpx;
    border: 1px solid #e9e9e9;
}

.use_cash_ticket {
    width: 160rpx;
    line-height: 70rpx;
    height: 70rpx;
    position: absolute;
    top: 0px;
    right: 0px;
    color: #fff;
    text-align: center;
} 
/* .use_coupon_tips{
  text-align:right;
} */
.use_coupon_tip {
    font-size: 22rpx;
    display: inline-block;
    padding: 12rpx 16rpx;
    border-radius: 8rpx;
    border: 1px solid #ababab;
    margin:8rpx 0px;
    text-align:center;
}
.gift_card_item {
    position: relative;
    line-height: 50rpx;
}

.gift_card_item .type_name{
  margin-top:10rpx;
}
.gift_card_item .message {
    font-size: 22rpx;
    line-height: 30rpx;
    display:block;
}

.gift_card_item .radio_select {
    bottom: auto;
    top: 0px;
    transform: translateY(-18%);
}
.gift_card_item .gifts_item{
  display:flex;
  align-items: flex-start;
  padding-top:5rpx;
}
.gift{
  display:flex;
  align-items: center;
  padding-bottom:5rpx;
}
.gift_img{
  display:block;
  width:50rpx;
  height:50rpx;
  border:1px solid #efefef;
  margin-right:10rpx;
}
.gift_status{
  color:#db585a;
}
/* .area_val {
    position: absolute;
    top: 20rpx;
    right: 50rpx;
    font-size: 22rpx;
} */

.postage_info .cont_item {
    display: inline-flex;
    justify-content: space-between;
    width: 100%;
    margin: 10rpx 0px;
}

.postage_info .cont_item .line-through-text{
    padding-right: 8rpx;
    color: #999999;
    text-decoration: line-through;
}
.postage_info .cont_item+.cont_item_tips{
    color: #888888;
    font-size: 0.9em;
}

.postage_info .cont_item:last-child {
    margin: 10rpx 0 0 0;
}

.postage_info .cont_item .tit {
    display: table-cell;
    vertical-align: middle;
    width: 130rpx;
}

.postage_info .cont_item .val {
    display: table-cell;
    vertical-align: middle;
}

.goods_text {
    line-height: 28rpx;
    color: rgba(127, 127, 127, 1);
    margin-top: 10rpx;
}
.goods_sn{
    padding-top: 10rpx;
}
.show_remarks_text {
    width: 100%;
    padding: 20rpx;
    box-sizing: border-box;
    min-height: 150rpx;
    word-wrap: break-word;
    word-break: break-all;
    border: 1px solid #e9e9e9;
}
.show_remarks_text.hide{
    visibility: hidden;
}
.placeholder_class {
    font-size: 26rpx;
    color: #7f7f7f;
}

.buy_detail_textarea {
    padding: 20rpx;
    width: 100%;
    height: 150rpx;
    box-sizing: border-box;
    border: 1px solid #e9e9e9;
}

.font_num {
    position: absolute;
    right: 20rpx;
    bottom: 20rpx;
}

.order_total_fixed {
    width: 100%;
    height: 100rpx;
    line-height: 100rpx;
    position: fixed;
    left: 0px;
    bottom: 0px;
    background: #fff;
    border-top: 1px solid #e9e9e9;
    display: inline-flex;
    justify-content: space-between;
    z-index: 2;
}

.order_total {
    display: table-cell;
    vertical-align: middle;
    padding-left: 40rpx;
}

.total_tit {
    vertical-align: middle;
    margin-right: 30rpx;
}

.total_val {
    font-weight: bold;
    font-size: 32rpx;
    vertical-align: middle;
}

.order_submit_box {
    height: 100%;
    display: inline-flex;
    align-items: center;
    justify-content: flex-end;
    padding-right: 18rpx;
    /* width:  */
}

.order_submit {
    vertical-align: middle;
    color: #fff;
    text-align: center;
    width: 383rpx;
    height: 86rpx;
    border-radius: 10rpx;
    display: inline-flex;
    justify-content: center;
    align-items: center;
    line-height:normal;
}

.shipping_way_cont {
    height: 60rpx;
    margin-top: 10rpx;
    padding:0px 100rpx;
    /* padding-top:20rpx; */
   
}
.shipping_way_txt {
    display: inline-block;
    vertical-align: middle;
}
.shipping_way_cont .radio_select{
  position:static;
  display:inline-block;
  vertical-align: middle;
  width:35rpx;
  height:35rpx;
  transform: translate(0);
  margin-right:10rpx;
}
.shipping_way_cont .radio_select image {
    display: block;
    width: 100%;
    height: 100%;
}

.bg {
    position: fixed;
    z-index: 11;
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.7);
    display: none;
    top: 0;
    left: 0;
}

.bg.isShow {
    display: block;
}

.identityWindow.isShow {
    transform: translateY(0);
}

.identityWindow {
    position: fixed;
    width: 100%;
    padding: 30rpx;
    box-sizing: border-box;
    left: 0;
    bottom: 0;
    transform: translateY(100%);
    transition: all 0.4s;
    background: #fff;
    z-index: 111;
    color: #000;
}

.identity_title {
    font-weight: bold;
    font-size: 28rpx;
    line-height: 28rpx;
    margin-bottom: 64rpx;
    position: relative;
    text-align: center;
}

.tips_top {
    font-size: 26rpx;
    line-height: 46rpx;
    margin-bottom: 35rpx;
    font-weight: bold;
}
 
.identity_label {
    display: inline-block;
    vertical-align: middle;
    font-size: 26rpx;
    line-height: 26rpx;
    margin-right: 18rpx;
}

.identity_input {
    width: 530rpx;
    height: 84rpx;
    box-sizing: border-box;
    font-size: 28rpx;
    padding-left: 35rpx;
    display: inline-block;
    vertical-align: middle;
    border:1px solid rgba(239, 239, 239, 1);
}

.identity_card_bottom {
    margin-top: 37rpx;
}

.identity_placeholderClass {
    color: #b2b2b2;
    font-size: 28rpx;
}

.tips_bottom {
    margin-top: 130rpx;
    font-size: 22rpx;
    line-height: 32rpx;
    padding: 0 15rpx;
    color: rgba(127, 127, 127, 1);
    padding-bottom: 34rpx;
    border-bottom: 1px solid #efefef;
}

.identity_button {
    margin-top: 22rpx;
    width: 100%;
    height: 88rpx;
    color: #fff;
    box-sizing: border-box;
    border-radius: 6rpx;
}

.identity_close {
    width: 20rpx;
    height: 20rpx;
    position: absolute;
    right: -14rpx;
    padding: 30rpx;
    top: 50%;
    transform: translateY(-50%);
}

.cash_coupon_scan{
  position:absolute;
  top:0px;
  right:180rpx;
  width:70rpx;
  height:70rpx;
  z-index:2;
}
.scan_icon{
  width:90%;
  height:90%;
}
.promote_assistance{
  width: 100%;
  position: fixed;
  bottom: calc(100rpx + 0rpx);
  left: 0;
}