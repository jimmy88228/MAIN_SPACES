.service_area {
  width: 100%;
  padding-left: 25rpx;
  box-sizing: border-box;
  overflow: hidden;
  display:flex;
  flex-wrap: wrap;
}
.item_area{
  flex-shrink: 0;
  border-bottom:1px solid #efefef;
}
.service_item{
  padding:10rpx 0px;
  position: relative;
}
.user_func_stay {
  vertical-align: middle;
  position: relative;
}
/* .user_func_stay image {
  display: block;
  width: 68rpx;
  height:68rpx;
  background-color:#efefef;
  margin:0 auto;
} */
.user_func_stay .func_icon{
  width: 80rpx;
  height: 80rpx;
  margin: 0 auto;
  background: center center no-repeat;
  background-size: auto 100%;
}
.service_txt{
  padding:10rpx 0px;
}
.service_item .r_arrow {
  position: absolute;
  top: 50%;
  right: 10rpx;
  margin-top: -25rpx;
}

.btn_link {
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  opacity: 0;
  z-index: 2;
}
/*联系导购*/
.btn_link .plugin-private-wx104a1a20c3f81ec2-components-cell-cell-2--wrapper {
  width: 100% !important;
  height: 100% !important;
  position: relative;
}

.btn_link functional-page-navigator {
  width: 100% !important;
  height: 100% !important;
  position: absolute;
  top: 0px !important;
  left: 0px !important;
}

.btn_link .plugin-private-wx104a1a20c3f81ec2-components-cell-cell-2--bubble {
  width: 100% !important;
  height: 100% !important;
  background: none !important;
}
