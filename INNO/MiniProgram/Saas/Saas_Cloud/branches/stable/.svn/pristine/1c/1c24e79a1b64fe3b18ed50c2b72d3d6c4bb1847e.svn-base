@import "../module/gift_goods.wxss";
@import "/static/iconFont/font.wxss";
.receive_status{
  padding: 40rpx 0px 55rpx 30rpx;
  background-color:#F04F05;
}
.status_txt{
  font-family: PingFangSC-Semibold;
  font-size: 34rpx;
  color: #FFFFFF;
  letter-spacing: 0;
  margin-bottom:14rpx;
  font-weight: bold;
}
.status_tip{
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #FFFFFF;
  letter-spacing: 0;
}
.gift_page{}
.msg_row{
  padding:35rpx 15rpx 35rpx 70rpx;
}
.row_t{
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #222222;
  letter-spacing: 0;
  padding-bottom:20rpx;
}
.row_c{
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #222222;
  letter-spacing: 0;
  vertical-align: top;
}
.r_c_r{
  margin-bottom:15rpx;
  
}
.arrow_r{
  font-size:50rpx;
  vertical-align: top;
  margin-top: -10rpx;
  color: #b2b2b2;
}
.gift_detail{
  background-color:#fff;
  position:relative;
}
.gift_goods {
  padding:25rpx;
}
.operate_box{
  position:fixed;
  left:0px;
  bottom:0px;
  width:100%;
  padding:20rpx;
  background-color:#fff;
  box-sizing: border-box;
}
.operate_btn{
  position:relative;
  border-radius: 6rpx;
  width:100%;
  height:88rpx;
  display:flex;
  justify-content: center;
  align-items: center;
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  letter-spacing: 0;
  background: #F04C05;
  color: #fff;
}