@import "/static/iconFont/font.wxss";
.main{
  position: fixed;
  bottom: 0;
  left: 0;
  min-height: 900rpx;
  width: 100%;
  background: #fff;
  padding: 30rpx 0;
  padding-bottom: 110rpx;
  box-sizing: border-box;
}
.ipX{
  padding-bottom: 68rpx;
}
.main.ipX{
  padding-bottom: 178rpx;
}
.main .title{
  padding-bottom: 35rpx;
}
.close{
  position: absolute;
  right: 0px;
  top: 0px;
  padding: 16rpx;
  z-index:2;
}
.icon-fill-del{
  font-size: 50rpx;
  color:#E4E4E4;
}
.line{
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
  background:#b2b2b2;
}
.row{
  width: 30rpx;
  height: 4rpx;
}
.col{
  width: 4rpx;
  height: 30rpx;
}
.padding_box{
  padding: 0 30rpx;
  box-sizing: border-box;
  position: relative;
}
.main .use_box{
  background: #FAFAFA;
  border-radius: 10rpx;
  border-radius: 10rpx;
  height: 100rpx;
  padding: 0 22rpx;
  width: 690rpx;
  margin: 0 auto;
}
.main .tips_box{
  padding: 0;
}
.tips_title_box{
  padding-top:30rpx;
}
.tips_item{
  padding: 30rpx 0;
  padding-bottom: 0;
}
/* .tips_item:first-child{
  padding-bottom: 0;
} */
.t_i_msg_box{
  line-height: 36rpx;
}

.tips_msg_box{
  position: relative;
  max-height: 500rpx;
  overflow: scroll;
}
.use_select{
  padding-right: 44rpx;
}
.select_box{ 
  position: absolute;
  right: 22rpx;
  top: 50%;
  transform: translateY(-50%);
  width: 32rpx;
  height: 32rpx;
  border-radius: 50%;
  background: #bdbdbd;
}
.correct{
  transform: rotate(-45deg);
  width: 10rpx;
  height: 6rpx;  
  border-left:1px solid #fff;
  border-bottom: 1px solid #fff;
  background: transparent;
}
.tip_label_box{
  padding: 15rpx;
}
.tip_label{
  width: 22rpx;
  height: 22rpx;
  border-radius: 50%;
  background-color: #fff;
  border:1px solid #bdbdbd;
}
.tip_label::after{
  content: "i";
  font-size: 16rpx;
  line-height: 16rpx;
  color: #bdbdbd;
}
.t_t_item{
  line-height: 24rpx;
}
.select_img{
  width: 32rpx;
  height: 32rpx;
  position: absolute;
  right: 22rpx;
  top: 50%;
  transform: translateY(-50%); 
}
.tip{
  padding: 15rpx;
  width: 32rpx;
  height: 32rpx;
}
.rule-list{
  max-height: calc(100vh - 300rpx);
}
.rule-condition{
  padding:20rpx 30rpx;
  justify-content: flex-start;
  box-shadow: 0px 4rpx 6rpx #F9F9F9;
}
.rule-tip{
  display:flex;
  height:26rpx;
  line-height:26rpx;
  font-size:20rpx;
  padding:0px 10rpx;
  position:relative;
  overflow: hidden;
  border-radius: 6rpx;
}
.r-t-bg{
  width:100%;
  height:100%;
  opacity: 0.2;
  position:absolute;
  top:0px;
  left:0px;
}
.r-t-cont{
  position: relative;
  z-index:2;
}
.rule-item-goods{
  padding:0px 30rpx;
}
.goods-item{
  padding: 20rpx 0px;
  border-bottom:1px solid #efefef;
  align-items: stretch;
}
.rule-goods{
  width:150rpx;
  height:150rpx;
  overflow: hidden;
  border-radius: 10rpx;
  position:relative;
  background-color:#EEEEEE;
}
.r-g-img{
  width:100%;
  height:100%;
}
.r-g-tip{
  background-color:rgba(156,156,156, 0.6);
  color:#fff;
  text-align:center;
  font-size:24rpx;
  display:flex;
  justify-content: center;
  align-items: center;
  height:40rpx;
  position:absolute;
  left:0px;
  bottom:0px;
  width:100%;
}
.r-g-name{
  line-height: 40rpx;
}
.rule-goods-detail{
  flex:1;
  display:flex;
  flex-direction: column;
  justify-content: space-between;
}
.icon-fill-add{
  font-size:50rpx;
}
.add-goods-btn{
  padding:10rpx 20rpx 10rpx 30rpx;
  position:absolute;
  left:0px;
  bottom:0px;
  width:100%;
  box-sizing: border-box;
}
.add-goods-btn-stay{
  display: flex;
  width:100%;
  justify-content: space-between;
  align-items: center;
}
.add-btn-l{
}
.add-btn-r{
}
.btn{
  width: 180rpx;
  color: #fff;
  border-radius: 6rpx;
  line-height: 80rpx;
  font-weight: bold;
}