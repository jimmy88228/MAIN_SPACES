.window-ext{
  z-index: 120!important;
}
.box{
  width: 560rpx;
  position: fixed;
  left: 50%;
  top: 44%;
  transform: translate(-50%,-50%);
  border-radius: 20rpx;
  color: #333;
  background: #fff;
  overflow: hidden;
  padding-top: 40rpx;
}
.content{
  padding: 0 45rpx 0 45rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  padding-bottom: 26rpx;
  /* justify-content: center; */
}
.tips_title{
  font-size: 30rpx;
  font-weight: bold;
  text-align: center;
  padding-bottom: 26rpx;
  line-height: 30rpx;
 }
.tips_box{
  padding: 14rpx 0;
  display: flex;
  align-items: center;
}
.img_goods{
  display: inline-block;
  vertical-align: middle;
  width: 80rpx;
  height: 80rpx;
  margin-right: 10rpx;
  flex-shrink: 0;
}
.goods_title{
  display: inline-block;
  vertical-align: middle;
  line-height: 30rpx;
  word-break: break-all;
}
.btn{
  width: 100%;
  height: 98rpx;
  background: #fff;
  border-top: 1px solid #EFEFEF;
  font-size: 28rpx;
}
.fail_box{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding-bottom: 36rpx;
}
.fail_title{
  font-size: 32rpx;
  font-weight: bold;
  line-height: 64rpx;
  /* line-height: 36rpx; */
  padding: 6rpx;
  text-align: center;
}