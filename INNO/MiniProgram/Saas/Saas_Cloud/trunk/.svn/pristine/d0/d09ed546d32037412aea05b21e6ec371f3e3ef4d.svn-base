/* pages/micro_mall/send_goods/send_goods_code.wxss */
page{
  background:#fff;
}
.send_goods_page{
  text-align:center;
}
.send_goods_tip{
  padding-top:50rpx;
}
.send_code_view{
  width:100%;
}
.send_code{
  width:650rpx;
  height:650rpx;
  margin:0 auto;
}
.send_code_num{
  padding-bottom:30rpx;
  margin-top:-10rpx;
  font-size:36rpx;
}
.refresh_code_btn{
  border:1px solid #71ABDC;
  border-radius:6rpx;
  color:#71ABDC;
  display:flex;
  justify-content: center;
  align-items: center;
  width:200rpx;
  height:60rpx;
  margin:0 auto;
}
.turn_home, .turn_store{
  background:none;
  text-decoration: underline;
  font-size:24rpx;
  padding:10rpx 20rpx;
  margin-top:100rpx;
}
.turn_wrapper{
    display: flex;
    align-items: center;
    justify-content: center;
}