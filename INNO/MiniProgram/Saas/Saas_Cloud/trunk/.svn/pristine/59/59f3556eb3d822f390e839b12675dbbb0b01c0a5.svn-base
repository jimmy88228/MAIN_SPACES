/* pages/micro_mall/selfGet/self_get_set.wxss */
.self_get_page{
  padding:20rpx;
}
.page_main_btn{
  margin:70rpx;
  text-align:center;
}
.page_main_btn .l_default_btn{
  width:260rpx;
  height:80rpx;
  color:#fff;
  border-radius:6rpx;
  margin:0 auto;
}
.input_item{
  position:relative;
}
.quick_get{
  position:absolute;
  top:50%;
  right:0px;
  background:#000;
  color:#fff;
  border-radius:200rpx;
  display:flex;
  justify-content: center;
  align-items: center;
  min-width:144rpx;
  height:54rpx;
  transform: translateY(-50%);
  z-index:10;
}
.select_store{
  background:#000;
  color:#fff;
  border-radius:200rpx;
  display:flex;
  justify-content: center;
  align-items: center;
  min-width:144rpx;
  height:54rpx;
}
.store_input{
  position:relative;
}
.store_input .select_store{
  position:absolute;
  top:50%;
  right:0px;
  transform: translateY(-50%);
  -moz-transform: translateY(-50%);
}