.activite_card{
  padding:30rpx;
}
.activite_tit{
  padding-bottom:20rpx;
  padding-left:20rpx;
}
.activite_cont{
  padding:20rpx;
  padding-left:30rpx;
  border:1px solid #efefef;
  background-color:#fff;
}
.cont_row{
  display:flex;
  padding:20rpx 0px;
}
.row_l{
  width:150rpx;
}
.row_r{
  display:inline-flex;
}
.activite_btns{
  padding-top:40rpx;
}
.activite_btn{
  margin:0 auto;
  display:flex;
  justify-content: center;
  align-items: center;
  background-color:#428F26;
  color:#fff;
  width:500rpx;
  height:90rpx;
  border-radius:10rpx;
}
.no_member_info{
  width:100%;
  height:200rpx;
  line-height:200rpx;
  text-align:center;
  color:#b2b2b2;
}