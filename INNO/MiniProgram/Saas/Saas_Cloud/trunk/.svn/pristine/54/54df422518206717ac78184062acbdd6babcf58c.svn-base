<!--pages/micro_mall/invoice/invoice_list/list.wxml-->
<view class="main">
  <view class="header">
    <view class="h_tab" wx:for="{{list}}" wx:key="index" catchtap="onTap" data-type="tab" data-index="{{index}}">
      <image src="{{index==0?title_invoice:''}}" mode="aspectFit" class="img_head"></image>
      <!-- <image src="{{index==0?scan_invoice:title_invoice}}" mode="aspectFit" class="img_head"></image> -->
      <view class="h_text">{{item.text}}</view>
    </view> 
  </view>
  <view class="l_box">
    <l_box id="l_box" class=""></l_box>
  </view>
</view>