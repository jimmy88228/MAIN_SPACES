.text_crossed {
  text-decoration: line-through;
  color: #777;
  font-size: 24rpx;
  margin: 0px 5rpx;
}

/*活动样式*/.activity_module {
  padding: 10rpx 0;
  position: relative;
  background: #fff;
}

.act_module_bg {
  position: absolute;
  top: 0px;
  left: 0px;
  z-index: 0;
  width: 100%;
  height: 100%;
  box-sizing: border-box;
}

.act_m_img_bg {
  width: 100%;
  height: 100%;
  display: block;
}

.activity_module_stay {
  box-shadow: 0px 2px 10px 0px rgba(0, 0, 0, 0.1);
  background: #fff;
  border-radius: 10rpx;
  overflow: hidden;
}

.act_title {
  padding: 0px 20rpx;
  position: relative;
}

.act_title .hold_cell {
  margin-left: 10rpx;
  display: inline-flex;
  align-items: center;
}

.act_count_down {
  font-weight: normal;
  border-radius: 15rpx;
  overflow: hidden;
}

.time_down_txt {
  font-size: 18rpx;
  line-height: 18rpx;
}

.day_down {
  margin-right: 4rpx;
}

.time_down {
  font-size: 18rpx;
  line-height: 18rpx;
}

.act_img_view {
  position: relative;
  width: 100%;
  padding-top: 100%;
}

.act_img_view_stay {
  position: absolute;
  top: 0px;
  left: 0px;
  width: 100%;
  padding-top: 100%;
  height: 0px;
  background-color: #fff;
}

.act_img_view_stay image {
  display: block;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  background-color:#fff;
}

.img_view_border {
  position: absolute;
  top: 0px;
  left: 0px;
  border: 2rpx solid #000;
  opacity: 0.18;
  width: 100%;
  height: 100%;
  z-index: 2;
}

.activity_name {
  font-size: 30rpx;
}

.act_tip {
  position: absolute;
  top: 7rpx;
  left: 6rpx;
  width: 62rpx;
  height: 26rpx;
  line-height: normal;
  color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 2;
  font-size: 18rpx;
}

.act_img_view image {
  width: 100%;
  height: 100%;
  display: block;
  backgrond-color:#fff;
}


.act_btn {
  color: #fff;
  width: 160rpx;
  height: 60rpx;
  line-height: normal;
  border-radius: 4rpx;
  margin-left: auto;
}

.json_act_count {
  display: flex;
  width: 62rpx;
  height: 26rpx;
  font-size: 18rpx;
  border: 1rpx solid #eb213a;
  border-radius: 100rpx;
  justify-content: center;
  align-items: center;
}
.act_item .price_tip {
  font-size: 18rpx;
}

.act_item .price_num {
  font-size: 22rpx;
  white-space: nowrap;
}

.act_item .original_p {
  font-size: 18rpx;
}

.act_item {
  box-sizing: border-box;
  width: 100%;
  flex-shrink: 0;
  border:1px solid rgba(239, 239, 239, 1);
  margin-bottom: 20rpx;
  overflow: hidden;
  border-radius: 6rpx;
  background: #fff;
}

.act_item .act_goods_name {
  font-size: 22rpx;
  word-break: break-all;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  line-height: 30rpx;
  position: relative;
  z-index: 2;
  margin-top: 10rpx;
  margin-bottom: 6rpx;
}
.act_goods_info{
  width: 100%;
  padding:15rpx;
  padding-bottom: 20rpx;
  box-sizing: border-box;
  text-align: center;
}

.ac_price{
  font-size: 26rpx;
  line-height: 26rpx;
  display: inline-block;
  vertical-align: baseline;
}
.ac_market_price{
  display: inline-block;
  vertical-align: baseline;
  font-size: 24rpx;
  line-height: 24rpx;
  color: rgba(178, 178, 178, 1);
  text-decoration: line-through;
  margin-left: 5rpx;
}

/* module_style: 6  2  5  7  8 */  /* 一 二 三 多 单 */
              
.act_list{
  padding: 0rpx 20rpx;
  display: flex;
  flex-wrap: wrap;
  box-sizing: border-box;
}

.m_s_list2 .act_item{
  width: 344rpx;
  margin-right: 12rpx;
}

.m_s_list2 .act_item:nth-child(2n){
  margin-right: 0;
}

.m_s_list2 .act_goods_name{
  -webkit-line-clamp: 2;
}

.m_s_list5 .act_item{
  width: 226rpx;
  margin-right: 12rpx;
}

.m_s_list5 .act_item:nth-child(3n){
  margin-right: 0;
}

.m_s_list6 .act_item{
  width: 100%;
  display: flex;
  padding:15rpx;
  border-radius: 10rpx;
}
.m_s_list6 .act_img_view{
  width: 30%;
  padding-top:30%;
  flex-shrink: 0;
}
.m_s_list6 .act_goods_info {
  flex-grow: 1;
  flex-shrink: 1;
  text-align: left;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  padding: 6rpx 20rpx;
}
.m_s_list6 .act_goods_name {
  -webkit-line-clamp: 2;
  margin: 0;
  font-size: 24rpx;
} 

.act_p_box{
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
}
.act_p_l_price{
  font-size: 32rpx;
  line-height: 32rpx;
  margin-left: 4rpx;
}
.act_p_l_price::before{
  content:"￥";
  font-size: 80%;
}
.act_p_m_l{
  color: #B2B2B2;
  text-decoration: line-through;
  padding-top: 8rpx;
}
.act_p_box_r{

}
.act_p_r{
  width: 172rpx;
  height: 52rpx;
  border-radius: 6rpx;
  color: #fff;
  font-size: 24rpx;
}
 
.progress_small {
  width: 82rpx;
  display: inline-block;
  vertical-align: middle; 
}

.progress_text {
  line-height: 18rpx;
  vertical-align: middle;
}

.progress_box{
  min-width: 172rpx;
  display: inline-flex;
  justify-content: space-between;
  align-items: center;
  padding-top: 10rpx;
}

.activityTitleName {
  width: 100%;
  text-align: center;
  word-break: break-all;
  box-sizing: border-box;
  padding: 0 30rpx;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  word-wrap: break-word;
  font-size: 22rpx;
  line-height: 26rpx;
}

._siwperStyle {
  width: 100%;
  min-height: 200rpx;
  background: #fff;
}

.act_item:last-child::after {
  content: "";
  display: none;
}

.time_box {
  display: flex;
  width: 718rpx;
  position: relative;
  box-sizing: border-box;
  margin: 0 auto;
  background-color: rgba(239,239,239,0.4);
  height: 88rpx;
  border-radius: 6rpx;
}
.time_msg_box{
  display: flex;
  height: 100%;
  flex-wrap: nowrap;
  padding: 6rpx;
  box-sizing: border-box;
}
.time_item {
  min-width: 25%;
  flex: 1;
  flex-shrink: 0;
  height:100%;
  display: inline-flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  border-radius: 6rpx;
  font-size: 22rpx;
}

.time_item:last-child {
  margin-right: 0;
}
 


.btn_link {
    position: absolute;
    top: 0px;
    left: 0px;
    right: 0px;
    bottom: 0px;
    z-index: 10;
    opacity: 0;
} 

