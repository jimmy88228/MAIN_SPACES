 }

.img_search {
    width: 29rpx;
    height: 29rpx;
    margin: 0 15rpx 0 25rpx;
}

.input {
    width: 610rpx;
    height: 56rpx;
    display: inline-block;
    font-size: 24rpx; 
    box-sizing: border-box;
    color: #333333;
}
.placeholder_style{
    color:rgba(51, 51, 51, 0.5);
    font-size: 24rpx;
}
.scroll {
    height: calc(100% - 170rpx);
    width: 100%;
}

.data_list_item {
    width: 100%;
    height: 102rpx;
    padding: 0 20rpx 0 20rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
    border-bottom: 1px solid rgba(239, 239, 239, 1);
    box-sizing: border-box;
}

.img_select {
    width: 38rpx;
    height: 38rpx;
}

.fix_bottom {
    width: 100%;
    height: 100rpx;
    background: rgba(255, 255, 255, 1);
    box-shadow: 0px 0px 10px 0px rgba(205, 113, 143, 0.1);
    box-sizing: border-box;
    position: fixed;
    bottom: 0;
    left: 0;
}

.bottom_text {
    /* display: inline-block; */
}

.button_confirm {
    width: 160rpx;
    height: 76rpx;
    border-radius: 4rpx;
    color: #fff;
    display: inline-flex;
}

.fix_bottom_box {
    position: relative;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    padding: 12rpx;
    padding-left: 25rpx;
    box-sizing: border-box;
    display: inline-flex;
    align-items: center;
    justify-content: space-between;
}
