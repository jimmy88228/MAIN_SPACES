.main{
  width: 100%;
  /* height: 206rpx; */
  height: 178rpx;
  padding-bottom: 20rpx;
  /* padding:20rpx 0; */
}
.scroll-box{
  width: 100%;
  box-sizing: border-box;
  height: 100%;
}
.f_box{
  display: flex;
  flex-wrap: nowrap;
  height: 100%;
  box-sizing: border-box;
  padding-left: 30rpx;
}
.f_item{
  width: 156rpx;
  height: 156rpx;
  margin-right: 13rpx;
  position: relative;
  flex-shrink: 0;
}
.img_goods{
  width: 100%;
  height: 100%;
}
.price_box{
  width: 74%;
  padding: 2rpx 10rpx;
  position: absolute;
  bottom: 0;
  left: 50%;
  transform: translate(-50%,50%);
  border:1px solid transparent;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 30rpx;
  background-color: #fff;
}
.unit{
  font-size: 22rpx;
  font-weight: bold;
  /* line-height: 20rpx; */
}
.unit::before{
  content: "¥";
  font-size: 100%;
  margin-right: 2rpx;
}
.market_price{
  font-size:20rpx;
  color:#ccc;
  text-decoration: line-through;
}
.market_price::before{
  content: "¥";
  font-size: 100%;
  margin:0px 2rpx;
}