/* pages/goods/popup/service.wxss */
.window-ext{
  z-index:999;
}
.alert_popup{
  position:fixed;
  left:50%;
  top:50%;
  transform: translate(-50%,-50%);
  z-index:999;
  background:#fff;
  border-radius:10rpx;
  overflow:hidden;
  width:600rpx;
}
.alert_title{
  position:relative;
  font-size:28rpx;
  padding:30rpx 0px;
  font-weight:bold;
  text-align:center;
  color:#fff;
}
.alert_cont{
  padding:20rpx 50rpx;
  text-align:left;
  color:#7f7f7f;
}
.alert_cont .cont_tip{
  padding:10rpx 0px;
}
.alert_btns{
  border-top: 1px solid #efefef;
  text-align:center;
}
.alert_btn{
  width:100%;
  height:90rpx;
  font-size:26rpx;
}