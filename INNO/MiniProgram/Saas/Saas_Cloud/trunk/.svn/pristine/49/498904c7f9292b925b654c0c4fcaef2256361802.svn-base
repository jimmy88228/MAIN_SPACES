page{
  height: 100%;
  background: #fff;
  color: #000;
}
.main{
  height: 100%;
}
.content_box{
  padding: 20rpx;
  box-sizing: border-box;  
  height: 350rpx;
}
.msg_box{
  padding: 20rpx;
  box-sizing: border-box;
}
.select_box{
  position: relative;
  font-size: 30rpx;
  line-height: 40rpx;
}
.clickBox{
  position: absolute;
  width: 180rpx;
  height: 100%;
  z-index: 2;
}
.clickBox.c_b_fixed{
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
}
.select{
  display: inline-block;
  vertical-align: middle; 
}
.arrow_select{
  width: 0;
  height: 0;
  border-top:6rpx solid #000;
  border-left:6rpx solid transparent;
  border-right:6rpx solid transparent; 
  display: inline-block;
  vertical-align: middle; 
  margin-left: 15rpx;
}
.s_option_box{
  padding:20rpx 0;
  border-radius: 8rpx;
  box-shadow: 0 0 20rpx 4rpx rgba(0, 0, 0, 0.10);
  background: #fff;
  position: absolute;
  bottom: -234rpx;
  left: 0;
  z-index: 3;
  opacity: 0;
  transition: opacity 0.32s;
}
.o_item{
  padding: 20rpx 40rpx;
}
.o_item:first-child{
  margin-bottom: 20rpx;
}
.time_box{
  display: flex;
  align-items: center;
  padding:25rpx 0;
  box-sizing: border-box;
  padding-left: 2rpx;
}
.t_title{
  font-size: 24rpx;
  color: #B2B2B2;
  padding-right: 30rpx;
}
.t_item{
  width:80rpx;
  height:44rpx;
  border-radius:22px;
  border:1px solid rgba(221,221,221,1);
  display: flex;
  justify-content: center;
  align-items: center;
  margin-right: 16rpx;
  font-size: 24rpx;
  line-height: 24rpx;
  box-sizing: border-box;
}
.u_info_box{
  height: 132rpx;
  border-radius: 10rpx;
  box-shadow: 0 0 10rpx 2rpx rgba(0, 0, 0, 0.15);
  display: flex;
  position: relative;
}
.line_r{
  position: absolute;
  width: 1px;
  height: 50rpx;
  top: 50%;
  transform: translateY(-50%);
  background: #DDDDDD;
  right: 0;
}
.user{
  min-width: 220rpx;
  max-width: 250rpx;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
  box-sizing: border-box;
  padding-left: 30rpx;
  padding-right: 30rpx;
}
.img_user{
  width: 60rpx;
  height: 60rpx; 
  border-radius: 50%;
  margin-right: 20rpx;
  flex-shrink: 0;
}
.u_name{
  line-height: 26rpx;
  word-break: break-all;
}
.u_rank_i{
  flex-grow: 1;
  flex-shrink: 0;
  display: flex;
  height: 100%;
  box-sizing: border-box;
  padding-left: 30rpx;
}
.u_r_box{
  display: flex;
  height: 100%;
  flex-direction: column;
  justify-content: center;
  padding-left: 40rpx;
}
/* .u_r_box:last-child{
} */
.u_r_title{
  font-size: 22rpx;
  line-height: 22rpx;
  color: #B2B2B2;
  padding-bottom: 16rpx;
}
.u_r_empty{
  font-size: 30rpx;
  font-weight: bold;
}
.rank_now{
  font-size: 32rpx;
  line-height: 32rpx;
  font-weight: bold;
}
.rank_change{
  /* margin-left: 30rpx; */
  position: relative;
}
.r_c_detail{
  font-size: 18rpx;
  line-height: 18rpx;
  color: #FF3400;
}
