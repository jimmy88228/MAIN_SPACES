.u_r_detail{
  display: flex;
  align-items: flex-end;
}
.r_arrow_box{
  display: flex;
  flex-direction: column;
  align-items: center;
  padding-bottom: 6rpx;
  margin:0 2rpx 0 6rpx;
}
.r_arrow{
  width: 4rpx;
  height: 12rpx;
}
.up_color{
  color: #FF3400;
}
.down_color{
  color: #00C03D;
}
.up_bg_color{
  background: #FF3400;
}
.down_bg_color{
  background: #00C03D;
}
.r_arrow_down{
  content: "";
  width: 0;
  height: 0;
  border-left:6rpx solid transparent;
  border-right:6rpx solid transparent; 
  border-top:8rpx solid #00C03D;   
}
.r_arrow_up{
  content: "";
  width: 0;
  height: 0;
  border-left:6rpx solid transparent;
  border-right:6rpx solid transparent; 
  border-bottom:8rpx solid #FF3400;   
}
.r_no_change{
  font-size: 18rpx;
  line-height: 18rpx;
  color: #B2B2B2;
  margin-left: 4rpx;
}
.list{
   height: calc(100% - 350rpx);
}