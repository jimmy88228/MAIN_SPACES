page{
    height: 100%;
    background: #fff;
}
.detail_cont{
  width:100%;
  height:100%;
  overflow: hidden;
  padding-top:110rpx;
  box-sizing: border-box;
}
.filter_area{
  padding:20rpx;
  text-align:right;
  position:absolute;
  top:0px;
  right:0px;
  width:100%;
  box-sizing: border-box;
  background-color:#fff;
}
.filter_curr{
  text-align:center;
  border-radius:10rpx;
  color:#fff;
  padding:0px 20rpx;
  display:inline-block;
  line-height:70rpx;
  height:70rpx;
}
.main{
    height: 100%;
}
.box {
    width: 750rpx;
    box-sizing: border-box;
    padding: 44rpx 30rpx 24rpx 30rpx;
    border-bottom: 1px solid rgba(0, 0, 0, 0.1);
}

.box:first-child {
    border-top: 1px solid rgba(0, 0, 0, 0.1);
}

.content {
    display: flex;
    width: 100%;
    justify-content: space-between;
    margin-bottom: 20rpx;
}

.content.flex {
    height: 30rpx;
    align-items: center;
}

.time {
    font-size: 22rpx;
    color: rgba(127, 127, 127, 1);
    line-height: 22rpx;
    /* display: inline-block;
    vertical-align: middle; */
}

.price {
    /* display: inline-block;
    vertical-align: middle; */
    font-size: 30rpx;
    line-height: 30rpx;
    font-weight: bold;
}

.content_bottom {
    line-height: 26rpx;
    font-size: 26rpx;
}
.none {
    height: 100%; 
    color: #7f7f7f;
    display: flex;
    justify-content: center;
    padding-top: 225px;
    box-sizing: border-box;
}