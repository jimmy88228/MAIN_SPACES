page{
  background-color:#F3F3F3;
  font-family:PingFangTC-Regular,PingFangTC;
}
.cover_img_area{
  width:750rpx;
  min-height:600rpx;
}
.cover_img{
  width:100%;
  display:block;
  background-color:#efefef;
}
.live_act{
  padding:24rpx;
  padding-top:28rpx;
  margin-bottom:50rpx;
}
.act_cont{
  background:rgba(255,255,255,1);
  box-shadow:0px 5rpx 20rpx 0px rgba(85,85,85,0.06);
  border-radius:20rpx;
  position:relative;
  margin-bottom:30rpx;
  text-align:center;
  padding:69rpx 42rpx 40rpx 42rpx;
}
.act_rule{
  position:absolute;
  top:0px;
  right:0px;
  padding:20rpx;
  font-size:24rpx;
  font-weight:400;
  color:rgba(155,155,155,1);
  line-height:25rpx;
}
.act_name{
  font-size:36rpx;
  font-weight:500;
  color:rgba(74,74,74,1);
  line-height:50rpx;
}
.act_time{
  font-size:22rpx;
  font-weight:400;
  color:rgba(155,155,155,1);
  line-height:28rpx;
  margin-top:20rpx;
}
.act_state_view{
  padding-top:45rpx;
  padding-bottom:26rpx;
}
.a_progress{
  width:100%;
  height:8rpx;
  background:rgba(239,239,239,1);
  border-radius:100rpx;
}
.a_p_active{
  width:0%;
  height:100%;
  border-radius:100rpx;
  background:rgba(254,34,10,1);
  position: relative;
  transition: width .35s;
  transition-delay: 1s;
}
.active_point{
  position:absolute;
  top:50%;
  right:0px;
  transform: translate(50%,-50%);
  width:28rpx;
  height:28rpx;
  background:rgba(254,34,10,1);
  border-radius: 100%;
  overflow: hidden;
}
.join_view{
  display:flex;
  justify-content: space-between;
  align-items: center;
  color:rgba(155,155,155,1);
  font-size:24rpx;
}
.join_user_area{
  display:flex;
  justify-content: flex-start;
  align-items: center;
}
.join_user_list{
  padding-left:30rpx;
  display:flex;
  justify-content: flex-start;
  align-items: center;
}
.join_user_item{
  width:52rpx;
  height:52rpx;
  background:rgba(216,216,216,1);
  border:8rpx solid rgba(255,255,255,1);
  margin-left:-20rpx;
  border-radius: 100%;
  overflow: hidden;
  display:inline-block;
  vertical-align: middle;
}
.user_icon{
  width:100%;
  height:100%;
  display:block;
}
.join_view_l,.join_view_r{
  white-space:nowrap;
}
.join_more{
  display:inline-block;
  white-space:nowrap;
  line-height:normal;
  font-size:0;
}
._more{
  width:6rpx;
  height:6rpx;
  display:inline-block;
  border-radius:100%;
  background-color:#b2b2b2;
  margin:0 4rpx;
}
.invite_area{
  display:block;
  padding-top:30rpx;
  /* position:absolute;
  bottom:-23rpx;
  left:50%;
  transform: translateX(-50%); */
}
.invite_f{
  width:420rpx;
  height:88rpx;
  margin:0 auto;
  background:linear-gradient(270deg,rgba(254,31,14,1) 0%,rgba(250,96,34,1) 100%);
  border-radius:200rpx;
  font-size:28rpx;
  font-weight:500;
  color:rgba(255,255,255,1);
}
.no_start{
  opacity: 0.3;
}
.is_end{
  background:rgba(240,240,240,1);
  color:#B2B2B2;
}
.act_bonus_list{
  background:rgba(255,255,255,1);
  box-shadow:0px 5rpx 20rpx 0px rgba(85,85,85,0.06);
  border-radius:20rpx;
  padding:42rpx;
  padding-right:30rpx;
  display:flex;
  align-items: center;
}
.act_bonus_t{
  width:25rpx;
  font-size:24rpx;
  font-weight:400;
  color:rgba(155,155,155,1);
  line-height:28rpx;
}
.act_bonus_l{
  display:inline-flex;
  width:100%;
  padding-left:40rpx;
  padding-right:23rpx;
  box-sizing: border-box;
  white-space:nowrap;
  margin-top:-23rpx;
}
.act_bonus_scroll{
  width:100%;
}
.bonus_items{
  padding-top:23rpx;
}
.bonus_item{
  display:inline-block;
  vertical-align: top;
  margin-right:20rpx;
}
.bonus_item_c{
  min-width:182rpx;
  height:125rpx;
  background:linear-gradient(360deg,rgba(252,250,250,1) 0%,rgba(255,233,222,1) 100%);
  border-radius:26rpx;
  /* border:1px solid rgba(186,89,0,0.05); */
  display:flex;
  justify-content: center;
  align-items: center;
  text-align: center;
  position:relative;
  padding: 0px 20rpx;
}
.bonus_name{
  font-size:26rpx;
  font-weight:600;
  color:rgba(253,79,1,1);
  line-height:37rpx;
}
.bonus_tip{
  font-size:22rpx;
  font-weight:600;
  color:rgba(254,34,10,1);
  line-height:28rpx;
  margin-top:8rpx;
}
.is_select{
  width:25rpx;
  height:25rpx;
  background-color:#2FB206;
  position:absolute;
  top:0;
  right:0;
  transform: translateY(-30%);
  border:8rpx solid rgba(255,255,255,1);
  border-radius: 100%;
  overflow: hidden;
}
.is_select::after{
  content:"";
  display:block;
  position: absolute;
  top:50%;
  left:50%;
  transform: translate(-50%,-60%) rotate(45deg);
  width:30%;
  height:60%;
  border-bottom:1px solid #fff;
  border-right:1px solid #fff;
  box-sizing: border-box;
}

