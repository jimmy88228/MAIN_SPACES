page {
    background: #fff;
} 
.couponPage{
  width: 100%;
  height: 100%;
}
.couponTab {
    text-align: center;
    background: #fff;
    color: #777;
    width: 100%;
    position: fixed;
    z-index: 2;
    box-shadow: 0 -10px 10px 10px #efeded;
    overflow: visible;
}

.active {
    color: #cd718f;
}

.lines {
    position: absolute;
    bottom: -3rpx;
    left: 50%;
    transform: translateX(-50%);
    width: 52rpx;
    height: 4rpx;
    border-radius: 20rpx;
    border: 1rpx solid transparent;
}

.couponTabTit {
    position: relative;
    width: 50%;
    line-height: 62rpx;
    height: 62rpx;
    box-sizing: border-box;
}

.couponTabTit text {
    display: inline-block;
    vertical-align: middle;
    font-size: 28rpx;
}

.couponList {
}

.couponItem {
    position: relative;
    margin: 20rpx 0;
}

.couponMsg {
    position: relative;
    background-color: #fff;
    width:710rpx;
    height:245.26rpx;
    overflow: hidden;
}

.couponTxt {
    display: flex;
    align-items: center;
    justify-content: space-between;
    color: #fff;
    position: absolute;
    bottom: 0px;
    left: 50%;
    width: 100%;
    transform: translateX(-50%);
    padding: 0 20rpx 20rpx 20rpx;
    z-index:2;
}

.couponTxt .minUse {
    display: block;
    margin-top: 10rpx;
    font-size: 20rpx;
}

.couponTxt .valueUnit {
    display: inline-block;
    vertical-align: middle;
    font-size: 26rpx;
    line-height: normal;
}

.bonus_type {
    display: inline-block;
    vertical-align: middle;
    border: 1px solid #fff;
    border-radius: 10rpx;
    padding: 0px 5rpx;
    margin-left: 10rpx;
    font-size: 26rpx;
}

.couponTxt button {
    text-align: center;
    background: none;
    border: 1px solid #fff;
    color: #fff;
    font-size: 26rpx;
    width: 150rpx;
    height: 55rpx;
    margin-top: 20rpx;
    border-radius: 5rpx;
}

.couponValue {
    font-size: 90rpx;
    line-height: normal;
    margin-right: 8rpx;
}

.couponMsg image {
    display:block;
    position: absolute;
    top: 0;
    left: 0;
    /* transform: translate(-50%,-50%); */
    width:710rpx;
    height:245.26rpx;
    /* width: 100%; */
}

.couponCrumbs {
    width: 84%;
    margin: 0 auto;
    border: 1px solid #e6e6e6;
    border-bottom-left-radius: 5px;
    border-bottom-right-radius: 5px;
    padding: 0px 5%;
    border-top: 0px;
}

.useLimitTxt {
    overflow: hidden;
    max-height: 0;
    transition: max-height 0.3s ease;
}

.useLimitTxt.show {
    max-height: 1000rpx;
}

.useLimitStay {
    padding: 5% 0px;
    color: rgba(130, 130, 130, 1);
}

.failureImg {
    width: 710rpx;
    height: 260rpx;
    position: absolute;
    top: 20rpx;
    left: 20rpx;
}

.CrumbsIndicator {
    text-align: center;
    border-top: 1px solid #e6e6e6;
    padding: 11rpx 0 15rpx 0;
}

.CrumbsIndicator .arrow {
    vertical-align: middle;
    width: 50rpx;
}

.CrumbsIndicator.active .arrow {
    transform: rotate(180deg);
}

.selectedCouponMark {
    position: fixed;
    z-index: 2;
    top: 0px;
    left: 0px;
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.5);
}

.selectedCoupon {
    padding: 24rpx 0 80rpx 0;
    background: rgba(255, 255, 255, 1);
    position: fixed;
    z-index: 2;
    width: 620rpx;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    border-radius: 6rpx;
}

.selectedCoupon .couponItem {
    padding: 0px;
}

.selectedCoupon .codeImg {
    margin-top: 20rpx;
    width: 100%;
    height: 150rpx;
}

.checkCodeBtn {
    display: block;
    text-align: center;
    padding: 10rpx 0px;
}

.largetCodeMark {
    position: fixed;
    top: 0px;
    left: 0px;
    width: 100%;
    height: 100%;
    z-index: 2;
    background: rgba(0, 0, 0, 0.6);
}

.largetCode {
    padding: 20rpx;
    position: fixed;
    z-index: 2;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%) rotate(90deg);
    background: #fff;
}

.largetCodeImg{
    width: 780rpx;
    height: 204rpx;
}

/*数据为空*/

.emptyCoupon {
    margin: 100rpx 0px;
    text-align: center;
}

.emptyCoupon image {
    width: 200rpx;
    height: 200rpx;
    margin: 0 auto;
    margin-bottom: 20rpx;
}

.CrumbsIndicator_text {
    display: inline-block;
    vertical-align: middle;
    color: rgba(130, 130, 130, 1);
    font-size: 26rpx;
}

.table_cell {
    display: flex;
    align-items: center;
}

.barCanvas {
    display: block;
    margin: 50rpx auto 29rpx auto;
}
.bonus_sn{
  padding-bottom:10rpx;
  text-align:center;
  color:rgba(127, 127, 127, 1);
}

.bouns_type {
    display: flex;
    flex-direction: column;
}

.couponTxt_select {
    width: 92%;
    padding: 0 40rpx 20rpx 40rpx;
    justify-content: flex-start;
}

.coupon_header {
    width: 100%;
    font-size: 28rpx;
    color: rgba(51, 51, 51, 1);
    text-align: center;
    font-weight: bold;
}

.close {
    display: block;
    width: 20rpx;
    height: 20rpx;
}

.icon_wrapper {
    display: flex;
    align-items: center;
    justify-content: center;
    position: absolute;
    top:2rpx;
    right:0rpx;
    width:50rpx;
    height:40rpx;
    padding:20rpx 15rpx;

}

.couponItem_select {
    margin: 56rpx 0 60rpx 0;
}

.the_line {
    width: 100%;
    height: 1rpx;
    border-top: 1rpx solid rgba(239, 239, 239, 1);
}

.couponMsg_select {
    margin: 0 50rpx 60rpx 50rpx;
    overflow: hidden;
    width:520rpx;
    height:179.63rpx;
}
.couponMsg_select .bg_wrapper image{
 width:100%;
 height:100%; 
}

.to_tips {
    font-size: 24rpx;
    color: rgba(127, 127, 127, 1);
    text-align: center;
}

.bg_wrapper {
    width: 100%;
    height: 178rpx;
}

.writeOff {
    width: 510rpx;
    height: 66rpx;
    margin: 0 auto;
    border-radius: 6rpx;
    color: #fff;
    font-size: 24rpx;
    margin-top: 53rpx;
}

.black_bg {
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    position: fixed;
    z-index: 2;
    background: rgba(0, 0, 0, 0.7);
    display: none;
}

.black_bg.isShow {
    display: block;
}

.share_box {
    box-sizing: border-box;
    position: fixed;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    width: 610rpx;
    background: #fff;
    /* transition: all 0.4s; */
    display: none;
    opacity: 0;
    background: rgba(255, 255, 255, 1);
    border-radius: 10rpx;
}

.share_box.isShow {
    display: block;
    opacity: 1;
    z-index: 111;
}

.share_box_text {
    position: relative;
    width: 100%;
    text-align: center;
    padding: 100rpx 0; 
    font-size: 28rpx;
    font-weight: bold;
}
 
.confirm {
    width: 50%;
    font-size: 28rpx; 
    box-sizing: border-box;
    line-height: 60rpx;
    text-align: center;
    display: inline-block;
    font-weight: bold; 
}
.confirm.cancel{
    color: #b2b2b2;
}

.f_col{
  display: flex;
  flex-direction: column;
  align-items: center;
}

.availableCoupon,.UnavailableCoupon{
  line-height: 40rpx;
  /* margin-top: -20rpx; */
}

.coupon_name{
  position: absolute;
  top: 38rpx;
  /* left: 22rpx; */
  left: 0;
  padding-left: 22rpx;
  z-index: 2;
  color: #fff;
  width: 100%;
  box-sizing: border-box;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: break-word;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  line-height: 58rpx;
  font-size: 52rpx;
}
.couponMsg .Un_c_n{
  padding-left: 35rpx;
}