.s_icon_img{
  width:100%;
  height:100%;
  display:block;
}
.select_txt{
  display:inline-block;
  vertical-align: middle;
  font-size:32rpx;
  margin-right:5rpx;
}
.agreet_name{
  display:inline-block;
  vertical-align: middle;
  line-height:36rpx;
}
.agreet_btns{
  padding-top:30rpx;
}
.agreet_btn_stay{
  display:flex;
  align-items: center;
  justify-content: center;
  width:100%;
  height:80rpx;
  border-radius:6rpx;
  color:#fff;
  background-color:#9A9A9A;
  font-size:26rpx;
}
.agreet_btn_txt{
  display:flex;
  align-items: center;
  justify-content: center;
  width:100%;
  height:80rpx;
  border-radius:6rpx;
  color:#fff;
  background-color:#9A9A9A;
  font-size:26rpx;
}
