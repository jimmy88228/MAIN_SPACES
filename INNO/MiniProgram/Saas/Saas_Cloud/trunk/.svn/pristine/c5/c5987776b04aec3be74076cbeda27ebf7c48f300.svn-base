page{
  background-color:#fff;
}
.agreet_page{
  /* position:fixed;
  top:0px;
  left:0px; */
  color:#000;
  width:100%;
  /* height:100%; */
  background-color:#fff;
  text-align:left;
  font-size:0px;
}
.agreet_cont{
  width:100%;
  padding:50rpx;
  box-sizing:border-box;
}
.tip{
  text-align:center;
  font-size:36rpx;
  padding-bottom:30rpx;
  font-weight:bold;
}
.cont{
  line-height:48rpx;
  font-size:26rpx;
}
.operates{
  padding-top:50rpx;
  font-size:22rpx;
  color:#b2b2b2;
}
.operate_icon{
  width:20rpx;
  height:20rpx;
  margin:0px 3rpx;
}
.select_area{
  display:inline-block;
  vertical-align: bottom;
  position:relative;
}
.select_area_btn{
  position:absolute;
  top:0px;
  left:0px;
  background:none;
  width:100%;
  height:100%;
}
.select_icon{
  display:inline-block;
  width:36rpx;
  height:36rpx;
  margin:0px 8rpx;
  position:relative;
  vertical-align: middle;
}
