.receive_page{
  font-family:PingFangSC-Semibold,PingFang SC;
}
.receive_bg{
  width:100%;
  height:616rpx;
  position:relative;
  overflow: hidden;
}
.receive_bg_icon{
  display:block;
  width:100%;
  /* height:100%;
  background:linear-gradient(180deg,rgba(255,56,0,1) 0%,rgba(255,162,0,1) 100%,rgba(255,255,255,1) 100%); */
}
.receive_head{
  position:absolute;
  left:0px;
  bottom:180rpx;
  padding-left:44rpx;
  padding-bottom:10rpx;
  color:#fff;
  width: 60%;
}
.r_h_title{
  font-size:56rpx;
  font-weight:600;
  color:rgba(255,255,255,1);
  line-height:78rpx;
  margin-bottom:14rpx;
}
.r_h_tip{
  font-size:22rpx;
  font-weight:400;
  color:rgba(255,255,255,1);
  line-height:30rpx;
  opacity:0.5;
}
.swiper_hold{
  position:relative;
  margin-top:20rpx;
}
.notice_list{
  width:100%;
  height:90rpx;
}
.no_touch{
  position:absolute;
  top:0px;
  left:0px;
  width:100%;
  height:100%;
  z-index:2;
}
.notice_item{
  
}
.notice_item_stay{
  display:inline-block;
  padding:20rpx 0px;
}
.n_item{
  display:flex;
  align-items: center;
  justify-content: space-between;
  background:rgba(0,0,0,0.3);
  border-radius:100rpx;
  padding:4rpx 6rpx;
  padding-right:20rpx;
}
.n_i_icon{
  width:44rpx;
  height:44rpx;
  border-radius: 100%;
  background-color:#fff;
  margin-right:15rpx;
  flex-shrink: 0;
}
.n_i_txt{
  font-size:20rpx;
  font-weight:400;
  color:rgba(255,255,255,1);
  line-height:28rpx;
}
.receice_cont{
  margin-top:-180rpx;
}
.bonus_area{
  padding:0px 20rpx;
}
.box{
  width: 100%;
  border-radius: 20rpx;
  overflow: hidden;
  position: relative;
  margin-bottom: 30rpx;
}
.box_cont{
  width: 100%;
  height: 180rpx;
  display: flex;
  box-shadow: 0 4rpx 4rpx 1px rgba(0, 0, 0, 0.05);
  position: relative;
  z-index: 2;
}
.c_coupon_box{
  display: flex;
  position: relative;
  color: #fff;
}
.c_coupon_box{
  width: 500rpx;
  display: flex;
  align-items: center; 
  background: rgba(255, 37, 0, 1);
  position:relative;
}
.c_coupon_bg{
  position:absolute;
  top:0px;
  left:0px;
  width:100%;
  height:100%;
  display:block;
  z-index:0;
}
.c_group_label{
  position: absolute;
  left: 0;
  top:0;
  padding: 8rpx 16rpx;
  background: rgba(8, 158, 0, 1);
  border-radius: 0 0 10rpx 0;
  font-size: 20rpx;
  line-height: 20rpx;
}
.c_price_box{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding:0 30rpx;
  padding-top: 6rpx;
  flex-shrink: 0;
  position: relative;
}
.c_split{
  position: absolute;
  right: 0;
  top: 50%; 
  transform: translateY(-50%);
  width: 1px;
  height: 50rpx;
  background: rgba(225, 225, 225, 0.2);
}
.c_price{
  font-size: 26rpx;
}
.c_price_ft{
  font-size: 60rpx;
  font-weight: bold;
}
.c_price_tip{
  font-size: 22rpx;
}
.c_name{
  padding-left: 30rpx;
  box-sizing: border-box;
  position:relative;
  z-index:1;
}
.btn_box{
  width: 210rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  background: #fff;
  position:relative;
  overflow: hidden;
}
.receive_box_icon{
  width:142rpx;
  position:absolute;
  top:0px;
  right:0px;
}
.invalid_box_icon{
  width:150rpx;
  position:absolute;
  top:50%;
  left:50%;
  transform: translate(-50%,-50%);
}
.btn{
  color:rgba(255,52,0,1);
  min-width:140rpx;
  box-sizing:border-box;
  padding:7rpx 26rpx;
  border-radius: 50rpx;
  background:linear-gradient(360deg,rgba(244,233,233,1) 0%,rgba(255,255,255,1) 100%);
  box-shadow: 0 0 2rpx 1px rgba(0, 0, 0, 0.05);
  margin-bottom: 15rpx;
  margin-top:10rpx;
  font-size:22rpx;
  font-weight:400;
  line-height:30rpx;
}
.check_btn{
  position:relative;
  z-index:10;
  color: rgba(241,77,0,1);
  border:1px solid rgba(241,77,0,1);
  background:#fff;
  margin-top:15rpx;
  margin-bottom:0px;
}
.receive_btn{
  font-size:28rpx;
  font-weight:bold;
  color:rgba(241,77,0,1);
  line-height:40rpx;
}
.invalid_btn{
  font-size:28rpx;
  font-weight:bold;
  color:#b2b2b2;
  line-height:40rpx;
}
.s_count{
  font-size:20rpx;
  font-weight:400;
  color:rgba(178,178,178,1);
  line-height:28rpx;
  display:flex;
  justify-content: space-between;
  align-items: center;
}
.progress{
  width:56rpx;
  height:8rpx;
  background:rgba(239,239,239,1);
  border-radius:4rpx;
  margin-right:12rpx;
  position: relative;
}
.pro_val{
  width:0;
  max-width:100%;
  height:100%;
  background:rgba(255,39,0,1);
  border-radius:4rpx;
  position: absolute;
  top:0px;
  left:0px;
}
.end_btn{
  font-size:26rpx;
  font-weight:400;
  color:rgba(178,178,178,1);
  line-height:37rpx;
}
.send_tip{
  margin-top:15rpx;
  font-size:18rpx;
  font-weight:400;
  color:rgba(178,178,178,1);
  line-height:25rpx;
  text-align: center;
}
.end_send_tip{
  font-size:22rpx;
}
.tips{
  box-sizing: border-box;
  padding:15rpx 40rpx 15rpx 30rpx;
  position: relative;
  background: #fff;
  overflow:hidden;
  /* box-shadow:0px 2rpx 15rpx 0 rgba(0,0,0,0.07);  */
}
.t_title_box{
  display: flex;
  justify-content: space-between;
  align-items: center;
  position: relative; 
  font-size:20rpx;
  font-weight:400;
  color:rgba(178,178,178,1);
  line-height:28rpx;
}
.t_tip{
}
.arrow{
  width: 0;
  height: 0;
  border-top: 8rpx solid #b2b2b2;
  border-left: 8rpx solid transparent;
  border-right: 8rpx solid transparent;
  position: absolute;
  right: -20rpx;
  top: calc(50% - 2rpx);
  z-index: 1;
}
.arrow_hide{
  width: 0;
  height: 0;
  border-top: 8rpx solid #fff;
  border-left: 8rpx solid transparent;
  border-right: 8rpx solid transparent;
  position: absolute;
  right: -20rpx;
  top: calc(50% - 6rpx);
  z-index: 2;
}
.t_header{ 
}
.tips_detail_box{
  max-height: 0;
  transition: max-height 0.35s;
  background:#fff;
  overflow: hidden;
  font-size:20rpx; 
  font-weight: 400;
  color: rgba(178,178,178,1);
} 
.tips_detail_box.active{
  max-height: 1000rpx;
}

.useLimitStay{
  box-sizing: border-box;
  padding: 20rpx 20rpx 20rpx 30rpx;
  padding-bottom: 0;
}
.limit,.tips_detail{
  line-height: 40rpx; 
}