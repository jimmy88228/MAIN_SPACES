.window-ext{
  /* z-index: 500; */
}
.main{
  position: absolute;
  left: 50%;
  /* z-index: 100; */
  background-color: #f5f5f5;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
  bottom: 0;
  width: 100%;
  transform: translateX(-50%) translateY(50rpx);
  padding: 36rpx;
  box-sizing: border-box;
  text-align: left;
  min-height: 60vh;
  max-height: 60vh;
  height: 60vh;
  padding-bottom: calc(90rpx + 20rpx);
  /* z-index: 180; */
  color: #333333;
  font-size: 24rpx;
  line-height: initial;
}
.header{
  text-align: left;
  line-height: 40rpx;
  margin-bottom: 20rpx;
  font-size: 30rpx;
  font-weight: bold;
}
.article_content{
  box-sizing: border-box;
  height: calc(60vh - 408rpx);
  margin-bottom: 20rpx;
  font-size: 28rpx;
  color: #333333;
}
.agree_child{
  text-indent: 0;
}
::-webkit-scrollbar {
  display:none;
  width:0;
  height:0;
  color:transparent;
}
.footer>.btn_group{
  display: flex;
  align-items: center;
  height: 80rpx;
}
.footer>.agree_tips{
  height: 40rpx;
  line-height: 40rpx;
  font-size: 28rpx;
  font-weight: bold;
  margin: 40rpx 0;
  text-indent: 10rpx;
}
.footer>.btn_group>.btn_view{
  flex: 1;
  height: 100%;
  display: inline-flex;
  align-items: center;
  justify-content: center;
}
.footer>.btn_group>.btn_view.confirm{
  justify-content: flex-end;
  position: relative;
}
.footer>.btn_group>.btn_view.deny{
  justify-content: flex-start;
}
.footer>.btn_group>.btn_view.deny>button{
  background-color: #ffffff;
  color: #bbbbbb;
  border: 1px solid #cccccc;
}
.btn_view>button.normal_btn{
  height: 100%;
  border-radius: 100rpx;
  width: 80%;
  display: inline-flex;
  align-items: center;
  justify-content: center;
  font-weight: bold;
  font-size: 28rpx;
}
.login_trigger_btn{
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 99;
  opacity: 0;
}