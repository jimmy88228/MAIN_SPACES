/* components/micro-page/items/advertise/advertise.wxss */

.list {
  display: flex;
  width: 100%;
  align-items: center;

  /* 白条测试 */
  margin-top: -0.5px;
}
.list.scroll{
  /* height: 500rpx; */
}
.list.warp{
  flex-wrap: wrap;
}
.list-item {
  /* flex: 1; */
  flex-shrink: 0;
  position: relative;
}
.list-img{
  width: 100%;
  position: relative;
  display: block;
}
.map_area{
  position: absolute;
  z-index: 11;
}
.init .list-img{
  /* min-height: 250rpx; */
  /* background: #dfdfdf; */
  width: 96%;
  margin: 8rpx auto;
}
.init .initBg{
  width: 100%;
  height: 100%;
  background: #dfdfdf;
  position: absolute;
  z-index: 2;
  left: 0;
  top: 0;
}
.video{
  width: 100%;
  height: 100%;
  display: block;
  position: absolute;
  top: 50%;
  left: 0px;
  transform: translateY(-50%);
}

.btn_link{
  position: absolute;
  top: 0px;
  left: 0px;
  right: 0px;
  bottom: 0px;
  z-index: 10;
  opacity: 0;
}
.btn_link.contactPluginCell{
  z-index: 9;
}