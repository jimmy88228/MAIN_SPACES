/* pages/store_pay/store_pay_history/history_list.wxss */
page{
  background:#fff;
}
.brand_icon{
  width:100%;
  text-align:center;
  line-height:90rpx;
  height:90rpx;
}
.brand_icon image{
  width:300rpx;
  display:inline-block;
  vertical-align: middle;
}
.store_pay_list{
  width:100%;
  height: 100%;
}
.pay_time{
  line-height:70rpx;
  height:70rpx;
  padding:0px 30rpx;
  background:#EFEFEF;
  color:#696969;
}
.store_pay_items{
  padding-left:30rpx;
}
.store_pay_item{
  padding:30rpx;
  padding-left:0px;
  border-bottom:1px solid rgba(239,239,239,1);
  display:table;
  width:100%;
  box-sizing:border-box;
}
.item_left{
  display:table-cell;
  vertical-align: top;
}
.item_store{
  font-size:28rpx;
  margin-bottom:18rpx;
}
.item_time{
  font-size:24rpx;
  color:#7f7f7f;
}
.item_right{
  display:table-cell;
  vertical-align: top;
  text-align:right;
  /* font-size:28rpx; */
  font-weight:bold;
}
.l_h_p{ 
  line-height: 38rpx;
}
.l_h{
  line-height: 38rpx;
  margin-top: 8rpx;
}