.alertMsgMark{
  position:fixed;
  top:0px;
  left:0px;
  width:100%;
  height:100%;
  background:rgba(0,0,0,0.5);
}
.alertMsg{
  width:600rpx;
  position:fixed;
  top:50%;
  left:50%;
  transform:translate(-50%,-50%);
  background:#fff;
  padding:40rpx 0px;
  border-radius:3px;
  text-align:center;
}
.alertTxt{
  font-size:14px;
  padding:50rpx 0px;
}
.alertMsg button{
  font-size:14px;
  width:220rpx;
  margin:0px 40rpx;
  display:inline-block;
  line-height:90rpx;
}
.cancelBtn{
  background:#f1f1f1;
  color:#ababab;
}
.okBtn{
  color:#fff;
}
.emptyCollection{
  margin:100rpx 0px; 
  text-align:center;
}
.emptyCollection image{
  display:block;
  width:200rpx;
  margin:0 auto;
  margin-bottom:20rpx;
}

.collectionItem{
    position: relative;
    width: 358rpx;
    flex: none 1 1 358rpx;
    margin-right: 10rpx;
}
.goods_pic{
    display: block;
    width: 358rpx;
    height: 358rpx;
}
.collectionPage{
    padding: 16rpx 10rpx 0 10rpx;
}
.infos{
    margin: 10rpx 0 30rpx 0;
}
.collectionList{
    display:flex;
    flex-wrap: wrap;
    margin-right: -10rpx;
}
.delBtn{
    position: absolute;
    top: 10rpx;
    right: 10rpx;
    width:40rpx;
    height:40rpx;
}
.del_icon{
    width:40rpx;
    height:40rpx;
}