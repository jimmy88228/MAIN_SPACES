page{
    background: #fff;
}
.collectImg{
  display:table-cell;
  vertical-align: top;
  margin-right:20rpx;
  border:1px solid #efefef;
  overflow:hidden;
  width:210rpx;
}
.collect_img_stay{
  width:99%;
  height:0px;
  padding-top:99%;
  overflow:hidden;
  position:relative;
}
.collect_img_stay image{
  width:100%;
  height:100%;
  position:absolute;
  top:50%;
  left:50%;
  display:block;
  transform: translate(-50%,-50%);
  -moz-transform: translate(-50%,-50%);
}
.collectMsg{
  display:table-cell;
  vertical-align: top;
  padding-right:35rpx;
  padding-left:20rpx;
}
.goodName,.goodSn{
    text-align:center;
}
.goodName{
    margin-bottom: 10rpx;
    margin-left:15rpx;
    margin-top:10rpx;
    box-sizing:border-box;
    text-align:left;
    word-break:break-all;
    overflow:hidden;
    text-overflow:ellipsis;
    display:-webkit-box;
    -webkit-line-clamp:1;
    -webkit-box-orient:vertical;
    font-size:22rpx;
}
