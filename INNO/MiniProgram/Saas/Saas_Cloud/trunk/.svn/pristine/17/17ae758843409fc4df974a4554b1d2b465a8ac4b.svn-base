page{
  background: #fbfbfb;
}
.main{
  position: relative;
  box-sizing: border-box;
  padding-bottom: 113rpx; /* 95 + 18 */
}
.main.i_p_box{
  padding-bottom: 171rpx;
}
.time_tip{
  background: rgba(190, 190, 190, 1);
  padding: 20rpx;
  text-align: center;
  color: #fff;
  font-size: 22rpx;
  box-sizing: border-box;
}
.box{
   padding: 30rpx;
   padding-right: 0;
   width: 100%;
   box-sizing: border-box;
   background: #fff;
   font-size: 24rpx; 
   margin-top: 12rpx;
   position: relative;
} 
.box.p_t_n{
  padding-top: 0;
}
.box.p_b_n{
  padding-bottom: 0;
}
.address{
  width: 100%;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  justify-content: center;
  position: relative;
}
.noneAdr{
  padding: 10rpx 0;
}
.img_adress{
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
}
.i_a_l{
    width: 31rpx;
    height: 37rpx;
    left: 24rpx;
}
.i_a_r{
    width: 50rpx;
    height: 50rpx;
    right: 20rpx;
}
.a_msg{
  padding: 30rpx 80rpx 30rpx 90rpx;
  box-sizing: border-box; 
}
.a_a_d{
  word-break: break-all;
}
.a_n_p{
  padding-bottom: 30rpx;
}
.a_name{
  padding-right: 20rpx;
}
.title{
  color: #7f7f7f;
  width: 100%;
  padding-bottom: 30rpx;
  box-sizing: border-box;
  position: relative;
}

.goods{
  display: flex;
  flex-wrap: nowrap;
  position: relative;
  padding-top: 30rpx;
  box-sizing: border-box;
}

.img_goods_box{
  width: 200rpx;
  height: 200rpx;
  flex-shrink: 0;
}
.img_goods{
  width: 100%;
  height: 100%;
}
.msg{
  /* width: 100%; */
  width: 490rpx;
  min-height: 200rpx;
  box-sizing: border-box;
  padding-left: 30rpx;
  display: flex;
  flex-direction: column;
  position: relative;
}
.m_title{
  padding-bottom: 20rpx;
}
.sku{
  color: #7f7f7f;
  font-size: 24rpx;
  padding-bottom: 40rpx;
}
.price_box{
  display: flex;
  justify-content: space-between;
  position: absolute;
  bottom: 0;
  padding-right: 30rpx;
  width: 100%;
  box-sizing: border-box;
}
.price{
  font-size: 30rpx;
}
.p_sm{
  font-size: 22rpx;
  text-decoration: line-through;
  padding-left: 10rpx;
}
.count{
  font-size: 28rpx;
}
.pay_msg{
  display: flex;
  justify-content: space-between;
  padding: 30rpx 30rpx 30rpx 0; 
  line-height: 30rpx;
  box-sizing: border-box;
}
._C_7f{
  color: #7f7f7f;
}
._bold{
  font-weight: bold;
}
.btn_buy_box{
  /* width: 740rpx; */
  position: fixed;
  bottom: 0rpx;
  width: 100%;
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background: #fff;
  z-index: 2;
}
.btn_buy{
  position: relative;
  border-radius: 6rpx;
  width: 740rpx;
  height: 90rpx;
  color: #fff;
}
.unit::before {
    content: "¥";
    margin-right: 4rpx;
    font-size: 70%;
}
.unit_sm::before {
    content: "¥";
    font-size: 80%;
}
.self_get_adr{
  padding-right: 30rpx;
  padding-top: 20rpx;
  box-sizing: border-box;
  word-break: break-all;
}
.box .s_g_msg_title{
  padding-bottom: 20rpx;
  /* margin-bottom: 30rpx; */
}
.box .self_get_title {
  padding-bottom: 20rpx;
}

.s_g_msg{
  padding-top: 20rpx;
}
.main .coupon_box{
  display: flex;
  justify-content: space-between;
  padding-right: 30rpx;
}
.cp_title{
  line-height: 26rpx;
}
.cp_box_right{
  display: flex;
  align-items: center;
  height: 100%;
}
.cp_right{
  position: absolute;
  right: 12rpx;
  top: 50%;
  transform: translateY(-50%);
}
.cp_tip{
  line-height: 26rpx;
  color: #fff;
  padding: 12rpx 16rpx;
  border-radius: 8rpx;
}
.cp_tip_none{
  color: #7f7f7f;
}
.cp_right_icon{
  width: 50rpx;
  height: 50rpx;
}