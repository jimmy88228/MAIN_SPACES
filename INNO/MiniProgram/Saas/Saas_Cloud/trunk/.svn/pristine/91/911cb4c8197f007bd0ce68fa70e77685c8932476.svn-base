@import "./promote.wxss";
page {
  height: 100%;
}

.main {
  position:fixed;
  left:0px;
  bottom:0px;
  width: 100%;
  padding: 30rpx 0px;
  box-sizing: border-box;
  transform: translateY(110%);
  z-index:10;
  background: #fff;
}
.main_scroll{
  width:100%;
  min-height: calc(100vh - 600rpx);
  max-height: calc(100vh - 250rpx);
}
