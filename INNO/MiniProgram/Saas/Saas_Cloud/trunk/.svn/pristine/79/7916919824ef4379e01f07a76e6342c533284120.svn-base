page{
  height: 100%;
  font-family:PingFangSC-Regular,PingFang SC;
}
.main{}
.tab_box{
  width: 100%;
  height: 98rpx;
  color: #B2B2B2;
  background: #fff;
  display: flex;
}
.tab{
  flex: 1;
  height: 100%;
  text-align: center;
  display: flex;
  align-items: center;
  justify-content: center;
}
.tab_box.active{
  color: #171717;
}
.content{
  /* padding:20rpx;
  padding-bottom:100rpx; */
  width: 100%;
  box-sizing: border-box;
  height: calc(100vh - 98rpx);
}
/**/
.box{
  width: 100%;
  border-radius: 20rpx;
  /* overflow: hidden; */
  position: relative;
  margin-bottom: 40rpx;
  padding:0px 10rpx;
  box-sizing: border-box;
}
.box_cont{
  width: 100%;
  /* height: 180rpx;
  display: flex;
  box-shadow: 0 4rpx 4rpx 1px rgba(0, 0, 0, 0.05);*/
  position: relative; 
  border-top-right-radius:20rpx;
  border-bottom-right-radius:20rpx;
  overflow: hidden;
  z-index: 2;
  font-size:0;
}
.c_coupon_box{
  display: flex;
  position: relative;
  color: #fff;
}
.c_coupon_box{
  width: 500rpx;
  display: flex;
  align-items: center; 
  background: rgba(255, 37, 0, 1);
  position:relative;
}
.c_coupon_bg{
  position:absolute;
  top:0px;
  left:0px;
  width:100%;
  height:100%;
  display:block;
  z-index:0;
}
/* .c_coupon_cont{
  position:relative;
  z-index:2;
} */
.c_group_label{
  position: absolute;
  left: 0;
  top:0;
  padding: 8rpx 16rpx;
  background: rgba(8, 158, 0, 1);
  border-radius: 0 0 10rpx 0;
  font-size: 20rpx;
  line-height: 20rpx;
}
.c_price_box{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding:0 30rpx;
  padding-top: 6rpx;
  flex-shrink: 0;
  position: relative;
}
.c_split{
  position: absolute;
  right: 0;
  top: 50%; 
  transform: translateY(-50%);
  width: 1px;
  height: 50rpx;
  background: rgba(225, 225, 225, 0.2);
}
.c_price{
  font-size: 26rpx;
}
.c_price_ft{
  font-size: 60rpx;
  font-weight: bold;
}
.c_price_tip{
  font-size: 22rpx;
}
.c_name{
  padding-left: 30rpx;
  box-sizing: border-box;
  position:relative;
  z-index:1;
}
.btn_box{
  width: 210rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  background: #fff;
  text-align:center;
}
.btn{
  min-width:140rpx;
  box-sizing:border-box;
  padding:7rpx 26rpx;
  background: rgba(253, 253, 253, 1);
  border-radius: 50rpx;
  box-shadow: 0 0 2rpx 1px rgba(0, 0, 0, 0.05);
  margin-bottom: 15rpx;
  margin-top:10rpx;
  font-size:22rpx;
  font-weight:400;
  color:rgba(23,23,23,1);
  line-height:30rpx;
}
.s_count{
  font-size:20rpx;
  font-weight:400;
  color:rgba(178,178,178,1);
  line-height:28rpx;
}
.l_progress{
  width:133rpx;
  height:8rpx;
  background:rgba(239,239,239,1);
  border-radius:5rpx;
}
.l_val{
  width:0;
  height:100%;
  background:rgba(255,39,0,1);
  border-radius:5rpx;
  transition: width .35s;
  transition-delay: 1s;
}
.end_btn{
  font-size:26rpx;
  font-weight:400;
  color:rgba(178,178,178,1);
  line-height:37rpx;
  margin-bottom:10rpx;
}
.send_tip{
  margin-top:15rpx;
  font-size:18rpx;
  font-weight:400;
  color:rgba(178,178,178,1);
  line-height:25rpx;
  text-align: center;
}
.end_send_tip{
  font-size:22rpx;
}
.tips{
  box-sizing: border-box;
  padding:15rpx 40rpx 15rpx 30rpx;
  position: relative;
  background: #fff;
  overflow:hidden;
  /* box-shadow:0px 2rpx 15rpx 0 rgba(0,0,0,0.07);  */
}
.t_title_box{
  display: flex;
  justify-content: space-between;
  align-items: center;
  position: relative; 
  font-size:20rpx;
  font-weight:400;
  color:rgba(178,178,178,1);
  line-height:28rpx;
}
.t_tip{
}
.arrow{
  width: 0;
  height: 0;
  border-top: 8rpx solid #b2b2b2;
  border-left: 8rpx solid transparent;
  border-right: 8rpx solid transparent;
  position: absolute;
  right: -20rpx;
  top: calc(50% - 2rpx);
  z-index: 1;
}
.arrow_hide{
  width: 0;
  height: 0;
  border-top: 8rpx solid #fff;
  border-left: 8rpx solid transparent;
  border-right: 8rpx solid transparent;
  position: absolute;
  right: -20rpx;
  top: calc(50% - 6rpx);
  z-index: 2;
}
.t_header{ 
}
.tips_detail_box{
  max-height: 0;
  transition: max-height 0.35s;
  background:#fff;
  overflow: hidden;
  font-size:20rpx; 
  font-weight: 400;
  color: rgba(178,178,178,1);
} 
.tips_detail_box.active{
  max-height: 1000rpx;
}
.useLimitStay{
  box-sizing: border-box;
  padding: 20rpx 20rpx 20rpx 30rpx;
  padding-bottom: 0;
}
.limit,.tips_detail{
  line-height: 40rpx; 
}


.red_back{
  width:553rpx;
  height:166rpx;
  background: #EA5B18;
  border-radius: 20px 0px 0px 20px;
  margin-left: auto;
}
.red_back.used{
  background: rgba(178, 178, 178, 1);
}
.red_cont{
  position:absolute;
  top:50%;
  left: 50%;
  transform: translate(-50%,-50%);
  width:100%;
  height: 96%;
  /* height:156rpx; */
  border-radius: 20rpx;
  background: #FFFAF0;
  box-sizing: border-box;
  padding:10rpx;
}
.red_cont.used{
  background:rgba(239, 239, 239, 1);
}
.red_c{
  display:flex;
  align-items: center;
  justify-content: center;
  width:250rpx;
  height:100%;
  border-radius: 15rpx;
  border: 1px solid rgb(216, 202, 173,.36);
  box-sizing: border-box;
  text-align:center;
  color: #FF5B00;
}
.red_c.used{
  color: rgba(178,178,178,1);
  border: 1px solid rgb(178, 178, 178,.36);
}
.red_val{
  font-family: PingFangSC-Medium;
  /* color: #FF5B00; */
  font-size: 60rpx;
  line-height: 60rpx;
  font-weight: bold;
}
.val_m{
  font-size: 26rpx;
  margin-right:7rpx;
}
.red_init_val{
  font-family: PingFangSC-Regular;
  font-size: 20rpx;
  /* color: #FF5B00; */
}
.red_face{
  position:absolute;
  /* top:50%;
  right:0px;
  transform: translate(0,-50%); */
  right: 0;
  top: 0;
  box-sizing: border-box;
  height: 100%;  
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.red_square{
  display:block;
  width:13rpx;
  height:13rpx;
  border:8rpx solid rgba(255,255,255,0.12);
  position: absolute;
  left:40rpx;
  top:50%;
  transform:translate(0, -50%) rotate(-45deg);
  z-index:3;
}
.red_f{
  background: linear-gradient(270deg, #FF4400 0%, #FF6600 100%, #FFFFFF 100%);
  margin-left:auto;
  border-radius:100rpx;
  border-bottom-right-radius:0px;
  border-top-right-radius: 0px;
}
.red_f.used{
  background:rgba(178,178,178,1);
}
.red_f_t{
  /* height:60rpx; */
  height:42%;
  width:445rpx;
}
.red_f_m{
  /* height:99rpx; */
  height: 64%;
  width:463rpx;
  margin-top:-25rpx;
  margin-bottom:-25rpx;
  position:relative;
  z-index:2;
}
.red_f_b{
  /* height:60rpx; */
  height:42%;
  width:445rpx;
}
.face_cont{
  position:absolute;
  top:0px;
  left:0px;
  z-index:2;
  width:100%;
  height:100%;
  display:flex;
  justify-content: flex-start;
  align-items: center;
  padding-left:100rpx;
  box-sizing: border-box;
}
.pack_type{
  position: absolute;
  right: -50rpx;
  top: 20rpx;
  padding: 0 50rpx;
  background-color: #fff;
  color: #FF5B00;
  transform: rotate(45deg);
  font-size: 22rpx;
  line-height: 42rpx;
  text-align: center;  
}
.face_title{
  font-family: PingFangSC-Medium;
  font-size: 32rpx;
  color: #FFE6C2;
  padding:8rpx 0px;
}
.face_title.used,.face_time.used{
  color: #fff;
}
.face_time{
  font-family: PingFangSC-Regular;
  font-size: 22rpx;
  color: #FFE1B5;
  padding:8rpx 0px;
}
.is_expired{
  /* filter: grayscale(100%);    */
 /* -webkit-filter: grayscale(100%);   
 -moz-filter: grayscale(100%);    
-ms-filter: grayscale(100%);   
 -o-filter: grayscale(100%); */
}
.img{
  width: 190rpx;
  height: 190rpx;
  position: absolute;
  right: -10rpx;
  right: 0;
  top: 50%;
  transform: translateY(-50%);
  z-index: 2;
}