.main{
  width: 640rpx;
  border-radius:20rpx;
  /* overflow: hidden; */
  top: 44%;
  left: 50%;
  transform: translate(-50%,-50%);
  position: absolute;
  box-sizing: border-box;
  /* padding-bottom: 30rpx; */
}
.ac_bg{
  position: fixed;
  width: 100%;
  height: 100%;
  left:0;
  top: 0;
  background-color: transparent;
  /* z-index: 0; */
}
.img_ad_box{
  width: 100%;
  height: 200rpx; 
  margin-bottom: 30rpx;
  overflow: hidden;
  /* background: #fff; */
}
.img_ad_box.radius{
  border-radius: 20rpx 20rpx 0 0;
}
.msg_ad_box{
  width: 100%;
  height: 200rpx; 
  /* margin-bottom: 30rpx; */
  display: flex;
  flex-direction: column;
  justify-content: flex-end;
  align-items: center;
  box-sizing: border-box;
}
.msg_ad_box.onlyMsg{
  justify-content: center;
  height: 230rpx;
  padding-top: 40rpx;
}
.userName{
  font-size: 36rpx;
  color: #fff;
  line-height: 40rpx;
}
.totalBonus{
  font-size: 38rpx;
  color: #fff;
  line-height: 38rpx;
  padding-top: 20rpx;
}
.title_tips{
  font-size: 24rpx;
  line-height: 24rpx;
  color: rgba(255, 255, 255, 0.6);
  padding-top: 30rpx;
}
.img_top_ad,.img_ad{
  width: 100%;
  height: 100%;
}
.content{
  padding:30rpx;
  box-sizing: border-box;
}
.m_title{
  width: 100%;
  display:-webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp:1;
  -webkit-box-orient:vertical;
  line-height: 26rpx;
  margin-top: 4rpx;
}
.box{
  padding: 0 30rpx;
  padding-bottom: 0;
  box-sizing: border-box;
}
.data_img_box{
  width: 100%;
  /* min-height: 160rpx; */
  margin-bottom: 15rpx;
  /* overflow: hidden; */
}
.data_img_box:last-child{
  margin-bottom: 0;
}
.data_box{
  width: 100%;
  min-height: 160rpx;
  background: #fff;
  display: flex;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
  padding-left: 30rpx;
  padding-right: 0;
  margin-bottom: 15rpx;
}
.data_box:last-child{
  margin-bottom: 0;
}
.data_box.assets{
  /* min-height: 200rpx; */
}
.flex_col{
  display: flex;
  justify-content: center;
  flex-direction: column;
  width: 360rpx;
}
.price_box{
  align-items: center;
  width: 200rpx;
  height: 100%;
  position: relative;
}
.line{
  position: absolute;
  left: -1px;
  width: 1px;
  height: 94%;
  background: #efefef;
}
.btn_use{
  width: 110rpx;
  height: 40rpx;
  border-radius: 8rpx;
  color: #fff;
  font-size: 20rpx;
  margin-top: 14rpx;
}
.scroll{
  box-sizing: border-box;
  min-height: 160rpx;
  max-height: 440rpx;
  margin:30rpx 0;
  /* height: 335rpx; */
}
.scroll assets{
  height: 335rpx;
}
.close{
  position: absolute;
  bottom: -84rpx;
  left: 50%;
  border-radius: 50%;
  transform: translateX(-50%) rotate(45deg);
  width: 50rpx;
  height: 50rpx;
  border: 4rpx solid #fff;
}
.c_line{
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
  background-color: #fff;
}
.row{
  width: 55%;
  height: 4rpx;
}
.col{
  height: 55%;
  width: 4rpx;
}
.m_tips{
  font-size: 22rpx;
  line-height: 22rpx;
  margin-top: 14rpx;
}
.p_num{
  font-size: 46rpx;
  margin-right: 8rpx;
  line-height: 46rpx;
}
.p_text{
  font-size: 28rpx;
}
.p_tips_box{
  font-size: 22rpx;
  color: #7f7f7f;
  line-height: 22rpx;
  margin-top: 13rpx;
}
.btn_more_box{
  height: 130rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  box-shadow: 0 -6rpx 10rpx 4rpx rgba(0,0,0,0.15)
}
.btn_more{
  width: 308rpx;
  height: 80rpx;
  border-radius: 6rpx;
  color: #fff;
  font-size: 32rpx;
  box-shadow: 0 0 15rpx 6rpx rgba(0,0,0,0.1);
}