.progressBox {
  position: relative;
  width: 99%;
  height: 12rpx;
  background: #EBEBEB;
  border-radius: 10rpx;
}

.bar {
  position: relative;
  width: 0%;
  height: 100%;
  border-radius: 10rpx;
  /* transition: width 0.4s; */
}
.tran{
  transition: width 0.4s;
}

.barCircle {
  position: absolute;
  left: 100%;
  top: 50%;
  transform: translate(-50%, -50%);
  width: 25rpx;
  height: 25rpx;
  border-radius: 50%;
  z-index: 1;
}

.barCircle::after {
  content: '';
  position: absolute;
  width: 10rpx;
  height: 10rpx;
  display: block;
  top: 50%;
  left: 50%;
  border-radius: 50%;
  transform: translate(-50%, -50%);
  background: #fff;
}
.end_cir{
  position: absolute;
  left: 100%;
  top: 50%;
  transform: translate(-50%, -50%);
  width: 25rpx;
  height: 25rpx;
  border-radius: 50%;
  background: #EBEBEB;
  z-index: 0;
}
.tip {
  /* font-size: 22rpx;
  line-height: 24rpx;
  left: 50%;
  top: -150%;
  transform: translate(-50%,-50%); 
  position: absolute;
  color: #fff; */
  line-height: 33rpx;
  padding: 5rpx 10rpx;
  border-radius: 18rpx;
  font-size: 18rpx;
  color: #fff;
  position: absolute;
  left: 50%;
  top: -175%;
  transform: translateX(-50%);
  white-space: nowrap;
}

.s_e_tip {
  display: flex;
  /* padding: 20rpx 0 40rpx 0; */
  padding: 10rpx 0;
  font-size: 26rpx;
}

.s_e_tip.hide_p{
  padding: 0;
}

.start, .end {
  flex: 1;
}

.start {
  /* text-align: left; */
  display: flex;
  align-items: center;
}

.end {
  /* text-align: right; */
  display: flex;
  align-items: center;
  justify-content: flex-end;
}
