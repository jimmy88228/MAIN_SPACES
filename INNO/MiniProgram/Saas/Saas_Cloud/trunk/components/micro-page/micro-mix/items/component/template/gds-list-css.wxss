.tab-box {
  display: flex; 
  background: rgba(247, 247, 247, 1); 
  box-sizing: border-box;
  padding: 10rpx;
}

.tab-item {
  flex: 1;
  flex-shrink: 0; 
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  position: relative; 
  font-size: 26rpx;
  color: #7f7f7f; 
  box-sizing: border-box; 
  line-height: 30rpx;
  padding: 20rpx;
}
.tab-item.active{
  color: #fff;
}
.tab-name {
  text-align: center;
  overflow: hidden;
  text-overflow: ellipsis;
  width: 100%;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  box-sizing: border-box;
  padding:0 8rpx;
} 

.line {
  display: none;
}

.active.line {  
  position: absolute;
  bottom: 2rpx;
  left: 0;
  width: 100%;
  height: 4rpx;
  display: flex;
  justify-content: center;
}

.active.line::after {
  content: "";
  width: 40%;
  height: 100%;
  background: #171717;
}

.list-box {
  display: flex;
  flex-wrap: wrap;
  color: #171717;
  box-sizing: border-box;
  padding: 0 10rpx;
  /* justify-content: space-between; */
}

.list-item {
  flex: 1;
  flex-shrink: 0;
  margin-bottom: 10rpx;
  box-sizing: border-box;
  padding: 0 10rpx;
  font-size: 26rpx;
  color: #171717;
}
 

.list-img {
  width: 100%;
  height: 100%;
  display: block;
  position: absolute;
  top: 50%;
  left: 0px;
  transform: translateY(-50%);
  background-color: #fff;
}
 

.list-msg-box {
  box-sizing: border-box;
  padding: 20rpx;
  line-height: 32rpx;
  position: relative;
}


.price-box {
  font-size: 0.9em;
  width: 100%;
  word-break: break-all;
}

.list-img-box {
  width: 100%;
  padding-top: 100%;
  position: relative;
  overflow: hidden;
  flex-shrink: 0;
}
.list-img-logo{
  position: absolute;
  left: 10rpx;
  top: 10rpx;

}

.property_box {
  background: #f8f8f8;
  position: relative;
  width: 100%;
  padding: 12rpx 6rpx;
  color: #7f7f7f;
  box-sizing: border-box;
}

.property {
  font-size: 20rpx;
  line-height: 24rpx;
  margin-left: 8rpx;
  color: rgba(127, 127, 127, 1);
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.prom_label_box{
  width: 100%;
  position: relative;
  padding: 12rpx 0;
}

.msg-detail-box {}

.list-msg-name {
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  margin-bottom: 6rpx;
}

.char::before {
  content: "¥";
  font-size: 1em;
}

.list-msg-sale-price.char::before {
  font-size: 0.6em;
  margin-right: 3rpx;
}

/* 一行1-4个 */
.list-item-one {
  min-width: 100%;
  max-width: 100%;
  font-size: 30rpx;
}

.list-item-two {
  min-width: 50%;
  max-width: 50%;
  font-size: 24rpx;
}

.list-item-three {
  min-width: 33.33%;
  max-width: 33.33%;
  font-size: 22rpx;
}

.list-item-four {
  min-width: 25%;
  max-width: 25%;
  font-size: 20rpx;
}

/* 只有一行数据 */
.one-row .list-item-one,
.one-row .list-item-two,
.one-row .list-item-three,
.one-row .list-item-four {
  margin-bottom: 0;
}

.list-msg-sale-price {
  font-weight: bold;
  font-size: 32rpx;
  margin-bottom: 0rpx;
  margin-left: 1rpx;
}

.list-msg-sale-price:last-of-type {
  margin-bottom: 10 !important;
}

.list-msg-market-price {
  color: #b2b2b2;
  text-decoration: line-through;
  font-size: 22rpx;
}

/* 一大两小 */
/* 小 */
.list-item-bigSmall {
  min-width: 50%;
  max-width: 50%;
  font-size: 24rpx;
}
/* 大 */
.list-item-bigSmall:nth-child(3n+1) {
  min-width: 100%;
  max-width: 100%;
  font-size: 30rpx;
}

/* 一行详细列表 */
.list-item-list {
  min-width: 100%;
  max-width: 100%;
  font-size: 30rpx;
  display: flex;
}

/* 一大两小、一行一个、详细列表 字体调整 */
/* .list-item-bigSmall:nth-child(3n+1) .list-msg-sale-price,
.list-item-bigSmall:nth-child(3n+1) .list-msg-market-price,
.list-item-one .list-msg-sale-price,
.list-item-one .list-msg-market-price,
.list-item-list .list-msg-sale-price,
.list-item-list .list-msg-market-price {
  font-size: 0.87em;
} */

/* 一行两个 现价大小 */
.list-item-two .list-msg-sale-price{
  font-size: 36rpx;
}

/* 一行一个、一大一小(大)、列表 现价大小调整 */
.list-item-one .list-msg-sale-price,
.list-item-bigSmall:nth-child(3n+1) .list-msg-sale-price
.list-item-list .list-msg-sale-price{
  font-size: 38rpx;
}

/* 一行一个、一大两小、详细列表 购买按钮调整 */
.list-item-bigSmall .btn-buy{
  width: 140rpx;
  height: 60rpx;
}
.list-item-one .btn-buy , .list-item-bigSmall:nth-child(3n+1) .btn-buy{
  width: 180rpx;
  height: 70rpx;
  font-size: 26rpx;
}
.list-item-list .btn-buy{
  width: 160rpx;
  height: 60rpx;
  margin-right: 20rpx;
}
.list-item-two .btn-buy{
  width: 140rpx;
  height: 60rpx;
}
.list-item-four .price-box{
  display: unset;
}
.list-item-four .btn-buy{
  width: 100%;
  font-size: 22rpx;
  border-radius: 0;
}

/* 一行三、四个 商品信息内容 */
.list-item-three .list-msg-box,
.list-item-four .list-msg-box {
  padding: 10rpx;
}

/* 一行三、四个 商品信息内容 */
.list-item-three .price-box,
.list-item-four .price-box {
  font-size: 1em;
}

/* 详细列表 */
.list-item-list .list-img-box {
  width: 200rpx;
  padding-top: 200rpx;
}

.list-item-list .list-msg-box { 
  width: 100%;
  display: flex;
  flex-direction: column;
  padding: 10rpx 20rpx;
}

.list-item-list .price-box {
  flex: 1;
  align-items: flex-end;
} 

.list-item-list .msg-detail-box.large {
  padding-bottom: 10rpx;
}

.list-item-list .msg-detail-box.small {
  padding-bottom: 28rpx;
} 
 
