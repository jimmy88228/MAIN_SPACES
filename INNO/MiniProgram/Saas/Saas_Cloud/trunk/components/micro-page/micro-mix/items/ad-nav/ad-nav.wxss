/* components/micro-page/items/advertise/advertise.wxss */
.list {
  display: flex;
  width: 100%;
  align-items: center;
}
.list.warp{
  flex-wrap: wrap;
}
.list-item {
  flex-shrink: 0;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  min-height: 90rpx;
  position: relative;
}
.list-item.text{
  flex: 1;
}
.list-img{
  /* width: 90rpx;
  height: 90rpx; */
  width: 100%;
  position: relative;
  display: block;
}
.init .list-img{
  background: #dfdfdf;
}
.list-img.limit{
  width: 110rpx;
  height: 110rpx;
}
.list-img.limit.small{
  width: 90rpx;
  height: 90rpx;
}
.list-img.limit.circle{
  border-radius: 50%;
}
.title{
  line-height: 40rpx;
  font-size: 24rpx;
  margin: 4rpx 0;
}
.main.init{
  margin: 15rpx 0;
}
.init.title{
  background: #dfdfdf;
  color: #dfdfdf;
  margin: 10rpx 0;
  line-height: 28rpx;
}
.init .title{
  background: #dfdfdf;
  color: #dfdfdf;
  margin: 10rpx 0;
  height: 30rpx;
}
.init .text .title{
  height: 100%;
}

.init .initBg{
  width: 100%;
  height: 100%;
  background: #dfdfdf;
  position: absolute;
  z-index: 2;
  left: 0;
  top: 0;
}

.btn_link.contactPluginCell{
  z-index: 9;
}