page {
    --bg-color: #f7f7f7;
    --card-bg-color: white;
    --main-color: #000;
    --sub-color: #7f7f7f;
    --tips-color: #b2b2b2;
    --line-color: #eee;
    --button-title-color: #333;
    --tip_bg_color: rgb(252, 242, 229);
