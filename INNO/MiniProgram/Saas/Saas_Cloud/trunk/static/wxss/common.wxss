.activity-tag {
    position: absolute;
    left: 6rpx;
    top: 6rpx;
    width: 46rpx;
    height: 22rpx;
    line-height: 22rpx;
    background: rgba(218, 114, 148, 1);
    border-radius: 1rpx;
    border: 1rpx solid transparent;
    font-size: 18rpx;
    color: rgba(255, 255, 255, 1);
    text-align: center;
}

.noScroll {
    width: 100%;
    height: 100vh;
    overflow: hidden;
    box-sizing: border-box;
}

.oh {
    overflow: hidden;
}

.fr {
    float: right;
}

.fl {
    float: left;
}

image {
    width: auto;
    height: auto;
    font-size: 0;
}

button {
    display: flex;
    align-items: center;
    justify-content: center;
    color: #333;
    background: none;
    margin: 0;
    padding: 0;
    white-space: nowrap;
    border-radius: 0;
    font-size: 26rpx;
    overflow: visible;
}

button:after {
    border: none;
}

button[disabled]:not([type]) {
    background-color: #eee !important;
    color: #ccc !important;
    border-color: #eee !important;
}

.btn_link {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0px;
    left: 0px;
    background: none;
    z-index: 1;
}

