/* pages/store_pay/pay_result/pay_success.wxss */
page{
  background:#fff;
}
.pay_result_page{
  padding-top:50rpx;
}
.has_no_pay{
  text-align:center;
  margin-top:60rpx;
}
.pay_success{
  text-align:center;
  margin-top:60rpx;
}
.result_tip{
}
.result_state_img{
  display:inline-block;
}
.result_state_img image{
  display:inline-block;
  width:40rpx;
  vertical-align: middle;
  margin-right:10rpx;
}
.result_state_txt{
  display:inline-block;
  vertical-align: middle;
  font-weight:bold;
  font-size:30rpx;
}
.get_points{
  color:#7f7f7f;
  margin:20rpx 0px;
}
.pay_amount{
  margin-top:20rpx;
}
.pay_amount_count{
  font-size:70rpx;
  font-weight:bold;
}



.get_index_page{
  width:350rpx;
  line-height:90rpx;
  margin:30rpx auto;
  margin-top:100rpx;
  border:1px solid #d2d2d2;
  border-radius:10rpx;
  text-align:center;
}



