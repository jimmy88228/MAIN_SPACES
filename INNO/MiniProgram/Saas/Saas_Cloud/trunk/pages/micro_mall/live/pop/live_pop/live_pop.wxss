.join_live{
  position:fixed;
  bottom:200rpx;
  right:0px;
  background:linear-gradient(270deg,rgba(254,31,14,1) 0%,rgba(250,96,34,1) 100%);
  border-radius:200rpx;
  border-top-right-radius: 0px;
  border-bottom-right-radius: 0px;
  color:#fff;
  width:121rpx;
  height:88rpx;
  display:flex;
  align-items: center;
  font-size:22rpx;
  justify-content: center;
  text-align:center;
  /* padding-left:20rpx; */
  box-sizing: border-box;
  overflow: hidden;
}
.lines{
  display:flex;
  align-items: center;
  justify-content: center;
  width:100%;
  margin-bottom:3rpx;
}
._line{
  border-radius:20rpx;
  width:6rpx;
  background-color:#fff;
  display:flex;
  margin:0 3.7rpx;
}
.line_1{
  height:12rpx;
}
.line_2{
  height:20rpx;
}
.line_3{
  height:28rpx;
}
.get_number_btn{
  width:100%;
  height:100%;
  position:absolute;
  top:0px;
  left:0px;
  opacity: 0;
}