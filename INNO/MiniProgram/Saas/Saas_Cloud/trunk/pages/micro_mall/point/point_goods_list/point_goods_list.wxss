page {
    height: 100%;
    background: #fff!important;
}

.background0 {
    background: #f3f3f3 !important;
}

.background1 {
    background: #fff !important;
}

.goods_list_main {
    height: 100%;
    /* display: flex;
    flex-direction: column; */
}

.header_module {
    background: #fff;
    position: relative;
}

.tabs {
    display: flex;
    align-items: center;
    height: 90rpx;
    position: relative;
    border-bottom: 1px solid rgba(243, 243, 243, 1);
}

.tabs_item {
    flex: 1;
    text-align: center;
    font-size: 26rpx;
}

.tabs_item.current {
    font-size: 28rpx;
}

.tabLine {
    width: 56rpx;
    height: 6rpx;
    border-radius: 10rpx;
    overflow: hidden;
    position: absolute;
    bottom: 0;
    transition: left 300ms;
}

.tabLine.delay {
    transition: left 350ms;
}

.user_module {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 0px 15rpx 0 35rpx;
    height: 94rpx;
    /* position:fixed;
    top:0;
    left: 0; */
    width:100%;
    box-sizing:border-box;
    z-index: 2;
    position: relative;
}

.user_module .bg_color {
    opacity: 0.1;
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    z-index: 10;
}
.user_module .clone_color{
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    z-index: 8;
    background: white;
}
.user_info {
    display: inline-flex;
    align-items: center;
    font-weight: 400;
}

.relative {
    position: relative;
    z-index: 11;
}

.user_img {
    width: 60rpx;
    border-radius: 100%;
    overflow: hidden;
    margin-right: 23rpx;
}

.point {
    margin-left: 10rpx;
    font-size: 28rpx;
    font-weight: 500;
}

.record {
    border: 1px solid #333;
    display: flex;
    justify-content: center;
    align-items: center;
    width: 128rpx;
    height: 40rpx;
    z-index: 20;
}

.goods_list {
    /* display: inline-flex;
    flex: 1;
    box-sizing: border-box; */
    /* margin-top: 184rpx; */
    /* position: relative;
    z-index: 0; */
}

.ordertTab {
    text-align: center;
    background: #fff;
    color: #777;
    width: 100%;
    z-index: 2;
    position: relative;
    display: flex;
    border-bottom: 1rpx solid rgba(243, 243, 243, 1);
    box-sizing: border-box;
    height: 94rpx;
}

.active {
    color: #cd718f;
    font-weight: bold;
}

.lines {
    position: absolute;
    bottom: -3rpx;
    left: 50%;
    transform: translateX(-50%);
    width: 52rpx;
    height: 4rpx;
    border-radius: 20rpx;
    border: 1rpx solid transparent;
}

.orderTabTit {
    position: relative;
    width: 50%;
    height: 100%;
    box-sizing: border-box;
    font-size: 28rpx;
}

.ordertTab text {
    display: inline-block;
    vertical-align: middle;
}

.record {
    width: 140rpx;
    height: 50rpx;
    color: #fff;
    border-radius: 6rpx; 
    border: 0;
}