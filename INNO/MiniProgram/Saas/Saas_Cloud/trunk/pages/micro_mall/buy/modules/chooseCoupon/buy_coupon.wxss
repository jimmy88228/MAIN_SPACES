.use_coupon_fixed{
  position:fixed;
  left:0px;
  bottom:0px;
  width:100%;
  display:flex;
  justify-content: center;
  align-items: center;
  padding:10rpx 25rpx;
  background-color:#fff;
  box-sizing:border-box;
  z-index:11;
}
.fiexed_info{
  display:flex;
  justify-content: flex-start;
  align-items: center;
}
.fiexed_info_stay{
  width:350rpx;
  font-size:24rpx;
}
.fiexed_row{
  /* padding:2rpx 0px; */
}
.f_row_tit{
  font-size: 20rpx;
  color: #7F7F7F;
}
.f_row_txt{
  font-size: 28rpx;
  color: #000000;
  margin-top:4rpx;
}
.fiexed_list{
  width:100%;
  display:inline-flex;
  justify-content: center;
  align-items: center;
}
.fiexed_item{
  display:block;
  width:160rpx;
  border-radius:10rpx;
}
.fiexed_item .operate_btn{
  width:100%;
  height:80rpx;
  background-color:#eee;
  border-radius:10rpx;
}
.fiexed_item.ok_item{
  width:200rpx;
  margin-left:12rpx;
}
.coupon_name{
  position: absolute;
  top: 22rpx;
  left: 0;
  padding-left: 22rpx;
  z-index: 2;
  color: #fff;
  width: 100%;
  box-sizing: border-box;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: break-word;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  line-height: 56rpx;
  font-size: 50rpx;
}
.couponMsg .Un_c_n{
  padding-left: 35rpx;
}
.coupon_view{
  position:fixed;
  left:0px;
  bottom:0px;
  width:100%;
  border-top-right-radius: 20rpx;
  border-top-left-radius: 20rpx;
  overflow: hidden;
  background-color:#fff;
  opacity: 0;
  transform: translateY(100%);
  height: calc(100vh - 250rpx);
}
.tab_list{
  display:flex;
  align-items: center;
  justify-content: center;
  width:100%;
  position:absolute;
  top:0px;
  left:0px;
  background-color:#fff;
}
.tab_item{
  font-size: 26rpx;
  color: #7F7F7F;
  padding-top:64rpx;
  padding-bottom:39rpx;
  flex:1;
  text-align:center;
}
.current_line{
  position:absolute;
  left:0px;
  bottom:0px;
}
._line{
  background-color:#F14D00;
  width:66rpx;
  height:4rpx;
  border-radius:10rpx;
  margin:0 auto;
  display: block;
}
.selected{
  color: #171717;
}
.content{
  /* min-height:300rpx; */
  height:100%;
  background-color:#F0F0F0;
  padding-top:170rpx;
  padding-bottom:110rpx;
  box-sizing: border-box;
}
.list_swiper{
  width:100%;
  height:100%;
}
.box_area{
  width: 100%;
  box-sizing: border-box;
  padding:0px 24rpx;
}
.box{
  width: 100%;
  border-radius: 20rpx;
  overflow: hidden;
  position: relative;
  margin-bottom: 30rpx;
  box-sizing: border-box;
}
.box_cont{
  width: 100%;
  height: 180rpx;
  display: flex;
  box-shadow: 0 4rpx 4rpx 1px rgba(0, 0, 0, 0.05);
  position: relative;
  z-index: 2;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
}
.c_coupon_box{
  display: flex;
  position: relative;
  color: #fff;
}
.c_coupon_box{
  width: 500rpx;
  display: flex;
  align-items: center; 
  background: rgba(255, 37, 0, 1);
  position:relative;
}
.c_coupon_bg{
  position:absolute;
  top:0px;
  left:0px;
  width:100%;
  height:100%;
  display:block;
  z-index:0;
}
