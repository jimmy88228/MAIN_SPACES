} */
.c_group_label{
  position: absolute;
  left: 0;
  top: 0;
  padding: 6rpx 12rpx;
  background-color: rgba(0, 0, 0, 0.5);
  border-radius: 0 0 10rpx 0;
  font-size: 17rpx;
  line-height: 17rpx;
  z-index:2;
}
.c_price_box{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding:0 30rpx;
  padding-top: 6rpx;
  flex-shrink: 0;
  position: relative;
}
.c_split{
  position: absolute;
  right: 0;
  top: 50%; 
  transform: translateY(-50%);
  width: 1px;
  height: 50rpx;
  background: rgba(225, 225, 225, 0.2);
}
.c_price{
  font-size: 26rpx;
}
.c_price_ft{
  font-size: 60rpx;
  font-weight: bold;
}
.c_price_tip{
  font-size: 22rpx;
}
.c_name{
  padding-left: 30rpx;
  padding-right:20rpx;
  box-sizing: border-box;
  position:relative;
  z-index:1;
}
.btn_box{
  width: 210rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  background: #fff;
  text-align:center;
  position:relative;
}
.btn{
  min-width:140rpx;
  box-sizing:border-box;
  padding:7rpx 26rpx;
  background: rgba(253, 253, 253, 1);
  border-radius: 50rpx;
  box-shadow: 0 0 2rpx 1px rgba(0, 0, 0, 0.05);
  margin-bottom: 15rpx;
  margin-top:10rpx;
  font-size:22rpx;
  font-weight:400;
  color:rgba(23,23,23,1);
  line-height:30rpx;
}
.s_count{
  font-size:20rpx;
  font-weight:400;
  color:rgba(178,178,178,1);
  line-height:28rpx;
}
.l_progress{
  width:133rpx;
  height:8rpx;
  background:rgba(239,239,239,1);
  border-radius:5rpx;
}
.l_val{
  width:0;
  height:100%;
  background:rgba(255,39,0,1);
  border-radius:5rpx;
  transition: width .35s;
  transition-delay: 1s;
}
.end_btn{
  font-size:26rpx;
  font-weight:400;
  color:rgba(178,178,178,1);
  line-height:37rpx;
  margin-bottom:10rpx;
}
.send_tip{
  margin-top:15rpx;
  font-size:18rpx;
  font-weight:400;
  color:rgba(178,178,178,1);
  line-height:25rpx;
  text-align: center;
}
.end_send_tip{
  font-size:22rpx;
}
.tips{
  box-sizing: border-box;
  padding:15rpx 40rpx 15rpx 30rpx;
  position: relative;
  background: #fff;
  overflow:hidden;
  border-bottom-left-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
  /* box-shadow:0px 2rpx 15rpx 0 rgba(0,0,0,0.07);  */
}
.t_title_box{
  display: flex;
  justify-content: space-between;
  align-items: center;
  position: relative; 
  font-size:20rpx;
  font-weight:400;
  color:rgba(178,178,178,1);
  line-height:28rpx;
}
.t_tip{
}
.arrow{
  width: 0;
  height: 0;
  border-top: 8rpx solid #b2b2b2;
  border-left: 8rpx solid transparent;
  border-right: 8rpx solid transparent;
  position: absolute;
  right: -20rpx;
  top: calc(50% - 2rpx);
  z-index: 1;
}
.arrow_hide{
  width: 0;
  height: 0;
  border-top: 8rpx solid #fff;
  border-left: 8rpx solid transparent;
  border-right: 8rpx solid transparent;
  position: absolute;
  right: -20rpx;
  top: calc(50% - 6rpx);
  z-index: 2;
}
.t_header{ 
}
.tips_detail_box{
  max-height: 0;
  transition: max-height 0.35s;
  background:#fff;
  overflow: hidden;
  font-size:20rpx; 
  font-weight: 400;
  color: rgba(178,178,178,1);
} 
.tips_detail_box.active{
  max-height: 1000rpx;
}
.useLimitStay{
  box-sizing: border-box;
  padding: 20rpx 20rpx 20rpx 30rpx;
  padding-bottom: 0;
}
.limit,.tips_detail{
  line-height: 40rpx; 
}
