.gift_goods{
  background: #FFFFFF;
  box-shadow: 0 2rpx 17rpx 0 rgba(0,0,0,0.08);
  border-radius: 10rpx;
  padding:16rpx;
  position:relative;
}
.list_item{
  margin-bottom:20rpx;
}
.goods_img{
  width:155rpx;
  height:155rpx;
  display:block;
  background-color:#efefef;
}
.goods_detail{
  padding:30rpx 20rpx 50rpx 35rpx;
}
.goods_btns{
  position:absolute;
  bottom:20rpx;
  right:20rpx;
  border-radius:100rpx;
  display:flex;
  align-items: center;
  justify-content: center;
  height: 50rpx;
  padding:0px 30rpx;
  color:#fff;
  background-color:#F14D00;
  box-sizing: border-box;
  font-size:24rpx;
}
.is_check_btn{
  color: #F14D00;
  border:1px solid #F14D00;
  background-color:#fff;
}
.overdue_img{
  width:140rpx;
  position:absolute;
  right:10rpx;
  bottom: 10rpx;
}