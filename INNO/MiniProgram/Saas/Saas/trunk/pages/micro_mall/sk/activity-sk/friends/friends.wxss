.box{
  width: 100%;
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 12;
  padding: 60rpx;
  box-sizing: border-box;
  background:rgba(255,255,255,1);
  box-shadow: 0px 4rpx 20rpx 4px rgba(0, 0, 0, 0.15);
  border-radius:24rpx 24rpx 0px 0px;
}

.f_box{
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  min-height: 240rpx;
}
.f_item{
  width: 88rpx;
  padding-right: 46rpx;
}
.f_item:nth-child(5n){
  padding-right: 0;
}
.img_user{
  width: 100%;
  height: 88rpx;
  border-radius: 50%;
  border:1px solid rgba(239, 239, 239, 1);
  box-sizing: border-box;
}
.name_user{
  padding-top: 4rpx;
  line-height: 22rpx;
  font-size:20rpx;
  color:rgba(40,40,40,1);
  text-align: center;
  word-break: break-all;
}
.tips{
  text-align: center;
  padding-bottom: 60rpx;
  font-size: 30rpx;
  line-height: 30rpx;
  font-weight: bold;
}
.close_area{
  position:absolute;
  top: 14rpx;
  right: 18rpx;
  padding:10rpx;
}
.close_icon{
  position:relative;
  width:40rpx;
  height:40rpx;
  transform: rotate(45deg);
}
.close_icon .row{
  position:absolute;
  top:50%;
  left:50%;
  width:5rpx;
  height:80%;
  transform: translate(-50%,-50%);
  background-color:#b2b2b2;
}
.close_icon .line{
  position:absolute;
  top:50%;
  left:50%;
  height:5rpx;
  width:80%;
  transform: translate(-50%,-50%);
  background-color:#b2b2b2;
}