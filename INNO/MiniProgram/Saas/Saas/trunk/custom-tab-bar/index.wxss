.tab-bar-item {
  flex: 1;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  position:relative;
  font-size:0px;
}
.box{
  text-align: center;
  display: flex;
  flex-direction: column;
  align-items: center; 
}
.tab-bar-item .img_box {
  width: 54rpx;
  height: 54rpx;
  position: relative;
  /* display: flex;
  justify-content: center; */
}
.tab-bar-item .tab_icon {
  width: 54rpx;
  height: 100%;
  position: relative;
}
.circle{
  width: 16rpx;
  height: 16rpx;
  position: absolute;
  top: 6rpx;
  right: -2rpx;
  z-index: 2;
  border-radius: 50%;
  border: 2rpx solid #fff;
  background: #d94923;
}
.tab-bar-item .tab_txt {
  font-size: 20rpx;
}
.center{
  margin-bottom:50rpx;
  border-radius:100%;
  position:relative;
  z-index:2;
  width:100rpx;
  height:100rpx;
  background-color:#CD718F;
  display:flex;
  justify-content: center;
  align-items: center;
  color:#fff;
  border:15rpx solid #efefef;
}
.v_line{
  width:5%;
  height:70%;
  position:absolute;
  top:50%;
  left:50%;
  transform: translate(-50%,-50%);
  background-color:#fff;
}
.h_line{
  width:70%;
  height:5%;
  position:absolute;
  top:50%;
  left:50%;
  transform: translate(-50%,-50%);
  background-color:#fff;
}
.tab_btn{
  position:absolute;
  top:0px;
  left:0px;
  width:100%;
  height:100%;
}
.pre_image{
  width:54rpx;
}
