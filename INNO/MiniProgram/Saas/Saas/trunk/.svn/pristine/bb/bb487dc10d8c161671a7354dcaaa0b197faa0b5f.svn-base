} 
.tips_detail_box.active{
max-height: 1000rpx;
}
.useLimitStay{
box-sizing: border-box;
padding: 20rpx 20rpx 20rpx 30rpx;
padding-bottom: 0;
}
.limit,.tips_detail{
line-height: 40rpx; 
}
.btn_box button{
width: 140rpx;
height: 50rpx;
font-size: 22rpx; 
border: 1px solid #EFEFEF;
border-radius: 26rpx;
box-sizing: border-box;
margin-bottom: 12rpx;
}
.btn_box button:last-child{
margin-bottom: 0;
}
.img_c_state{
width: 100%;
height: 100%;
}
.info_tip{
width:100%;
text-align: center;
padding-top:40rpx;
font-size:26rpx;
}
.show_tips{
