/* pages/coupon/my_coupon.wxss */

page {
  background: #fff;
  height: 100%;
}

.couponPage{
  height: 100%;
}
.couponTab {
  text-align: center;
  background: #fff;
  color: #777;
  width: 100%;
  position: fixed;
  z-index: 3;
  box-shadow: 0 -10px 10px 10px #efeded;
  overflow: visible;
}

.active {
  color: #cd718f;
}

.lines {
  position: absolute;
  bottom: -3rpx;
  left: 50%;
  transform: translateX(-50%);
  width: 52rpx;
  height: 4rpx;
  border-radius: 20rpx;
  border: 1rpx solid transparent;
}

.couponTabTit {
  position: relative;
  width: 50%;
  line-height: 90rpx;
  height: 90rpx;
  box-sizing: border-box;
}

.couponTabTit text {
  display: inline-block;
  vertical-align: middle;
  font-size: 28rpx;
}

.couponList {
  background: #EFEFEF;
  padding-top: 90rpx;
  box-sizing: border-box;
  height: 100%;
}

.couponItem {
  position: relative;
}

.couponMsg {
  position: relative;
  background-color: #fff;
  width:710rpx;
  height:245.26rpx;
  overflow: hidden;
}

.useLimitStay {
  padding: 5% 0px;
  color: rgba(130, 130, 130, 1);
}

/* .CrumbsIndicator  */
.arrow {
  vertical-align: middle;
  width: 28rpx;
  /* width: 50rpx; */
}

/* .CrumbsIndicator */
.arrow_active {
  transform: rotate(180deg);
}

.selectedCouponMark {
  position: fixed;
  z-index: 51;
  top: 0px;
  left: 0px;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
}

.selectedCoupon {
  padding: 24rpx 0 80rpx 0;
  background: rgba(255, 255, 255, 1);
  position: fixed;
  z-index: 51;
  width: 620rpx;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  border-radius: 6rpx;
}

.couponItem {
  margin: 20rpx;
}

.selectedCoupon .couponItem {
  padding: 0px;
}

.selectedCoupon .codeImg {
  margin-top: 20rpx;
  width: 100%;
  height: 150rpx;
}

.checkCodeBtn {
  display: block;
  text-align: center;
  padding: 10rpx 0px;
}

.largetCodeMark {
  position: fixed;
  top: 0px;
  left: 0px;
  width: 100%;
  height: 100%;
  z-index: 52;
  background: rgba(0, 0, 0, 0.6);
}

.largetCode {
  padding: 20rpx;
  position: fixed;
  z-index: 53;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%) rotate(90deg);
  background: #fff;
}

.largetCodeImg{
  width: 780rpx;
  height: 204rpx;
}

/*数据为空*/

.emptyCoupon {
  margin: 100rpx 0px;
  text-align: center;
}

.emptyCoupon image {
  width: 200rpx;
  height: 200rpx;
  margin: 0 auto;
  margin-bottom: 20rpx;
}

.CrumbsIndicator_text {
  display: inline-block;
  vertical-align: middle;
  color: rgba(130, 130, 130, 1);
  font-size: 26rpx;
}

.table_cell {
  display: flex;
  align-items: center;
}
.show_big_click{
overflow: hidden;
position:relative;
}
.barCanvas_area{
position:absolute;
z-index:1;
/* opacity: 0;
width:0px;
height:0px;
overflow: hidden; */
transform: translate(1000px,1000px);
}
.barCanvas {
  display: block;
  margin: 50rpx auto 29rpx auto;
}
.barCanvas_img{
margin:0 auto;
display: block;
}
.show_bonus_code{
position:absolute;
z-index:2;
}
.bonus_sn{
padding:10rpx 0;
text-align:center;
color:rgba(127, 127, 127, 1);
}

.bouns_type {
  display: flex;
  flex-direction: column;
}
.startTimeTile{
text-align: center;
font-size: 40rpx; 
}
.startTimeTip{
text-align: center;
margin-top: 25rpx;
}

.couponTxt_select {
  width: 92%;
  padding: 0 40rpx 20rpx 40rpx;
  justify-content: flex-start;
}

.coupon_header {
  width: 100%;
  font-size: 28rpx;
  color: rgba(51, 51, 51, 1);
  text-align: center;
  font-weight: bold;
}

.close {
  display: block;
  width: 20rpx;
  height: 20rpx;
}

.icon_wrapper {
  display: flex;
  align-items: center;
  justify-content: center;
  position: absolute;
  top:2rpx;
  right:0rpx;
  width:50rpx;
  height:40rpx;
  padding:20rpx 15rpx;

}

.couponItem_select {
  margin: 56rpx 0 60rpx 0;
}

.the_line {
  width: 100%;
  height: 1rpx;
  border-top: 1rpx solid rgba(239, 239, 239, 1);
}

.couponMsg_select {
  margin: 0 50rpx 60rpx 50rpx;
  overflow: hidden;
  width:520rpx;
  height:179.63rpx;
}
.couponMsg_select .bg_wrapper image{
width:100%;
height:100%; 
}

.to_tips {
  font-size: 24rpx;
  color: rgba(127, 127, 127, 1);
  text-align: center;
}

.bg_wrapper {
  width: 100%;
  height: 178rpx;
}
.coupon_operate{
display:flex;
justify-content: space-around;
align-items: center;
}
.writeOff {
  width: 240rpx;
  height: 70rpx;
  margin: 0 auto;
  border-radius: 6rpx;
  color: #fff;
  font-size: 24rpx;
  margin-top: 53rpx;
}

.black_bg {
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  position: fixed;
  z-index: 52;
  background: rgba(0, 0, 0, 0.7);
  display: none;
}

.black_bg.isShow {
  display: block;
}

.share_box {
  box-sizing: border-box;
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  width: 610rpx;
  background: #fff;
  /* transition: all 0.4s; */
  display: none;
  opacity: 0;
  background: rgba(255, 255, 255, 1);
  border-radius: 10rpx;
  z-index:52;
}

.share_box.isShow {
  display: block;
  opacity: 1;
  z-index: 111;
}

.share_box_text {
  position: relative;
  width: 100%;
  text-align: center;
  padding: 100rpx 0; 
  font-size: 28rpx;
  font-weight: bold;
}

.confirm {
  width: 50%;
  font-size: 28rpx; 
  box-sizing: border-box;
  line-height: 90rpx;
  height:90rpx;
  text-align: center;
  display: inline-block;
  font-weight: bold;
  border-top: 1px solid #efefef; 
}
.confirm.cancel{
  color: #b2b2b2;
  border-right: 1px solid #efefef; 
}
.coupon_name{
position: absolute;
top: 38rpx;
/* left: 22rpx; */
left: 0;
padding-left: 22rpx;
z-index: 2;
color: #fff;
width: 100%;
box-sizing: border-box;
display: -webkit-box;
overflow: hidden;
text-overflow: ellipsis;
word-wrap: break-word;
-webkit-line-clamp: 1;
-webkit-box-orient: vertical;
line-height: 58rpx;
font-size: 52rpx;
}
.couponMsg .Un_c_n{
padding-left: 35rpx;
}
.availableCoupon{
 height: 100%;
}
.content{
  height: 100%;
}
.coupon_box{

}
.c_btn_box button{
  background: #fff;
  border:1px solid #EFEFEF;
  width: 140rpx;
  height: 50rpx;
  font-size: 22rpx;
}
.box{
width: 100%;
border-radius: 14rpx;
/* overflow: hidden; */
position: relative;
margin-bottom: 30rpx;
overflow: hidden;
/* border: 1px solid #efefef; */
box-sizing: border-box;
}
.box_cont{
width: 100%;
height: 180rpx;
display: flex;
box-shadow: 0 4rpx 10rpx 1px rgba(0, 0, 0, 0.05);
position: relative;
z-index: 2;
}
.c_coupon_box{
display: flex;
position: relative;
color: #fff;
}
.c_coupon_box{
width: 526rpx;
display: flex;
align-items: center; 
position:relative;
flex-shrink: 0;
}
.c_coupon_bg{
position:absolute;
top:0px;
left:0px;
width:100%;
height:100%;
display:block;
z-index:0;
}
.c_group_label{
position: absolute;
left: 0;
top:0;
padding: 6rpx 12rpx;
background-color: rgba(0, 0, 0, 0.5);
border-radius: 0 0 10rpx 0;
font-size: 17rpx;
line-height: 17rpx;
}
.c_price_box{
display: flex;
flex-direction: column;
justify-content: center;
align-items: center;
padding:0 30rpx;
padding-top: 6rpx;
flex-shrink: 0;
position: relative;
}
.c_split{
position: absolute;
right: 0;
top: 50%; 
transform: translateY(-50%);
width: 1px;
height: 50rpx;
background: rgba(225, 225, 225, 0.2);
}
.c_price{
font-size: 26rpx;
}
.c_price_ft{
font-size: 70rpx;
font-weight: bold;
}
.c_price_tip{
font-size: 22rpx;
}
.c_name{
padding-left: 30rpx;
box-sizing: border-box;
position:relative;
z-index:1;
}
.btn_box{
width: 100%;
display: flex;
flex-direction: column;
justify-content: center;
align-items: center;
background: #fff;
text-align:center;
}
.btn{
min-width:140rpx;
box-sizing:border-box;
padding:7rpx 26rpx;
background: rgba(253, 253, 253, 1);
border-radius: 50rpx;
box-shadow: 0 0 2rpx 1px rgba(0, 0, 0, 0.05);
margin-bottom: 15rpx;
margin-top:10rpx;
font-size:22rpx;
font-weight:400;
color:rgba(23,23,23,1);
line-height:30rpx;
}
.s_count{
font-size:20rpx;
font-weight:400;
color:rgba(178,178,178,1);
line-height:28rpx;
}
.l_progress{
width:133rpx;
height:8rpx;
background:rgba(239,239,239,1);
border-radius:5rpx;
}
.l_val{
width:0;
height:100%;
background:rgba(255,39,0,1);
border-radius:5rpx;
transition: width .35s;
transition-delay: 1s;
}
.end_btn{
font-size:26rpx;
font-weight:400;
color:rgba(178,178,178,1);
line-height:37rpx;
margin-bottom:10rpx;
}
.send_tip{
margin-top:15rpx;
font-size:18rpx;
font-weight:400;
color:rgba(178,178,178,1);
line-height:25rpx;
text-align: center;
}
.end_send_tip{
font-size:22rpx;
}
.tips{
box-sizing: border-box;
padding:15rpx 20rpx 15rpx 30rpx;
position: relative;
background: #fff;
overflow:hidden;
/* box-shadow:0px 2rpx 15rpx 0 rgba(0,0,0,0.07);  */
}
.t_title_box{
display: flex;
justify-content: space-between;
align-items: center;
position: relative; 
font-size:20rpx;
font-weight:400;
color:rgba(178,178,178,1);
}
.t_title{
display:flex;
align-items: center;
}
.t_title.title_active{
  color: #F14D00;
}
.t_tip{
}
