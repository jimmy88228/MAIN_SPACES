page{
  background-color:#fff;
}
.fans_item{
  width:100%;
  padding:45rpx 30rpx;
  border-bottom:1px solid #efefef;
}
.fans_head{
  width:60rpx;
  height:60rpx;
  margin-right:15rpx;
}
.fans_head image{
  display:block;
  width:100%;
  height:100%;
  border-radius:100%;
}
.fans_btn{
  width:120rpx;
  height:50rpx;
  color:#fff;
  background-color:#000;
  box-shadow:0px 0px 10rpx 0px rgba(145,30,68,0.43);
  border-radius:6rpx;
  margin-left:auto;
}
.fansed_btn{
  background:none;
  border:1px solid #b2b2b2;
  color:#666;
  box-shadow:0px 0px 10rpx 0px rgba(172, 172, 172, 0.43);
}