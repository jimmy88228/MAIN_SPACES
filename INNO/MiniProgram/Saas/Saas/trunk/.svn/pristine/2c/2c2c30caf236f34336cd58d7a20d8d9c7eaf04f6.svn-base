.main{
  position: fixed;
  top: 50%;
  left: 50%; 
  background: #fff;
  padding: 45rpx; 
  box-sizing: border-box;
  border-radius: 10rpx;
  width: 640rpx;
  transform: translate(-50%,-50%);
  min-height: 500rpx;
}
.box{
  line-height: 36rpx;
  padding: 20rpx 0;
}
._bold{
  font-weight: bold;
}
.btn{
  width: 220rpx;
  height: 80rpx;
  border-radius: 10rpx;
  margin: 60rpx 0 0 0;
  color: #fff;
  font-weight: bold;
}