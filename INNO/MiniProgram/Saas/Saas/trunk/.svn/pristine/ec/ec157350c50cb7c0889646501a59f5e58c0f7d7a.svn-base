.presale_order_list{
  padding-bottom:100rpx;
}
.presale_order_list.ipx{
  padding-bottom:168rpx;
}
.pre_order_list {
    padding-top: 7rpx;
}

.order_item_cont {
    background-color: #fff;
    margin-bottom: 10rpx;
    padding-left: 30rpx;
}

.pay_status {
    display:flex;
    align-items:flex-end;
    justify-content:space-between;
    padding: 20rpx 30rpx 20rpx 0;
}

.order_pay_status {
    font-size: 24rpx;
    font-weight: bold;
    text-align: right;
}

.order_goods_info {
    padding: 24rpx 30rpx 24rpx 0;
    position: relative;
    border-top: 1px solid rgba(239, 239, 239, 1);
    border-bottom: 1px solid rgba(239, 239, 239, 1);
}

.order_goods_img_area {
    width: 160rpx;
    padding-right: 20rpx;
}

.order_goods_img_area_stay {
    border: 1px solid #efefef;
    width: 160rpx;
}

.order_goods_img_stay {
    width: 160rpx;
    height: 160rpx;
    margin-right: 20rpx;
    border:1px solid #efefef;
}

.order_goods_img {
    width: 160rpx;
    height: 160rpx;
}

.order_goods_info_cell {
    position: relative;
    flex: 1;
}

.goods_name {
    margin-bottom: 10rpx;
    font-size:26rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.o_goods_size {
    margin-top: 7rpx;
}

.order_deliver_time {
    font-size:18rpx;
    display: flex;
    align-items: flex-end;
    justify-content: space-between;
}

.goods_amount {
    position: absolute;
    top: 0px;
    right: 0px;
    padding-top: 7rpx;
}

.goods_count {
    position: absolute;
    bottom: 0px;
    right: 0px;
}

.order_operate {
    text-align: right;
    padding: 20rpx 30rpx 20rpx 0;
    font-size: 0px;
}

.order_operate button {
    font-size: 24rpx;
    width: 180rpx;
    height: 65rpx;
    line-height: 65rpx;
    -moz-border-radius: 5rpx;
    -webkit-border-radius: 5rpx;
    display: inline-block;
    /*line-height:normal;*/
    text-align: center;
    padding: 0px;
    margin: 0px;
    margin-left: 10rpx;
}

.order_operate button:after {
    display: none;
}

.order_operate .cancel_order_btn {
    color: #a1a1a1;
    border: 1px solid #efefef;
}

.order_add_time {
    color: #818181;
    font-size: 22rpx;
}

.failed {
    color: #767676;
}

.finish {
    color: #000;
}
.o_goods_color, .o_goods_size{
    font-size:24rpx;
    color:rgba(127,127,127,1);
}
.o_goods_color{
    margin-bottom: 4rpx;
}
.o_goods_size{
    margin-bottom: 20rpx;
}
.p_rela{
    display: flex;
    align-items: center;
    justify-content: space-between;
}
.goods_number{
    font-size:26rpx;
    color:rgba(127,127,127,1);
}
.common_btn{
    background:rgba(255,255,255,1) !important;
    border:1px solid #7f7f7f !important;
    border-radius:4px !important;
    color:rgba(27,27,27,1) !important;
}
.cancel_btn{
    background:rgba(255,255,255,1) !important;
    border:1px solid #7f7f7f !important;
    border-radius:4px !important;
    color:rgba(127,127,127,1) !important;
}
.color_title,.size_title{
    margin-right: 12rpx;
}
.good_max_name{
    width:400rpx;
    word-wrap: break-word;
    white-space: pre-wrap;
}