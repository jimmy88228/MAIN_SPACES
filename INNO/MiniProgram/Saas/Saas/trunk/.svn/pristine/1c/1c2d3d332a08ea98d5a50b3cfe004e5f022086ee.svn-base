.main{
  width: 600rpx;
  border-radius: 20rpx;
  top: 44%;
  left: 50%;
  transform: translate(-50%,-50%);
  position: fixed;
  box-sizing: border-box;
  background-color:#fff;
}
.main_tip{
  line-height:55rpx;
  font-size:30rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
}
.main_cont{
  padding-top:30rpx;
  padding-bottom:40rpx;
  text-align:center;
}
