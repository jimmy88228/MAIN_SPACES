.main_img_area{
  padding:35rpx 0px;
}
.main_img{
  width:100%;
  height:175rpx;
  display:block;
  margin:0 auto;
}
.head_img{
  width:124rpx;
  height:124rpx;
  border-radius: 100%;
  overflow: hidden;
  background:#efefef;
}
.operate_btns{
  text-align:center;
  font-size:29rpx;
  font-family:PingFangSC-Regular,PingFang SC;
}
.o_btn{
  width:100%;
  line-height:100rpx;
  height:100rpx;
  border-top:1px solid #efefef;
  font-weight:400;
  color:#B2B2B2;
}
.ok_o_btn{
  color:#00A0F3;
}
