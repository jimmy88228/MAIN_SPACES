.verify_box {
  padding: 8rpx 14rpx;
  border-radius: 20rpx;
  background-color: var(--tip_bg_color);
  color: var(--tip_font_color);
  display: flex;
  justify-content: center;
  align-items: center;
  float: right;
  margin-right: 15rpx;
}

.verify_icon {
  width: 28rpx;
  height: 28rpx;
}

.verify_tip {
  font-size: 20rpx;
  line-height: 20rpx;
  padding: 0 5rpx;
}

.verify_arrow {
  border-bottom: 1px solid var(--tip_font_color);
  border-right: 1px solid var(--tip_font_color);
  transform: rotate(-45deg);
  width: 8rpx;
  height: 8rpx;
}

