  line-height: 16rpx;
  color: #fff;
  border-radius: 12rpx;
  background-color: #FF3400;
}

.r_t_arrow {
  width: 0;
  height: 0;
  border-top: 8rpx solid #FF3400;
  border-right: 16rpx solid transparent;
  margin-left: -12rpx;
}

