}

.qr_tips {
  text-align: center;
  padding-bottom: 54rpx;
  margin-top: -40rpx;
  line-height: 24rpx;
  position: relative;
  z-index: 1;
}

.hide_canvas {
  width: 0px;
  height: 0px;
  opacity: 0;
  overflow: hidden;
  position: absolute;
  left: -1000px;
  right: -1000px;
  transform: translate(-100%, -100%);
}

.rotate {
  animation: rotateAnimate linear .3s infinite;
  -webkit-animation: rotateAnimate linear .3s infinite;
}

@keyframes rotateAnimate {
  0% {
    -webkit-transform: rotate(0deg);
    transform: rotate(0deg);
  }

  100% {
    -webkit-transform: rotate(1turn);
    transform: rotate(1turn);
  }
}

.rankTip {
  position: absolute;
  right: -20rpx;
  top: -10rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.r_t_name {
  padding: 6rpx 8rpx;
