.box{
  width: 100%;
  height: 100%;
  position: relative;
}
.tips{
  position: absolute;
  top: 46%;
  left: 50%;
  transform: translate(-50%,-50%);
  padding: 56rpx 56rpx;
  background: rgba(0, 0, 0, 0.55);
  color: #fff;
  line-height: 40rpx;
  border-radius: 10rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.tips.small{
  padding: 50rpx 40rpx;
}
.tip_text{
  padding-top: 16rpx;
}