@import "/static/iconFont/font.wxss";
page {
    background-color: #fff;
    height: 100%;
    --main_color: "#fff";
    --light_color: "#fff";
}

.category_page {
    height: calc(100% - 190rpx);
    /* padding-top: 80rpx; */
    box-sizing: border-box;
    position: relative;
    z-index: 1;
}

.goods_list {
    width: 100%;
    padding: 10rpx 13rpx;
    box-sizing: border-box;
    letter-spacing: 0.5px;
    font-size: 0;
    /* width: 100%; */
    /* box-sizing: border-box; */
    /* padding-top: 200rpx; */
    /* padding-top: 100rpx; */
    /* display:inline-block; */
}


.goods_item_box {
    width: 100%;
    display: inline-flex;
    flex-wrap: wrap;
    margin-bottom: 30rpx;
    /* padding:0px 10rpx; */
    box-sizing: border-box;
    position: relative;
    font-size: 0px;
}

.list_item {
    width: 50%;
    display: inline-block;
    vertical-align: top;
    padding: 7rpx;
    box-sizing: border-box;
    border-top-left-radius: 20rpx;
    border-top-right-radius: 20rpx;
}

.p_item {
    width: 100%;
    height: 100%;
    position: relative;
    display: flex;
    flex-direction: column;
}

.goods_item_box.right {
    padding-right: 5rpx;
    padding-left: 0px;
}

.goods_item {
    width: 100%;
    height: 100%;
    vertical-align: top;
    background: none;
    text-align: center;
    /* position: relative; */
    position: absolute;
    top: 0px;
    left: 0px;

}

.goods_list .goods_item:nth-child(odd) {
    /* margin-right: 20rpx; */
}

.goods_img_box {
    margin: 0 auto;
    width: 100%;
    height: 0px;
    padding-top: 100%;
    position: relative;
}

.goods_img_stay {
    /* width: 360rpx;
    height: 360rpx; */
    width: 100%;
    height: 100%;
    overflow: hidden;
    position: absolute;
    top: 0px;
    left: 0px;
    border-top-left-radius: 20rpx;
    border-top-right-radius: 20rpx;
    background-color: #ffffff;
}

.goods_img_stay image {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    /* background-color:#efefef; */
}

.property_box {
    position: relative;
    margin: 0 0 12rpx 0;
    padding: 0rpx 10rpx;
    border-radius: 100rpx;
    background-color: #fafafa;
    box-sizing: border-box;
    max-width: 100%;
    color: #7f7f7f;
    display: inline-flex;
    align-items: center;
    justify-content: space-between;
    place-self: flex-start;
}

.property_box:last-of-type {
    margin-bottom: 0;
}

.property_left {
    display: flex;
    align-items: center;
}

.property_right .iconfont {
    color: #b2b2b2;
    font-size: 20px;
    margin-right: -10rpx;
}

.property_box .property_circle {
    padding: 2rpx;
    border-radius: 50%;
    border: currentColor solid 4rpx;
    margin-right: 10rpx;
    background-color: #ffffff;
}

.propertyClass {
    word-break: break-all;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
    height: 100%;
    font-size: 20rpx;
    color: #7f7f7f;
    
    /* line-height: 37rpx; */
}

.goods_info {
    flex: 1;
    display: flex;
    flex-direction: column;
    text-align: left;
    width: 100%;
    padding: 10rpx 20rpx;
    box-sizing: border-box;
    background-color: #ffffff;
    border-bottom-left-radius: 20rpx;
    border-bottom-right-radius: 20rpx;
    /* min-height:200rpx; */
}

.goods_name {
    word-break: break-all;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    color:#333333;
    line-height: 30rpx;
    margin-bottom: 12rpx;
    font-size: 26rpx;
    /* height:60rpx; */
}

.label_box {
    width: 100%;
    /* margin-top: 6rpx; */
    display: flex;
    /* align-items: center; */
    flex-wrap: wrap;
    /* min-height: 12rpx;  */
}

.label_content {
    margin-right: 7rpx;
    margin-bottom: 5rpx;
    /* width: 52rpx; */
    /* width: ; */
    /* height: 23rpx; */
    display: inline-flex;
}

.tag_img {
    max-width: 100%;
    max-height: 100rpx;
    min-height: 30rpx;
    margin-right: 6rpx;
    margin-bottom: 10rpx;
    vertical-align: top;
}

.tag_empty {
    margin-top: 30rpx;
}

.price_symbol {
    font-size: 0.6em;
    margin-right: 4rpx;
}

.sales_box {
    font-size: 22rpx;
    line-height: 22rpx;
    color: #b2b2b2;
}

.market_price {
    text-decoration: line-through;
    color: #b2b2b2;
    font-weight: 300;
}

.shop_price {
    display: inline-flex;
    font-weight: bold;
}

.good_integral {
    display: inline-flex;
    font-size: 28rpx;
    line-height: 28rpx;
    margin-left: 5rpx;
}

.has_no_goods {
    width: 100%;
    text-align: center;
    padding-top: 200rpx;
}

.has_no_goods image {
    width: 200rpx;
    display: inline-block;
}

.has_no_goods .txt {
    margin-top: -20rpx;
    color: #7f7f7f;
}

.empty {
    min-height: 28rpx;
}

.stay_white {
    position: relative;
    /* position: fixed;
    top: 0px;
    left: 0px;
    z-index: 99;
    background-color: #fff;
    height: 150rpx;
    width: 100%; */
    /* width: 100%;
    height: 110rpx; */
}

._input_box {
    /* width: 100%;
    position: fixed;
    z-index: 99;
    top: 0;
    left: 0; */
}

.commission_box {
    /* border: 1px solid var(--light_color); */
    border: 1px solid #F14D00;
    /* color: var(--main_color); */
    color: #F14D00;
    font-size: 20rpx;
    display: inline-flex;
    align-items: center;
    border-radius: 6rpx;
    margin-left: 2px;
}

.commission_text {
    padding: 6rpx 8rpx;
    height: 100%;
    border-radius: 0 0 16rpx 0;
    box-sizing: border-box;
    color: #fff;
    background-image: linear-gradient(138deg, #F98400 0%, #F14D00 100%);
    /* background-image: linear-gradient(to bottom right, var(--light_color), var(--main_color)); */
    flex-shrink: 0;
    /* line-height: 20rpx; */
}

.commission_price {
    font-size: 18rpx;
    line-height: 20rpx;
    padding: 0 6rpx;
}

.btn-buy-box {

}

.goods_price_box+.btn-buy-box {
    margin-top: 10rpx;
}

.btn-buy {
    position: relative;
    z-index: 1;
    width: 308rpx;
    height: 60rpx;
    background-color: #fafafa;
    font-size: 24rpx;
    border-radius: 100rpx;
}

.price_sale_buy {
    display: flex;
    flex-direction: column;
    flex: 1;
    justify-content: flex-end;
}

.specMod {
    position: relative;
    z-index: 101;
}