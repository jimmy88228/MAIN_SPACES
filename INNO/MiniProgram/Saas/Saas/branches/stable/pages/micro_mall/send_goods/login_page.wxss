/* pages/micro_mall/send_goods/send_goods_code.wxss */
page{
  background:#fff;
}
.login_view{
  position:relative;
}
.login_view_bg{
  width:100%;
  height:810rpx;
  display:block;
}
.login_view_logo{
  position:absolute;
  top:350rpx;
  left:50%;
  width:283rpx;
  height:283rpx;
  display:block;
  transform: translate(-50%,-50%);
}
.operate_btn{
  padding-top:50rpx;
}
.get_number_btn{
  width:500rpx;
  height:90rpx;
  display:flex;
  justify-content: center;
  align-items: center;
  border-radius:11rpx;
  margin:0 auto;
  background:rgba(7,193,96,1);
  color:#fff;
}
.cancel_login_btn{
  font-size:24rpx;
  margin:0px auto;
  margin-top:50rpx;
  width:150rpx;
  height:70rpx;
  padding:10rpx 20rpx;
  background:none;
  border:0 none;
  text-decoration: underline;
}