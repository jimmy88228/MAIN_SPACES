  padding: 0 13rpx;
}

.module_type3 .goods_item {
  padding: 7rpx;
}

.module_type3 .goods_item:nth-child(3n+1) {
    width: 100%;
    display: block;
}

.module_type3 .goods_item .good_name {
  font-size: 26rpx;
}

.module_type3 .goods_item:nth-child(3n+1) .good_name {
