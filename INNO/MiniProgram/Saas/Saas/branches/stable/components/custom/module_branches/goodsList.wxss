}
.module_type3 .present_p {
  font-size: 36rpx;
}
.module_type3.goods_item:nth-child(3n+1) .present_p {
  font-size: 38rpx;
}


/* .module_type3 .goods_item:nth-child(3n+1) .good_name {
    font-size: 26rpx;
} */

.module_type3 .goods_item:nth-child(3n+1) .price_tip {
    font-size: 25rpx;
}

.module_type3 .goods_item:nth-child(3n+1) .price_num {
    font-size: 25rpx;
}

.module_type3 .goods_item:nth-child(3n+1) .sales_vol_box {
  font-size: 22rpx;
}

.module_type4.goods_module {
  display: block;
  padding: 7rpx 13rpx;
}

/* 一行三个 */
.module_type5 .goods_info{
  padding: 10rpx;
}

.module_type5 .goods_item {
  width: 33.33%;
}

.module_type5 .good_name {
  font-size: 24rpx;
}

.module_type5 .text_crossed.original_p {
  width: auto;
  margin-right: 10rpx;
}

.module_type5 .present_p {
  order: 4;
  width: 100%;
}

.module_type5 .sales_vol_box {
  order: 3;
  margin: 0;
}
