/*详细列表*/
.module_type6.goods_module {
  padding: 0 13rpx;
}

.module_type6 .goods_item {
  width: 100%;
  border-radius: 20rpx;
  padding: 7rpx;
}

.module_type6 .good_name {
  font-size: 26rpx;
}

.module_type6 .present_p{
  font-size: 38rpx;
}

.module_type6 .goods_item_stay {
    display: flex;
    flex-direction: row;
    width: 100%;
    position: relative;
    padding: 10rpx;
    border-radius: 20rpx;
    align-items: center;
}

.module_type6  .goods_item:last-child .goods_item_stay::after {
    display: none;
}

.module_type6 .good_img_stay {
    display: table-cell;
    vertical-align: top;
    width: 166rpx;
    border-radius: 10rpx;
}

.module_type6 .goods_info {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  height: 100%;
  box-sizing: border-box;
}

.module_type6 .good_sku {
  margin-top: 0;
  margin-bottom: 0;
}

.module_type6 .tag_img_list {
    padding: 0px;
}

/* .module_type6 .good_name {
  margin-bottom: 16rpx;
} */

.module_type6 .property_box {
  max-width: 440rpx;
  margin-left: 10rpx;
  /* margin-bottom: 44rpx; */
}

.module_type6 .property_box .property_left {
  width: 95%;
}

.module_type6 .good_img image {
  border-radius: 10rpx;
}


/* common */

/*热区*/

.map_area {
  position: absolute;
  z-index: 10;
}

.play_icon_area {
  position: absolute;
  top: 0px;
  left: 0px;
  width: 100%;
  height: 100%;
  z-index: 11;
}

.ad_module image.play_icon, .play_icon {
  position: absolute;
  top: 50%;
  left: 50%;
  width: 120rpx;
  height: 120rpx;
  transform: translate(-50%, -50%);
  background: rgba(0, 0, 0, 0.4);
  border-radius: 100%;
}


.btn_link {
    position: absolute;
    top: 0px;
    left: 0px;
    right: 0px;
    bottom: 0px;
    z-index: 10;
    opacity: 0;
} 
 


.commission_box {
  /* border: 1px solid var(--light_color); */
  border: 1px solid #F14D00;
  /* color: var(--main_color); */
  color: #F14D00;
  font-size: 18rpx;
  display: inline-flex;
  align-items: center;
  border-radius: 6rpx;
}

.commission_text {
  padding: 6rpx 8rpx;
  height: 100%;
  border-radius: 0 0 16rpx 0;
  box-sizing: border-box;
  color: #fff;
  background-image: linear-gradient(138deg, #F98400 0%, #F14D00 100%);
  /* background-image: linear-gradient(to bottom right, var(--light_color), var(--main_color)); */
  line-height: 18rpx;
}

.commission_price {
  font-size: 18rpx;
  line-height: 18rpx;
  padding: 0 6rpx;
}