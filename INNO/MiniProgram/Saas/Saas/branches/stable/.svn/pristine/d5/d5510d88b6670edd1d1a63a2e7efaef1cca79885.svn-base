.mode-main-box{
  border-radius: 12rpx 12rpx 0 0;
  overflow: hidden;
}
.mode-box{}
.widthFix{
  width: 100%;
}
.aspectFit{
  width: 100%;
  height: 100%;
}
.aspectFill{
  position: relative;
  width: 100%;
  height: 100%;
  overflow: hidden;
}
.ori-image{
  width:100%;
  height:100%;
}
.aspectFill .ori-image{
  width: 100%; 
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
}
.widthFix .ori-image{
  width: 100%;
}