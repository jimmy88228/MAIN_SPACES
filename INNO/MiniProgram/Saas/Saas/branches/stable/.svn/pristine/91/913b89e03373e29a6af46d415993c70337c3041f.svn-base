.main{
  padding:20rpx;
  box-sizing: border-box;
}
.header{
  margin-bottom: 20rpx;
  border-radius: 12rpx;
  overflow:hidden;
}
.content{
  border-radius: 12rpx;
  overflow:hidden;
}
.invoice_box{
  width: 100%;
  display: flex;
}
.c_title_box{
  background: #fff;
  /* padding:40rpx 0; */
  height: 110rpx;
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
}

.btn_box{
  width: 100%;
  min-height: 88rpx;
  background: #fff;
  position: absolute;
  left: 0;
  bottom: 0;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}
.btn{
  /* width: 190rpx; */
  padding: 0 24rpx;
  height: 50rpx;
  border-radius: 34rpx;
  border: 1px solid transparent;
  margin-right: 25rpx;
  box-shadow: 0 0 2rpx 1px rgba(225, 225, 225, 0.1);
  font-size: 24rpx;
}
.c_title{
  color: rgba(150, 50, 40, 1);
  position: absolute;
  text-align: center;
  width: 100%;
  top: 50%;
  transform: translateY(-50%);
  z-index: 2;
}
.circle_1{
  width: 140rpx;
  height: 80rpx;
  border: 6rpx solid rgba(244, 170, 191, 1);
  position: relative;
  z-index: 1;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.circle_1::after{
  content: "";
  width: 110rpx;
  height: 60rpx;
  border: 2rpx solid rgba(244, 170, 191, 1);
  border-radius: 50%;
}
.padding_bg{
  width: 100%;
  height: 30rpx;
  background: #fff;
}
.img{
  width: 100%;
  height: 300rpx;
}