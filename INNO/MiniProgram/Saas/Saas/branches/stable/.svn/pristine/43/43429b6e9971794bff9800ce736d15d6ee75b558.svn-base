
.stores_a{
  display: flex;
  height: 90rpx;
  position: fixed;
  left:0px;
  top:0px;
  width: 100%;
  justify-content: space-between;
  align-items: center;
  z-index: 11;
  box-sizing:border-box;
  padding:0px 20rpx;
  box-shadow:inset 0 -2rpx 6rpx 1rpx rgba(225,225,225,0.2);
  background-color:#fff;
}
.brand_name{
  padding-left:36rpx;
  padding-right:40rpx;
}
.icon_search_area{
  display:inline-flex;
  align-items: center;
  max-width:380rpx;
  width:100%;
  height:55rpx;
  border-radius:100rpx;
  border:1px solid #efefef;
  padding:0px 20rpx;
}
.icon_search{
  /* position: absolute;
  right: 0;
  top: 50%; */
  width: 30rpx;
  height: 30rpx;
  padding-right:10rpx;
  /* transform:translateY(-50%); */
  /* padding: 15rpx 30rpx;   */
}
.search_txt{
  color:#C4C4C4;
}
.store_name{ 
  line-height:26rpx;
  font-size: 26rpx;
  min-width: 60rpx;
  color: #000;
  font-weight:bold;
}
.img_logo{
  /* padding-right:6rpx; */
  position: absolute;
  left: 25rpx;
  top: 50%;
