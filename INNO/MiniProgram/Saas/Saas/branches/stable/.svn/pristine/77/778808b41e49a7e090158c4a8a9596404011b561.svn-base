.box{
  width: 100%;
  min-height: 100rpx;
  padding:0 30rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
  background: #fff;
}
.item_l{
  flex-shrink: 0;
  min-width: 150rpx;
  font-weight: bold;
}
.item_l._C_7f{
  color: #7f7f7f;
  font-weight: normal;
}
.item_r{
  flex-grow: 1;
  flex-shrink: 1;
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}