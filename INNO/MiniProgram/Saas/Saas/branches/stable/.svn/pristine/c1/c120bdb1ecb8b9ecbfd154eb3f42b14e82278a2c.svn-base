.short_link_popup {
  background: #f5f5f5;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: 12;
  width: 662rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 28rpx 40rpx;
  box-sizing: border-box;
  border-radius: 20rpx;
}
.s_l_header{
  font-size: 32rpx;
  margin-bottom: 24rpx;
  box-sizing: border-box;
}
.s_l_content{
  border-radius: 20rpx;
  background-color: #ffffff;
  width: 100%;
  min-height: 110rpx;
  box-sizing: border-box;
  padding: 20rpx;
  color: #999999;
  font-size: 24rpx;
  margin-bottom: 24rpx;
  word-break: break-all;
}
.s_l_confirm{
  border-radius: 100rpx;
  padding: 2rpx;
  width: 80%;
  color: #ffffff;
}
.s_l_close{
  width: 20rpx;
  position: absolute; 
  padding: 10rpx;
  top: 30rpx;
  right: 18rpx;
}